/* 테이블 */
.table_box {
  margin: 0 auto;
  display: flex;
  flex-direction: column;
  gap: 14px;
}

.table_box .title {
  width: 141px;
  padding: 0 23px;
  margin: 0 auto;
  background-color: rgba(255, 255, 255, 1);
  border: 3px solid rgba(205, 231, 223, 1);
  border-radius: 24px;
  font-weight: 700;
  font-size: 20px;
  line-height: 32px;
  letter-spacing: -0.5px;
  position: relative;
}

.table_box .title::before {
  content: "";
  width: calc(100% + 20px);
  height: 20px;
  background-color: rgba(205, 231, 223, 1);
  border-radius: 10px;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  z-index: -1;
}

.table_box table {
  width: 750px;
  position: relative;
}

.table_box table::before {
  content: "";
  width: 175px;
  height: 100%;
  border: 2px solid rgba(221, 238, 233, 1);
  border-right: 0;
  border-top-left-radius: 20px;
  border-bottom-left-radius: 20px;
  position: absolute;
  left: 0;
  bottom : -2px;
  z-index: -1;
}

.table_box table::after {
  content: "";
  width: 582px;
  height: 100%;
  border: 2px solid rgba(218, 218, 218, 1);
  border-left: 0;
  border-top-right-radius: 20px;
  border-bottom-right-radius: 20px;
  position: absolute;
  right: 0;
  bottom : -2px;
  z-index: -1;
}

.table_box tr th,
.table_box tr td {
  height: 68px;
  position: relative;
  font-weight: 700;
  font-size: 16px;
  line-height: 24px;
  letter-spacing: -0.5px;
}

.table_box tr th {
  width: 137px;
  background-color: rgba(221, 238, 233, 1);
  position: relative;
}

.table_box tr th::before {
  content: "";
  width: 3px;
  height: calc(100% + 4px);
  background-color: rgba(255, 255, 255, 1);
  position: absolute;
  top: 50%;
  right: -1.5px;
  transform: translateY(-50%);
  z-index: 10;
}

.table_box tr:first-child th {
  border-top-left-radius: 20px;
}

.table_box tr:last-child th {
  border-bottom-left-radius: 20px;
}

.table_box tr:last-child th::after {
  content: "";
  width: 92%;
  height: 3px;
  background-color: rgba(255, 255, 255, 1);
  position: absolute;
  top: -1.5px;
  left: 50%;
  transform: translateX(-50%);
  z-index: 10;
}

.table_box tr td {
  width: 114px;
  position: relative;
  text-align: center;
}

.table_box tr:first-child td {
  background-color: rgba(240, 240, 240, 1);
}

.table_box tr:first-child td:last-child {
  border-top-right-radius: 20px;
}

.table_box tr:first-child td:not(:last-child)::before {
  content: "";
  width: 3px;
  height: 71%;
  background-color: rgba(255, 255, 255, 1);
  position: absolute;
  top: 50%;
  right: -1.5px;
  transform: translateY(-50%);
}

.table_box tr:last-child td:last-child {
  border-bottom-right-radius: 20px;
}

.table_box tr:last-child td:not(:last-child) {
  border-right: 1px solid rgba(218, 218, 218, 1);
}

