/* ema516_06_ih_0007 */
.ema516_06_ih_0007 .boolean_wrap {
  background: url(./../../common_practice/img/EMA516_06_IH/0007_01.png) no-repeat 50% / 100% auto;
  width: 732px;
  height: 341px;
  margin: 0 auto;
}

.ema516_06_ih_0007 .boolean_wrap button {
  width: auto;
  height: auto;
  aspect-ratio: unset;
  border-radius: 0;
  position: relative;
  padding: 0;
  background-color: transparent !important;
  border-width: 0;
}

.ema516_06_ih_0007 .boolean_wrap button:before {
  content: '';
  display: inline-block;
  width: 60px;
  height: 60px;
  mask: url(./../../common_practice/common/img/icon_checked_round.svg) no-repeat 50% / 100% auto;
  position: absolute;
  left: -50px;
  top: -20px;
  background-color: transparent;
}

.ema516_06_ih_0007 .boolean_wrap button + button {
  margin-top: 12px;
}

.ema516_06_ih_0007 .boolean_wrap button.selected {
  border-color: transparent;
}

.ema516_06_ih_0007 .boolean_wrap button.selected:before {
  background-color: var(--font-main);
}
.ema516_06_ih_0007 .boolean_wrap button.hint:not(.selected):before {
  background-color: var(--secondary-red);
}
.ema516_06_ih_0007 .boolean_wrap button[data-answer-single='true'].correct:before {
  background-color: var(--primary-blue);
}

.ema516_06_ih_0007 .boolean_wrap button:hover {
  border-color: transparent;
}

.ema516_06_ih_0007 .boolean_wrap button p {
  font-weight: 700;
}

