/* ema516_06_ih_0001 */
.ema516_06_ih_0001 .quiz_area {
  width: 512px;
  height: 301px;
  background: url('../img/EMA516_06_IH/0001_01.svg') no-repeat center / cover;
  margin-top: 50px;
  position: relative;
  padding: 102px 0 0 87px;
}

.ema516_06_ih_0001 .quiz_area + .quiz_area {
  margin-left: 65px;
}

.ema516_06_ih_0001 .quiz_area .question {
  position: absolute;
  top: 21px;
  left: 3px;
  width: 135px;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 32px;
  font-weight: 700;
}

.ema516_06_ih_0001 .quiz_area .canvas_wrap .hint {
  width: 100%;
  font-size: 24px;
  font-weight: 700;
  color: var(--font-negative);
  text-align: center;
  letter-spacing: -1px;
  border-radius: 12px;
  opacity: 0;
  position: absolute;
  top: -50px;
  left: 0;
  z-index: 10;
  pointer-events: none;
}
.ema516_06_ih_0001 .quiz_area .canvas_wrap .hint.show {
  opacity: 1;
}

.ema516_06_ih_0001 .quiz_area .quiz_list {
  display: flex;
  flex-direction: column;
}

.ema516_06_ih_0001 .quiz_area .quiz_list > li {
  display: flex;
  align-items: center;
}

.ema516_06_ih_0001 .quiz_area .quiz_list > li + li {
  margin-top: 18px;
}

.ema516_06_ih_0001 .quiz_area .quiz_list > li .quiz_tit {
  width: 81px;
  height: 40px;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #fff;
  background-color: #abacad;
  border-radius: 20px 20px 0px 20px;
  margin-right: 10px;
}

.ema516_06_ih_0001 .quiz_area .quiz_list > li .canvas_wrap {
  width: 278px;
  height: 60px;
  position: relative;
}

.ema516_06_ih_0001 .quiz_area .quiz_list > li .canvas_wrap .line {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 1px;
  border-top: dashed 1px #00a0e9;
}

.ema516_06_ih_0001 .quiz_area .quiz_list > li .canvas_wrap .line2 {
  top: 35%;
}

.ema516_06_ih_0001 .quiz_area .quiz_list > li .canvas_wrap .line3 {
  top: auto;
  bottom: 0;
}

.ema516_06_ih_0001 .quiz_area .quiz_list > li .canvas_wrap canvas {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}

.ema516_06_ih_0001 .quiz_area .quiz_list > li .input_wrap {
  width: 280px;
}

.ema516_06_ih_0001 .quiz_area .quiz_list > li .input_wrap math-field:not(.textarea) {
  width: 100%;
}

.ema516_06_ih_0001 .btn.btn_draw {
  left: 40px;
  bottom: 30px;
  position: absolute;
}

