.question_box {
  white-space: normal;
}
.title.group {
  padding-bottom: 0;
  padding-left: 0;
}
.tit_data {
  display: flex;
  justify-content: center;
  margin-bottom: 14px;
}
.tit_data .inner {
  position: relative;
  padding: 0 6px;
}
.tit_data .inner:before {
  content: '';
  position: absolute;
  top: 50%;
  left: 0;
  width: 100%;
  height: 22px;
  transform: translateY(-50%);
  border-radius: 22px;
  background: #fdddce;
}
.tit_data span {
  position: relative;
  display: flex;
  align-items: center;
  justify-content: center;
  height: 32px;
  padding: 0 16px;
  border: 2px solid #fdddce;
  border-radius: 32px;
  font-size: 24px;
  font-weight: bold;
  color: #911c1a;
  background-color: #fff;
}
.table_wrap {
  width: 100%;
  max-width: 960px;
}
.table_wrap th,
.table_wrap td {
  font-weight: normal;
  height: 46px;
  padding: 12px;
  font-size: 24px;
}
.table_wrap thead th {
  background-color: #fde6db;
}
.table_wrap tbody th {
  background-color: #eaebec;
}
.table_wrap.row th,
.table_wrap.row td {
  height: 52px;
}
.table_wrap.row thead td {
  background-color: #eaebec;
}
.table_wrap.row thead tr:first-child td:last-child {
  border-radius: 0 19px 0 0;
}
.table_wrap.row tbody th {
  background-color: #fde6db;
}
.table_wrap.row thead td {
  border-left: 2px solid #fff;
}
.example_box.small {
  position: absolute;
  top: 100%;
  min-height: 45px;
  padding: 0 0 0 50px;
  background: none;
  border-radius: 0;
}
.example_box.small:before {
  top: 0;
  left: 0;
}
.example_box.small p {
  color: var(--font-negative);
}
.question_box {
  white-space: normal;
}

.ema526_07_ih_0001 .dir_col {
  width: 1300px;
  margin: 60px auto 0;
}
.ema526_07_ih_0001 .text_bullet:before {
  top: 30px;
}
.ema526_07_ih_0001 .boolean_wrap {
  display: inline-flex;
  gap: 4px;
}
.ema526_07_ih_0001 .boolean_wrap button {
  position: relative;
  width: auto;
  height: 68px;
  min-width: 40px;
  padding: 0;
  border: none;
  border-radius: 0;
  background: none !important;
  aspect-ratio: unset;
}
.ema526_07_ih_0001 .boolean_wrap button:before {
  position: absolute;
  top: 50%;
  left: 50%;
  width: 68px;
  height: 68px;
  transform: translate(-50%, -50%);
  border-radius: 100%;
  border: 4px solid #222;
}
.ema526_07_ih_0001 .boolean_wrap button.selected:before {
  content: '';
}
.ema526_07_ih_0001 .boolean_wrap.result button.hint[data-answer-single='true']:before {
  content: '';
  border-color: var(--font-negative);
  background: rgba(255, 67, 67, 0.3);
}
.ema526_07_ih_0001 .boolean_wrap.result button.selected[data-answer-single='true']:before {
  border-color: var(--primary-blue);
  background: rgba(59, 113, 254, 0.3);
}
.ema526_07_ih_0001 .solve_area .conts_solve .text_bullet:before {
  top: 20px;
}
.ema526_07_ih_0001 .solve_area .conts_solve .fraction_box {
  vertical-align: middle;
}

.ema526_07_ih_0002 .circle {
  display: inline-flex;
  width: 44px;
  height: 44px;
  margin-right: 10px;
  border: 2px solid #222;
  border-radius: 100%;
  justify-content: center;
  align-items: center;
  font-size: 24px;
}
.ema526_07_ih_0002 .boolean_wrap {
  flex-direction: column;
  justify-content: space-around;
  margin: 0 auto;
  padding: 35px 60px;
  align-items: flex-start;
  width: 722px;
  height: 224px;
  background: url(../img/EMA526_07_IH/0002_bg.svg) no-repeat center center / contain;
}
.ema526_07_ih_0002 .boolean_wrap button {
  position: relative;
  display: block;
  width: auto;
  height: auto;
  aspect-ratio: unset;
  padding: 0;
  border: none;
  border-radius: 0;
  background: none;
  text-align: left;
  letter-spacing: -1px;
}
.ema526_07_ih_0002 .boolean_wrap button .circle {
  position: absolute;
  top: 0;
  left: 0;
  margin-top: -3px;
}
.ema526_07_ih_0002 .boolean_wrap button:after {
  position: absolute;
  top: -8px;
  left: -38px;
  width: 50px;
  height: 50px;
  background: url(../common/img/icon_checked_round.svg) no-repeat center center / contain;
}
.ema526_07_ih_0002 .boolean_wrap button.selected::after {
  content: '';
}
.ema526_07_ih_0002 .boolean_wrap.result button[data-answer-single='true']:after {
  content: '';
  filter: invert(18%) sepia(97%) saturate(6541%) hue-rotate(357deg) brightness(91%) contrast(122%);
}
.ema526_07_ih_0002 .boolean_wrap.result button.selected[data-answer-single='true']:after {
  filter: invert(36%) sepia(61%) saturate(3771%) hue-rotate(215deg) brightness(102%) contrast(99%);
}
.ema526_07_ih_0002 .question_box {
  margin-top: 20px;
}
.ema526_07_ih_0002 .question_box .input_wrap {
  width: 1223px;
}
.ema526_07_ih_0002 .question_box .input_wrap math-field {
  width: 100%;
}

