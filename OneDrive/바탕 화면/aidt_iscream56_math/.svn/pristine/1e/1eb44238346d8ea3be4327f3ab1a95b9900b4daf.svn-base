.boolean_wrap button.hint {
  border-width: 4px;
  /* border-color: transparent; */
}
.img_area {
  position: relative;
}
.img_area p {
  position: absolute;
}
.input_area > span,
.input_row > span {
  display: inline-block;
  vertical-align: middle;
}
math-field.has_ex_text + .text_hint:before {
  content: 'ì˜ˆ';
  display: inline-block;
  vertical-align: middle;
  margin-right: 10px;
  width: 44px;
  height: 44px;
  background: var(--primary-blue);
  border-radius: 12px;
  color: #fff;
  line-height: 42px;
}

/* ema521_03_ih_0001 */
.ema521_03_ih_0001 h4 {
  width: 341px;
  height: 68px;
  background: url(./../../common_practice/img/EMA521_03_IH/0001_01.png) no-repeat 50% / cover;
  color: #4d76b7;
  font-weight: 700;
  font-size: 32px;
  line-height: 66px;
  text-align: center;
  margin: 0 auto;
  letter-spacing: -1px;
}
.ema521_03_ih_0001 .boolean_wrap {
  padding: 0 110px;
}
.ema521_03_ih_0001 .table_wrap {
  position: relative;
  width: 100%;
}
.ema521_03_ih_0001 .table_wrap table {
  border-radius: 20px;
  box-shadow: 0 0 0 1px #d2d2d2;
  border-style: hidden;
  border-collapse: collapse;
  overflow: hidden;
}
.ema521_03_ih_0001 .table_wrap table col:first-of-type {
  width: 282px;
}
.ema521_03_ih_0001 .table_wrap th,
.ema521_03_ih_0001 .table_wrap td {
  font-size: 28px;
  height: 66px;
  border: 1px solid #d2d2d2;
}
.ema521_03_ih_0001 .table_wrap thead th {
  background: #d4ebf9;
}
.ema521_03_ih_0001 .table_wrap thead td {
  background: #f4f4f4;
}
.ema521_03_ih_0001 .table_wrap tbody th {
  background: #d4ebf9;
}

.ema521_03_ih_0001 .button_box {
  position: absolute;
  top: 45px;
  left: 283px;
  display: flex;
  width: calc(100% - 279px);
  justify-content: space-around;
}
.ema521_03_ih_0001 .button_box button {
  width: 110px;
  height: 110px;
  background: none;
}
