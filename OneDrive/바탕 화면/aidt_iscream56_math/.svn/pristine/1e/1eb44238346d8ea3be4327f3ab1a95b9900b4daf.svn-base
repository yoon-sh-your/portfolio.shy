.ema521_03_ih_0001 .page_3 .input_row {
  display: flex;
  align-items: center;
  justify-content: center;
  margin-top: 50px;
  gap: 10px;
  letter-spacing: -1px;
}
.ema521_03_ih_0001 .page_3 .input_area {
  width: 707px;
  height: 270px;
  background: url(./../../common_practice/img/EMA521_03_IH/0001_02.png) no-repeat 50% / 100% auto;
}

.text_indent {
  margin-left: -8px;
}

/* ema521_03_ih_0002 */

.ema521_03_ih_0002 .boolean_wrap {
  width: 935px;
  height: 203px;
  background: url(./../../common_practice/img/EMA521_03_IH/0002_01.png) no-repeat 50% / 100% auto;
  margin: 0 auto;
}
.ema521_03_ih_0002 .button_box {
  display: flex;
  gap: 65px;
}
.ema521_03_ih_0002 .button_box button {
  background: none;
  width: 68px;
  height: 68px;
}
.result .ema521_03_ih_0002 .button_box button[data-answer-single='true'].correct {
  border-color: var(--primary-blue-800);
  background: rgba(59, 113, 254, 0.3);
}

/* ema521_03_ih_0003 */
.ema521_03_ih_0003 .question_box {
  position: relative;
  width: 577px;
  height: 412px;
  background: url(../img/EMA521_03_IH/0003_box.png) no-repeat center / 577px 412px;
  margin-left: 530px;
}
.ema521_03_ih_0003 .tit {
  position: absolute;
  top: 30px;
  left: 653px;
  font-weight: 700;
  font-size: 32px;
  letter-spacing: -1px;
  width: 329px;
  height: 87px;
  display: flex;
  align-items: center;
  justify-content: center;
}
.ema521_03_ih_0003 .tit.t2 {
  top: 256px;
}
.ema521_03_ih_0003 .draws.draws_active {
  background: #3b71fe;
}
.ema521_03_ih_0003 .draws-input-wrap {
  width: 570px;
  height: 18px !important;
  position: absolute;
  top: 142px;
  left: 533px;
  cursor: pointer;
}
.ema521_03_ih_0003 .draws-input-wrap.diw1 {
  transform: rotate(180deg);
}
.ema521_03_ih_0003 .draws-input-wrap.diw2 {
  top: 266px;
}
.ema521_03_ih_0003 .input-wrap {
  position: relative;
  width: 0;
  height: 100%;
  margin-top: -2px;
}
.ema521_03_ih_0003 .input-wrap .dropdown_wrap {
  position: absolute;
  top: 3px;
  right: -8px;
}
.ema521_03_ih_0003 .diw1 .dropdown_wrap {
  transform: rotate(180deg);
}

.ema521_03_ih_0003 .custom_select.math_symbol {
  width: 18px;
  height: 18px;
}
.ema521_03_ih_0003 .custom_select.math_symbol .select_trigger {
  background: #3b71fe;
  border: none !important;
}
.ema521_03_ih_0003 .draws-input-wrap.val-1 .select_trigger {
  background: #fff url(../img/EMA521_02_IH/dot2.png) no-repeat center / 100% !important;
}
.ema521_03_ih_0003 .draws-input-wrap.val-2 .select_trigger {
  background: url(../img/EMA521_02_IH/dot1.png) no-repeat center / 100% !important;
}
.ema521_03_ih_0003 .custom_dropdown.hint + .custom_select.math_symbol .select_trigger {
  background: #fff url(../img/EMA521_02_IH/dot2.png) no-repeat center / 100% !important;
}
.ema521_03_ih_0003 .draws-input-wrap input {
  width: 100%;
  height: 6px;
  pointer-events: none;
}
.ema521_03_ih_0003 .draws-input-wrap .range {
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-top: 20px;
}
.ema521_03_ih_0003 .draws-input-wrap.diw1 .range {
  transform: rotate(180deg);
  position: relative;
  top: -102px;
}
.ema521_03_ih_0003 .draws-input-wrap .range > span {
  width: 42px;
  font-weight: 700;
  font-size: 32px;
  display: flex;
  align-items: center;
  justify-content: center;
  letter-spacing: -1px;
  padding: 0 26px;
}

.ema521_03_ih_0003 .select_options li {
  display: inline-flex !important;
  align-items: center;
  justify-content: center;
  padding-left: 10px !important;
}
.ema521_03_ih_0003 .select_options li:before {
  content: '';
  display: block;
  width: 24px;
  height: 24px;
  border-radius: 100%;
  background: url(../img/EMA521_02_IH/dot1.png) no-repeat center / 100%;
}
.ema521_03_ih_0003 .select_options li:nth-child(1):before {
  background: url(../img/EMA521_02_IH/dot2.png) no-repeat center / 100%;
}

