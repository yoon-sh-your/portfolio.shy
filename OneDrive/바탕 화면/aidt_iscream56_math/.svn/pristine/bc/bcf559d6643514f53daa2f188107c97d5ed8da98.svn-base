/* ema525_03_SU_0004 page_1 */
.ema525_03_su_0004 .img_box {
  position: relative;
  margin: -5px auto 0;
  width: 190px;
}
.ema525_03_su_0004 .img_box img {
  width: 100%;
}
.flex_col_box2 .img_paper {
  position: absolute;
  top: 0px;
  left: 0px;
  width: 100%;
  height: 100%;
}
.ema525_03_su_0004 button:focus,
.ema525_03_su_0004 button:focus-visible,
.ema525_03_su_0004 *[role="button"]:focus,
.ema525_03_su_0004 *[role="button"]:focus-visible {
  outline: 3px dashed transparent !important;
  outline-offset: -3px;
  border-color: transparent !important;
  /* border: 3px dashed black !important; */
  border-radius: 8px;
  box-sizing: border-box !important;
  background-clip: padding-box !important;
}
/* 기본설정 */
.flex_col_box2 .img_paper .dropdown_wrap {
  position: absolute;
}
.flex_col_box2
  .img_paper
  .custom_select.math_symbol
  .select_options
  li[data-value="solid"] {
  background-size: 42px;
  background-image: url(../../common_contents/img/EMA612_04_SU/0005_ico_solid.png);
}
.flex_col_box2
  .img_paper
  .custom_select.math_symbol
  .select_options
  li[data-value="dotted"] {
  background-size: 42px;
  background-image: url(../../common_contents/img/EMA612_04_SU/0005_ico_dotted.png);
}
.flex_col_box2
  .img_paper
  .custom_select.math_symbol
  .select_trigger.completion[data-value="solid"] {
  background-image: none;
  background-color: var(--primary-blue);
}
.flex_col_box2
  .img_paper
  .custom_select.math_symbol
  .select_trigger.completion[data-value="dotted"] {
  background-image: none;
  background: repeating-linear-gradient(
    90deg,
    var(--primary-blue) 0px,
    var(--primary-blue) 3px,
    white 3px,
    white 6px
  );
}
.flex_col_box2
  .img_paper
  .custom_dropdown.hint[data-answer-single="solid"]
  + .custom_select.math_symbol
  .select_trigger {
  background-color: var(--primary-blue);
  background: transparent;
}
.flex_col_box2
  .img_paper
  .custom_dropdown.hint[data-answer-single="dotted"]
  + .custom_select.math_symbol
  .select_trigger {
  background-image: none;
  background: repeating-linear-gradient(
    90deg,
    var(--primary-blue) 0px,
    var(--primary-blue) 3px,
    white 3px,
    white 6px
  );
}

.flex_col_box2 .img_paper .custom_select.math_symbol {
  width: 0px;
  height: 0px;
}
.flex_col_box2 .img_paper .custom_select.math_symbol .select_trigger {
  background-color: transparent;
  border: 0;
  border-radius: 0;
  padding: 0;
  font-size: 0;
  height: 3px;
  transform-origin: left center;
  z-index: 1;
  position: absolute;
  /* background-color: rgba(0, 0, 0, 0.2); */
}

