.search_wrap {
    display: flex;
    gap: 12px;
    width: 155px;
    height: 56px;
    border: 1px solid #3B71FE;
    border-radius: 40px;
    padding: 12px 20px;
}
.search_wrap input {
    width: 100%;
    font-size: 20px;
    line-height: 32;
    letter-spacing: -0.5px;
    font-weight: 700;
}       
.search_wrap input::placeholder {
    color: #000;
}
.search_wrap .btn_search {
    min-width: 40px;
    width: 40px;
    height: 30px;
    background: #3B71FE url(../../common_contents/img/EMA614_05_SU/0005_ico.svg) no-repeat center / 100%;
    padding: 0; 
    border-radius: 20px;
}
.search_wrap .btn_search i {
    position: absolute;
    width: 1px;
    height: 1px;
    padding: 0;
    margin: -1px;
    overflow: hidden;
    clip: rect(0, 0, 0, 0);
    white-space: nowrap;
    border: 0;
}

