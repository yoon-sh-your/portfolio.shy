/*EMA613_11_SU_0005_s*/
.ema613_11_su_0005 {
  .boolean_wrap {
    width: 1235px;
    height: 110px;
    background: url(./../../common_contents/img/EMA613_11_SU/0005_bg.png) no-repeat 50% / 100% auto;
    margin: 0 auto;
    display: flex;
    gap: 67px;
  }

  button {
    width: auto;
    height: auto;
    border-radius: 0;
    background: none;
    aspect-ratio: unset;
    padding: 0;
    border: 0;
    position: relative;
  }

  button.selected:after {
    content: '';
    width: 45px;
    height: 45px;
    background: url(./../../common_contents/img/EMA613_11_SU/icon_chk.png) no-repeat 50% / 100% auto;
    position: absolute;
    top: -2px;
    left: -38px;
  }

  .result .selected:after {
    background: url(./../../common_contents/img/EMA613_11_SU/icon_chked.png) no-repeat 50% / 100% auto;
  }
}

/*EMA613_11_SU_0006_s*/
.ema613_11_su_0006 {
  .name {
    position: absolute;
  }

  .question_box {
    width: 1108px;
    height: 369px;
    background: url(./../../common_contents/img/EMA613_11_SU/0006_img01.png) no-repeat 50% / 100% auto;
    margin: 0 auto;

    .flex.justify_center.dir_col.gap_40 {
      margin-top: 25px;
    }

    .txt_box {
      position: relative;
      width: 534px;
      height: 310px;
      padding: 50px 30px 50px 140px;

      .example_box {
        height: 180px;
      }

      &.dir_row {
        position: relative;

        .example_box {
          &::after {
            content: '';
            display: block;
            width: 97px;
            height: 126px;
            background: url(./../../common_contents/img/EMA613_11_SU/0006_img02.png) no-repeat center 20px;
            margin: 0 auto;
          }

          .flex {
            width: 97px;
            height: 222px;
            position: absolute;
            left: 52%;
            top: -36px;
            color: #1454b9;
          }
        }
      }

      .input_wrap {
        position: absolute;
        left: 0;
        top: 106%;
        right: 0;
        white-space: normal;
      }

      .name {
        left: 57px;
        top: 6px;
        display: flex;
        color: #fff;

        span:nth-child(1) {
          display: inline-block;
          transform: rotate(-10deg);
          margin-top: 3px;
        }

        span:nth-child(2) {
          display: inline-block;
          transform: rotate(1deg);
        }
      }

      &+.txt_box .flex {
        span:nth-child(1) {
          position: absolute;
          right: 82px;
          top: 90px;

          .gray {
            position: absolute;
            top: 0;
            right: -25px;
          }
        }

        span:nth-child(2) {
          position: absolute;
          right: 120px;
          top: 90px;
        }

        span:nth-child(3) {
          position: absolute;
          right: 60px;
          top: 50px;
        }

        span:nth-child(4) {
          position: absolute;
          right: 75px;
          top: 128px;
          letter-spacing: 6px;

          .gray {
            position: absolute;
            top: 0;
            right: -25px;
          }
        }

        span:nth-child(5) {
          position: absolute;
          right: 60px;
          top: 170px;
        }

        .input_wrap {
          position: absolute;
          left: 0;
          top: 106%;
          right: 0;
          white-space: normal;
        }
      }

      .abs_1 {
        margin-top: 20px;
        margin-left: -100px;
      }

      .abs_2 {
        position: absolute;
        bottom: -12px;
        left: 30px;
        display: flex;
        gap: 30px;
      }
    }
  }
}

/*EMA613_11_SU_0007_s*/
.ema613_11_su_0007 {
  section.title.type2+section.contents {
    padding-top: 0;
  }

  .page_1 {
    .input_area {
      width: 876px;

      .flex {
        width: 336px;
        margin: auto;

        .text_input {
          width: 100%;
        }
      }

      .input_wrap {
        width: 100%;

        math-field.text_input {
          width: 100%;
        }
      }
    }
  }

  .page_2 {
    .input_area {
      position: relative;

      .tit {
        position: absolute;
        top: 0;
        left: 40px;
        width: 189px;
        height: 64px;
        background-color: #FFF5ED;
        color: #C0573D;
        padding: 8px 24px;
        border-radius: 20px 20px 0px 0;
      }

      .flex {
        margin: 50px auto 0;
        background: #fff5ed;
        padding: 20px;
        display: inline-flex !important;
        border-radius: 40px;

        .text_input {
          width: 100%;
        }

        .input_wrap {
          &:nth-child(1) {
            max-width: 426px;
          }

          &:nth-child(2) {
            position: relative;
            padding-left: 50px;

            .hin {
              position: absolute;
              left: -10px;
              top: 10px;
              display: inline-flex;
              width: 48px;
              height: 42px;
              background-color: #fff;
              border-radius: 50% 50% 0;
              align-items: center;
              justify-content: center;
              color: #c0573d;
            }
          }

          math-field.text_input {
            width: 100%;
          }
        }
      }
    }
  }

  .picture_wrap {
    height: 494px;

    .txt_box {
      font-size: 20px;
      letter-spacing: -0.5px;
      z-index: 99;
      text-align: center;

      &.abs_1 {
        position: absolute;
        top: 76px;
        left: 153px;

        .name {
          position: absolute;
          opacity: 0;
        }
      }

      &.abs_2 {
        position: absolute;
        top: 25px;
        left: 406px;

        .name {
          position: absolute;
          opacity: 0;
        }
      }
    }
  }
}

/*EMA613_11_SU_0008_s*/
.ema613_11_su_0008 section.title.type2 + section.contents{
  padding-top: 0px;
}
.ema613_11_su_0008 {
  .inner_wrap {
    position: relative;
    width: 1435px;
    height: 1281px;
    margin: auto;
    background: url(../img/EMA613_11_SU/0008_img01.png) no-repeat center 0 / 100%;

    .h3 {
      font-size: 43px;
      padding: 120px 210px 100px;
      font-weight: 700;
    }

    .input_wrap {
      min-width: auto;
    }

    .input_wrap math-field:not(.textarea) {
      width: 80px;
    }

    .items {
      position: relative;
      margin-left: 70px;

      .flex:first-child {
        margin-bottom: 30px;
      }

      .question_box {
        position: relative;
        left: 90px;
        top: 40px;
      }

      &:nth-child(1) {
        .question_box {
          position: relative;
          left: 90px;
          top: 0;
          width: 273px;
          height: 331px;
          font-size: 32px;

          &:before {
            content: '';
            position: absolute;
            left: 20px;
            top: 0;
            right: 0;
            bottom: 0;
            background: url(../img/EMA613_11_SU/0008_img02.png) no-repeat 7px 70px;
          }
        }

        span {
          position: absolute;
          letter-spacing: 32px;

          &:nth-child(1) {
            top: 80px;
            left: 51px;
          }

          &:nth-child(2) {
            top: 80px;
            left: 0;
          }

          &:nth-child(3) {
            top: 10px;
            left: 51px;
          }

          &:nth-child(4) {
            top: 130px;
            left: 51px;
          }

          &:nth-child(5) {
            top: 180px;
            left: 141px;
          }

          &:nth-child(6) {
            top: 230px;
            left: 141px;
          }

          &:nth-child(7) {
            top: 290px;
            left: 141px;
          }
        }

        .input_wrap {
          position: absolute;
          top: 0px;
          left: 120px;
        }
      }

      &:nth-child(2) {
        p {
          padding-left: 40px;

          &:nth-last-child(1),
          &:nth-last-child(2) {
            padding-left: 122px;
          }
        }
      }

      &:nth-child(3) {
        .flex:nth-child(2) {
          position: relative;

          &::before {
            content: '';
            position: absolute;
            left: 0px;
            top: 0;
            right: 0;
            bottom: 0;
            background: url(../img/EMA613_11_SU/0008_img03.png) no-repeat center;
          }
        }
      }
    }

    .box_wrap {
      margin: 80px 160px 0 399px;

      .arr {
        width: 45px;
        height: 40px;
        background: url(../img/EMA613_11_SU/0008_img01_1.png) no-repeat center;
      }

      .tag1 {
        display: inline-flex;
        width: 48px;
        height: 42px;
        background-color: #b5b5b6;
        border-radius: 50% 50% 0;
        align-items: center;
        justify-content: center;
        color: #fff;
      }

      .tag2 {
        display: inline-flex;
        width: 90px;
        height: 42px;
        background-color: #b5b5b6;
        border-radius: 50% 50% 0;
        align-items: center;
        justify-content: center;
        color: #fff;
      }

      .items_wrap {
        .items:nth-child(1) {
          margin-left: 0;
          width: 275px;
          height: 121px;
          background-color: #feebbe;
          border-radius: 20px;
          padding: 30px;
          position: relative;
          left: 17px;
        }

        & .boolean_wrap {
          button {
            width: 32px;
            height: 32px;
            padding: 0;
            border: 2px solid var(--primary-blue);
            border-radius: 4px;

            span {
              display: none;
            }

            &.selected span {
              display: block;
            }
          }
        }
      }
    }
  }

  .way1 {
    .text_hint {
      top: -12px;
    }
  }

  .bag {
    margin-left: 43px;

    >div:first-child {}

    >div:last-child {
      margin-left: 40px;
    }
  }

  .item.flex.gap_20.align_center {
    margin-bottom: 0;
  }
}

.gap_70 {
  gap: 80px;
}

