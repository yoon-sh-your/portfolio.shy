/*EMA613_11_SU_0002_s*/
.ema613_11_su_0002 {
  .contents .flex {
    position: relative;

    &::after {
      content: '';
      position: absolute;
      height: 281px;
      left: 50%;
      border: 1px dashed #6a6a6a;
    }
  }

  .question_box {
    position: relative;
    width: 586px;
    height: 281px;
    background: url(../img/EMA613_11_SU/0002_img01.png) no-repeat center 0 / 100%;
    margin: 0 auto;

    .abs_1 {
      position: absolute;
      top: 130px;
      left: 15px;
    }

    .abs_2 {
      position: absolute;
      top: 27px;
      left: 170px;
      gap: 12px;
    }

    .abs_3 {
      position: absolute;
      top: 130px;
      left: 337px;
      display: flex;
      align-items: center;
      gap: 5px;
    }

    .abs_4 {
      position: absolute;
      top: 207px;
      left: 332px;
      display: flex;
      align-items: center;
      gap: 5px;

      .input_wrap {
        min-width: auto;
      }

      math-field:not(.textarea) {
        width: 80px;
      }
    }
  }
}

/*EMA613_11_SU_0003_s*/
.ema613_11_su_0003 {

  /*ema512_10_su_0005_s*/
  .ex {
    width: 52px;
    height: 32px;
    background-color: rgba(255, 255, 255, 1);
    border-radius: 4px;
    border: 2px solid rgba(238, 79, 36, 1);
    display: flex;
    justify-content: center;
    align-items: center;
    font-weight: 700;
    font-size: 16px;
    color: rgba(238, 79, 36, 1);
    line-height: 24px;
    letter-spacing: -0.5px;
  }

  .boolean_wrap {
    width: 1122px;
    padding: 37px;
    border: 3px solid rgba(238, 79, 36, 1);
    border-radius: 20px;
    display: flex;
    align-items: flex-start;
    gap: unset;
    flex-wrap: wrap;
    position: relative;
    left: 50%;
    transform: translateX(-50%);

    .ex {
      position: absolute;
      top: 0;
      left: 24px;
      transform: translateY(-50%);
    }
  }

  .down_box {
    margin-top: 50px;
    display: inline-flex;
    flex-direction: column;
    gap: 30px;
    position: relative;
    left: 50%;
    transform: translateX(-50%);

    li {
      display: flex;
      align-items: center;
      gap: 30px;
    }
  }

  .question_box .input_wrap {
    margin: 0 4px;

    &.fraction_wrap math-field {
      width: 203px;
    }

    &.fraction_wrap.single {
      math-field {
        width: 100%;
      }
    }
  }
}

