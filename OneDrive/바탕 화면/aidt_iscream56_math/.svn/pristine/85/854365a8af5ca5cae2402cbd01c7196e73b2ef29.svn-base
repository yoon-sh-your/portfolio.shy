  background-size: 100%;
  margin-right: -5px;
}
.draggable_wrap {
  margin: 0 auto;
}
.draggable_wrap {
  display: flex;
  gap: 20px;
  width: 300px;
  height: 108px;
  padding: 10px 50px;
  justify-content: center;
  align-items: center;
  flex-shrink: 0;
  border-radius: 16px;
  border: 0.797px solid var(--Line-Gray-300, #d2d2d2);
  background: var(--BG-Gray-50, #f7f7f7);
  position: relative;
}
.draggable_wrap .triangle{
  transform: translateY(5px);
}
/* 움직일 도형 */
.triangle {
  background: url(../../common_contents/img/EMA521_02_SU/ico_drag_triangle_.svg) no-repeat center center / 100%;
  width: 70px;
  height: 70px;
}
.circle {
  background: url(../../common_contents/img/EMA521_02_SU/ico_drag_circle_.svg) no-repeat center center / 100%;
  width: 70px;
  height: 70px;
}
/* 매칭 박스 wrap */
.drag_group {
  display: flex;
  align-items: center;
  justify-content: center;
  gap: 45px;
  padding: 10px 50px;
  background-repeat: no-repeat;
  background-position: center bottom;
}
.drag_group.g1 {
  background-image: url(../../common_contents/img/EMA521_02_SU/0006_bg_contents.png);
  background-size: contain;
  height: 120px;
  margin-top: 40px;
}
/* 매칭 숫자들 */
.droppable {
  font-size: 32px;
  font-weight: bold;
  text-align: center;
  line-height: 70px;
  background-image: none;
  width: 70px;
  height: 70px;
  position: relative;
}
/* 드래그 후 매칭숫자 하위에 생기는 div */
.droppable .ui-draggable-handle {
  top: -100%;
}
/* 제출전 하위div */
.droppable[data-value="true true"] .ui-draggable-handle {
  position: absolute !important;
  top: 0;
}
/* 제출후 상태 */
/* 힌트 */
/* 삼각형 단일 */
.drag_group.hint .droppable[data-value="true,false"].triangle,
.drag_group.hint .droppable[data-value="true,false"] .ui-draggable-handle.triangle {
  background: url(../../common_contents/img/EMA521_02_SU/ico_drag_triangle_hint.svg) no-repeat center center / 100%;
}
/* 원형단일 */
.drag_group.hint .droppable[data-value="false,true"].circle,
.drag_group.hint .droppable[data-value="false,true"] .ui-draggable-handle.circle {
  background: url(../../common_contents/img/EMA521_02_SU/ico_drag_circle_hint.svg) no-repeat center center / 100%;
}
/* 삼각형+원형 */
.drag_group.hint .droppable[data-value="true,true"].circle.triangle,
.drag_group.hint .droppable[data-value="true,true"] .ui-draggable-handle.circle,
.drag_group.hint .droppable[data-value="true,true"] .ui-draggable-handle.triangle {
  background: url(../../common_contents/img/EMA521_02_SU/ico_drag_together_hint.svg) no-repeat center center / 100%;
}
/* 하위 div */
.droppable[data-value="true,true"] .ui-draggable-handle,
.droppable[data-value="false,true"] .ui-draggable-handle,
.droppable[data-value="true,false"] .ui-draggable-handle,
.droppable[data-value="false,false"] .ui-draggable-handle {
  position: absolute !important;
  top: 0;
}
/* 하위div 힌트*/
.drag_group.hint .droppable[data-value="true,true"] .ui-draggable-handle.circle,
.drag_group.hint .droppable[data-value="true,true"] .ui-draggable-handle.triangle,
.drag_group.hint .droppable[data-value="false,true"] .ui-draggable-handle.circle.drag_group.hint .droppable[data-value="true,false"] .ui-draggable-handle.triangle {
  position: absolute !important;
  top: 0;
}
/* 정답 */
.drag_group.correct .triangle {
  background: url(../../common_contents/img/EMA521_02_SU/ico_drag_triangle_correct.svg) no-repeat center center / 100%;
}
.drag_group.correct .circle {
  background: url(../../common_contents/img/EMA521_02_SU/ico_drag_circle_correct.svg) no-repeat center center / 100%;
}
.drag_group.correct .circle.triangle {
  background: url(../../common_contents/img/EMA521_02_SU/ico_drag_together_correct.svg) no-repeat center center / 100%;
}




