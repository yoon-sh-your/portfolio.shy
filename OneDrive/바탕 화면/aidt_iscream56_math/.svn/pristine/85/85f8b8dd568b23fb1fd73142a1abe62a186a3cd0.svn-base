/* ema624_06_ih_0005 */
.ema624_06_ih_0005 .text_wrap {
	width: 700px;
}
.ema624_06_ih_0005 .text_wrap > div {
	width: 100%;
	padding: 25px 28px;
	border-color: #a0cf8e;
	border-style: solid;
	border-radius: 26px;
}
.ema624_06_ih_0005 .text_wrap .box1 {
	border-width: 2px 2px 0 2px;
	position: relative;
}
.ema624_06_ih_0005 .text_wrap .box1:after {
	content: "";
	display: inline-block;
	width: calc(100% - 43px);
	height: 0px;
	border: 1px dashed #a0cf8e;
	bottom: -1px;
	left: 19px;
	position: absolute;
}
.ema624_06_ih_0005 .text_wrap .box2 {
	border-width: 0 2px 2px 2px;
}
.ema624_06_ih_0005 .text_wrap dt {
	display: flex;
	align-items: center;
	gap: 14px;
	margin-bottom: 16px;
}
.ema624_06_ih_0005 .text_wrap dt .label_method {
	width: 72px;
	height: 35px;
	line-height: 31px;
	font-size: 20px;
}
.ema624_06_ih_0005 .text_wrap dt p {
	font-weight: 700;
	font-size: 24px;
}
.ema624_06_ih_0005 .text_wrap math-field {
	height: 80px;
	width: 643px;
	min-height: unset;
}
.ema624_06_ih_0005 .text_wrap .example_box {
	width: 643px;
	/* position: absolute;
	top: 100%; */
}
.ema624_06_ih_0005 .text_wrap .example_box p {
	white-space: normal;
	gap: 0;
	flex-wrap: wrap;
}
.ema624_06_ih_0005 .input_area {
	margin-left: 60px;
	gap: 5px;
}
.ema624_06_ih_0005 .solve_area .conts_solve .nemo {
	margin: 0;
}
.nemo {
	display: inline-block;
	width: 32px;
	height: 32px;
	border: 2px solid var(--primary-blue);
	border-radius: 4px;
}
