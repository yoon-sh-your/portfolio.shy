.title .icon_creative {flex-shrink:0;}
.box_line {border:20px solid #C2E1EE; border-radius:90px;}
.box_line .inner {display:flex; align-items:center; min-height:116px; margin:-12px; padding:0 80px; gap:40px; border:4px dashed #fff; border-radius:90px;}
.box_dot {border:2px dashed #D2D2D2; border-radius:20px; background:#EEF9FF; padding:10px;}
.box_dot .inner {display:flex; align-items:center; background:#fff; border:2px dashed #8CD6FF; border-radius:12px;}
.input_wrap {width:120px;}
.boolean_wrap.type_chk button {position:relative; width:auto; border:none; border-radius:0; background:none !important; aspect-ratio:unset; padding:0;}
.boolean_wrap.type_chk button:after {content:''; display:inline-block; width:60px; height:60px; mask: url(./../../common_practice/common/img/icon_checked_round.svg) no-repeat 50% / 100% auto; background-color:transparent; position:absolute; top:50%; left:-47px; transform:translateY(-50%); margin-top:-8px;}
.boolean_wrap.type_chk button.selected:after {background-color: var(--font-main);}
.result .boolean_wrap.type_chk button[data-answer-single='true']:after {background-color: var(--secondary-red);}
.result .boolean_wrap.type_chk button.selected[data-answer-single='true']:after {background-color: var(--primary-blue);}
.gap_200 {gap:200px;}
.long_division .top {display:flex; justify-content:center;}
.long_division .top > span {display:block; width:100%; padding:0 12px; text-align: right;}
.long_division .top:has(.input_wrap) {margin-bottom:8px;;}
.long_division .top .text_hint {top:-36px;}
.long_division .row {position:relative;}
.long_division .row .left {position:absolute; top:0; left:0; transform:translateX(-101%)}
.long_division .row .right {display:flex; justify-content:center; align-items:center; border-top:1px solid #222; background:url(../img/EMA622_02_IH/long_divi.svg) no-repeat left top / auto 100%;}
.long_division .row .right,
.long_division .under,
.long_division .bottom {padding:0 12px;}
.long_division .row .right > span,
.long_division .under > span,
.long_division .bottom > span {font-size:0;}
.long_division span em {font-size:32px;;}
.long_division .bottom {border-top:1px solid #222;}
.long_division em {display:inline-block; font-style:normal; width:0.8em; text-align:center;}
.long_division .dot {width:auto; text-align:center;}
.long_division .hidden {opacity:0;}
.completed .long_division .hidden {opacity:1;}
.completed .long_division .red {color:#f00;}

.boolean_wrap.type_chk button {position:relative; width:auto; border:none; border-radius:0; background:none !important; aspect-ratio:none; padding:0;}
.boolean_wrap.type_chk button:after {content:''; display:inline-block; width:60px; height:60px; mask: url(./../../common_practice/common/img/icon_checked_round.svg) no-repeat 50% / 100% auto; background-color:transparent; position:absolute; top:50%; left:-47px; transform:translateY(-50%); margin-top:-8px;}
.boolean_wrap.type_chk button.selected:after {background-color: var(--font-main);}
.result .boolean_wrap.type_chk button[data-answer-single='true']:after {background-color: var(--secondary-red);}
.result .boolean_wrap.type_chk button.selected[data-answer-single='true']:after {background-color: var(--primary-blue);}
.boolean_wrap .num_sign {margin-right:10px;}
.num_sign {flex-shrink:0;}

.ema622_06_ih_0001 .box {position:relative; display:flex; justify-content:center; width:314px; height:520px; padding:20px 0; border:2px dashed #d2d2d2; border-radius:30px;}
.ema622_06_ih_0001 .box .long_division .right {justify-content:flex-start;}
.ema622_06_ih_0001 .box .canvas-container {position:absolute !important; top:0; left:0;}
.ema622_06_ih_0001.completed .box .canvas-container {display:none;} 

.ema622_06_ih_0002 .table_wrap {width:840px;}
.ema622_06_ih_0002 .table_wrap th {width:33.333%; background-color:#E2EDD7;}
.ema622_06_ih_0002 .table_wrap .text_hint {top:50%; left:100%; width:auto; padding:0 16px; transform:translateY(-50%);}

.ema622_06_ih_0003 .box {display:flex; justify-content:center; align-items:center; width:267px; height:140px; background-position:center center; background-repeat:no-repeat; background-size:contain;}
.ema622_06_ih_0003 .box1 {background-image:url(../img/EMA622_06_IH/0003_bg1.svg);}
.ema622_06_ih_0003 .box2 {background-image:url(../img/EMA622_06_IH/0003_bg2.svg);}

.ema622_06_ih_0004 .boolean_wrap button {width:auto; height:auto; background:none !important; aspect-ratio:unset; border:none;}
.ema622_06_ih_0004 .boolean_wrap button .inr_btn {display:flex; justify-content:center; align-items:center; width:110px; height:110px; border-radius:50%; border:6px solid transparent;}
.ema622_06_ih_0004 .boolean_wrap button.selected .inr_btn {border-color:#222;}
.ema622_06_ih_0004.completed .boolean_wrap button[data-answer-single="true"] .inr_btn {border-color:var(--secondary-red); background:rgba(255, 67, 67, 0.3);}
