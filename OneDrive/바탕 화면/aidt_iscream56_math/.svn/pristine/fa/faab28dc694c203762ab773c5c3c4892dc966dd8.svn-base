.ema622_06_ih_0005 .box_line .inner {height:292px;}

.ema622_06_ih_0006 .img {position:relative;}
.ema622_06_ih_0006 .img span {position:absolute; background:#fff;}
.ema622_06_ih_0006 .img .txt1 {top:0; left:233px; transform:translateX(-50%)}
.ema622_06_ih_0006 .img .txt2 {top:117px; left:365px;}

