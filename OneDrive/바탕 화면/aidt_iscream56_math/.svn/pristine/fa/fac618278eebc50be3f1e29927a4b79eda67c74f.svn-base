/* ema613_04_ih_0005 */
.ema613_04_ih_0005 .img_area {
  position: relative;
}
.ema613_04_ih_0005 .img_area .texts {
  position: absolute;
  top: 84px;
  left: 50%;
  transform: translateX(-50%);
}
.ema613_04_ih_0005 .img_area .texts > p {
  margin-bottom: 18px;
}
.ema613_04_ih_0005 .boolean_wrap {
  position: absolute;
  top: 79px;
  left: 126px;
}
.ema613_04_ih_0005 .boolean_wrap button {
  background: transparent;
  border-width: 0;
  width: 200px;
  height: 60px;
  margin-bottom: 5px;
  position: relative;
  border-radius: 0;
}
.ema613_04_ih_0005 .boolean_wrap button:after {
  content: '';
  display: inline-block;
  width: 60px;
  height: 60px;
  background: url(./../common/img/icon_checked_round.svg);
  position: absolute;
  left: -34px;
  top: -10px;
  opacity: 0;
}
.ema613_04_ih_0005 .boolean_wrap button.selected:after {
  opacity: 1;
}
.ema613_04_ih_0005.completed .boolean_wrap button[data-answer-single='true']:after {
  filter: invert(49%) sepia(100%) saturate(3512%) hue-rotate(345deg) brightness(93%) contrast(101%);
  opacity: 1;
}
.ema613_04_ih_0005.completed .boolean_wrap button[data-answer-single='true'].selected:after {
  filter: invert(48%) sepia(46%) saturate(7213%) hue-rotate(214deg) brightness(101%) contrast(101%);
}
