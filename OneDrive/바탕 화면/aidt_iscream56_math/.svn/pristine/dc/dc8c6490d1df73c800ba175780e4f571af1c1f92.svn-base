/* ####  ema_07_005  */
.ema616_07_ih_0005 .boolean_wrap {
  width: 638px;
  height: 400px;
  margin: 0 auto;
  background: url(./../img/EMA616_07_IH/twochildren.png) no-repeat;

  position: relative;
}

.ema616_07_ih_0005 .boolean_wrap button {
  min-width: unset;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 120px;
  height: 120px;
  aspect-ratio: 1 / 1;
  color: var(--font-main);
  background-color: var(--bg-default);
  border-radius: 50%;
}

.ema616_07_ih_0005 .boolean_wrap button:nth-of-type(2) {
  right: 110px;
}

.ema616_07_ih_0005 .boolean_wrap button {
  position: absolute;
  background: none;
  font-size: 18px;
  top: 200px;
  color: transparent;
}
.ema616_07_ih_0005 .boolean_wrap {
  position: relative;
  /* border:1px solid;  */
}

.ema616_07_ih_0005 .boolean_wrap .talkbox p {
  font-size: 24px;
  font-family: 'Pretendard', sans-serif;
  font-weight: 400;
  color: var(--font-main);
  line-height: 1.5;
  letter-spacing: -0.5px;
}
.ema616_07_ih_0005 .boolean_wrap .talkbox p:first-child {
  width: 225px;
  position: absolute;
  top: 10px;
  left: 10px;
  text-align: center;
  word-break: keep-all;
}

.ema616_07_ih_0005 .boolean_wrap .talkbox p:last-child {
  width: 250px;
  position: absolute;
  top: 50px;
  right: 127px;
  text-align: center;
}

