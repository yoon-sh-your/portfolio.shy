/* ema522_09_su_0002 */
.ema522_11_su_0002 {
  .empty_box {
    display: inline-block;
    width: 32px;
    height: 32px;
    border: 2px solid var(--primary-blue);
    border-radius: 4px;
  }

  .input_wrap math-field:not(.textarea) {
    width: 80px;
  }

  .container {
    width: 725px;
    height: 200px;
    background-image: url(../img/EMA522_11_SU/0002_img02.png);
    display: flex;
    justify-content: center;
    align-items: center;
    gap: 20px;
  }
}

/* .ema522_09_su_0003 */
.ema522_11_su_0003 {
  .sign_start {
    position: relative;
    top: -10px;
    font-size: 100px;
    font-weight: 100;
    line-height: normal;
  }

  .question_box {
    padding: 40px 50px;
    margin: 0 20px;
    border: 5px solid #a0d1c0;
    border-radius: 30px;
    height: 292px;
    gap: 20px;

    .tit {
      display: flex;
      gap: 10px;
      align-items: center;
      justify-content: center;
      font-size: 20px;
      line-height: 32px;
      letter-spacing: -0.5px;
    }
  }

  .input_wrap,
  .input_wrap input,
  .input_wrap math-field {
    width: 80px;
    min-width: 80px;
  }

  .page_1 {
    .label {
      display: inline-flex;
      width: 107px;
      height: 52px;
      background: url(../../common_contents/common/img/title_mid_9.svg) no-repeat center;
    }

    .label i {
      position: absolute;
      width: 1px;
      height: 1px;
      padding: 0;
      margin: -1px;
      overflow: hidden;
      clip: rect(0, 0, 0, 0);
      white-space: nowrap;
      border: 0;
    }
  }

  .page_2 {
    .label {
      display: inline-flex;
      width: 107px;
      height: 52px;
      background: url(../../common_contents/common/img/title_mid_10.svg) no-repeat center;
    }

    .label i {
      position: absolute;
      width: 1px;
      height: 1px;
      padding: 0;
      margin: -1px;
      overflow: hidden;
      clip: rect(0, 0, 0, 0);
      white-space: nowrap;
      border: 0;
    }
  }

  .question_box {
    /* flex-wrap: wrap;
        width: 1000px;
        gap: 100px 200px; */

    .flex {

      &:nth-child(1),
      &:nth-child(2) {

        .input_wrap,
        .input_wrap input,
        .input_wrap math-field {
          width: 80px;
          min-width: 80px;
        }
      }
    }

    &>.flex {
      flex: 1;
    }
  }
}

/* .ema522_09_su_0004 */
.ema522_11_su_0004 {
  .question_box {
    flex-wrap: wrap;
    width: 1100px;
    gap: 100px 200px;

    .flex {

      &:nth-child(1),
      &:nth-child(2) {

        .input_wrap,
        .input_wrap input,
        .input_wrap math-field {
          width: 247px;
          min-width: 80px;
        }
      }
    }

    &>.flex {
      flex: 1;
    }
  }
}

