.ema624_11_ih_0001 .title.puzzle {
	background-image: url(../img/EMA624_11_IH/title_puzzle.svg);
}

.ema624_11_ih_0001 .map_area,
.ema624_11_ih_0001 .img_wrap {
	position: relative;
}

.ema624_11_ih_0001 .map_area .map {
	max-height: 650px;
}
.ema624_11_ih_0001 .map_area svg,
.ema624_11_ih_0001 .map_area [class^="obj"] {
	position: absolute;
}
.ema624_11_ih_0001 .map_area svg {
	top: 0;
	left: 0;
}
.ema624_11_ih_0001 .map_area .obj_move {
	top: 10px;
	left: 25px;
	width: 90px;
}
.ema624_11_ih_0001 .map_area .obj_arrival {
	bottom: 29px;
	right: 18px;
	width: 104px;
	opacity: 0;
}
.ema624_11_ih_0001 .pop_area .quiz_pop {
	display: flex;
	justify-content: center;
	align-items: center;
	width: 1635px;
	height: 684px;
	padding: 40px;
	background-color: var(--bg-default);
	border-radius: 20px;
	position: absolute;
	top: 50%;
	left: 50%;
	transform: translate(-50%, -50%);
}
.ema624_11_ih_0001 .pop_area .quiz_pop button.close {
	position: absolute;
	top: 40px;
	right: 40px;
	min-width: unset;
	width: 40px;
	height: 40px;
	background: url(../common/img/icon_times_blk.svg) no-repeat center / 40px;
}
.ema624_11_ih_0001 .pop_area .quiz_pop .inner_pop {
	width: 100%;
	max-width: 1200px;
	display: flex;
	flex-direction: column;
	align-items: center;
	gap: 30px;
}
.ema624_11_ih_0001 .pop_area .quiz_pop .txt_quiz {
	display: flex;
	justify-content: center;
	align-items: center;
	width: 100%;
	height: 128px;
	background: #eef9ff;
	border-radius: 128px;
	font-size: 32px;
	font-weight: bold;
}
.ema624_11_ih_0001 .pop_area .quiz_pop .txt_quiz > i {
	display: inline-block;
	vertical-align: middle;
	width: 40px;
	height: 40px;
	border-radius: 50%;
	background: #17baa4;
	text-align: center;
	line-height: 38px;
	font-size: 30px;
	font-weight: 700;
	color: #fff;
	font-style: normal;
	margin-right: 10px;
}
.ema624_11_ih_0001 .pop_area .quiz_pop .txt_quiz > div {
	display: flex;
	align-items: center;
	gap: 5px;
}

.ema624_11_ih_0001 .pop_area .quiz_pop .question_box {
	gap: 140px;
}
.ema624_11_ih_0001 .pop_area .quiz_pop .question_box button {
	display: flex;
	flex-direction: column;
	align-items: center;
	justify-content: center;
	background: none;
	min-width: unset;
	height: auto;
	font-size: 32px;
	color: #000;
}
.ema624_11_ih_0001 .pop_area .quiz_pop .question_box .left:after,
.ema624_11_ih_0001 .pop_area .quiz_pop .question_box .right:after {
	content: "";
	display: block;
	width: 200px;
	height: 106px;
	background: url(../img/EMA515_10_IH/0001_arrow.png) no-repeat;
}
.ema624_11_ih_0001 .pop_area .quiz_pop .question_box .right:after {
	transform: rotate(180deg);
}
@keyframes shake {
	0% {
		margin-left: 0;
	}
	20% {
		margin-left: -30px;
	}
	40% {
		margin-left: 30px;
	}
	60% {
		margin-left: -15px;
	}
	80% {
		margin-left: 15px;
	}
	100% {
		margin-left: 0;
	}
}
.ema624_11_ih_0001 .pop_area .quiz_pop.error {
	animation: shake 1s ease-in-out;
}
.ema624_11_ih_0001 .pop_area .quiz_pop button[data-answer-single="true"] {
	position: relative;
}
.ema624_11_ih_0001 .pop_area .quiz_pop button[data-answer-single="true"]::before {
	content: "";
	position: absolute;
	top: 0;
	left: 0;
	right: 0;
	bottom: 0;
	background: url(../../common_practice/common/img/hint_marking.svg) no-repeat center center / contain;
	z-index: 2;
	opacity: 0;
	transition: 0.2s 0.1s;
}
.ema624_11_ih_0001 .pop_area .quiz_pop.result button[data-answer-single="true"]::before {
	opacity: 1;
}
@keyframes arrival {
	0% {
		transform: translateY(0);
	}
	20% {
		transform: translateY(-10px);
	}
	40% {
		transform: translateY(10px);
	}
	60% {
		transform: translateY(-5px);
	}
	80% {
		transform: translateY(5px);
	}
	100% {
		transform: translateY(0);
	}
}
.ema624_11_ih_0001 .map_area.result .obj_arrival {
	opacity: 1;
	animation: arrival 1s ease-in-out;
}

