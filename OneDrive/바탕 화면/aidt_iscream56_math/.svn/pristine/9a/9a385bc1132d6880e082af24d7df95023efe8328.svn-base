function filterBadWords(checkText) {
    let filteredText = checkText;
    badWords.forEach((badWord) => {
        let escapedBadWord = badWord.replace(/[.*+?^${}()|[\]\\]/g, "\\$&"); // 정규식 특수 문자를 이스케이프
        const regex = new RegExp(`(?<!\\w)${escapedBadWord}(?!\\w)`, "gi");

        filteredText = filteredText.replace(regex, ""); // 금칙어 문구 제거
    });
    return filteredText;
  }

/** 금칙어 설정 기능 */
let isInputComposing = false; // 현재 한글 조합 중인지 여부를 관리하는 플래그

// 모든 input 및 textarea 요소 가져오기
const inputElements = document.querySelectorAll("input, textarea");

// 조합이 시작되었을 때
inputElements.forEach(element => {
    element.addEventListener("compositionstart", () => {
        isInputComposing = true;
    });

    element.addEventListener("compositionend", function () {
        isInputComposing = false;
        // 조합이 끝난 후에만 텍스트를 검사
        let filteredContent = filterBadWords(this.value);
        if (filteredContent !== this.value) {
            this.value = filteredContent;
            toastCheckMsg("바른 말을 써 보세요.");
        }
    });

    element.addEventListener("input", function (event) {
        event.preventDefault();
        event.stopPropagation();

        // 금칙어 있으면 경고 메시지 표시
        if (!isInputComposing) {
            let filteredContent = filterBadWords(this.value);
            if (filteredContent !== this.value) {
                this.value = filteredContent;
                toastCheckMsg("바른 말을 써 보세요.");
            }
        }
    });
});