
/* ema617_03_su_0002 */
.ema617_03_su_0002 .input_area {
  margin-bottom:30px;
}
.ema617_03_su_0002 .input_wrap {
  margin: 0 auto;
}
.ema617_03_su_0002 dt {
  width: 189px;
  min-height: 72px;
  padding:12px 0;
  align-content: center;
  border-radius: 20px;
  background-color: #FFDE6A;
}
.ema617_03_su_0002 dd {
  width: 1103px;
  height: 100%;
}
.ema617_03_su_0002 dd math-field.textarea {
  width: 100%;
  min-height: 100%;
}
.ema617_03_su_0002 dd.h_cst1 math-field.textarea {
  height: 142px;
  min-height: 142px;
}
.ema617_03_su_0002 dd.h_cst1 .canvas_wrap {
  margin-top:10px;
  padding: 14px 10px 14px 20px;
  font-size: 24px;
  font-weight: 700;
  border: 1px solid var(--stroke-default);
  border-radius: 12px;
  background: var(--bg-input);
}
.ema617_03_su_0002 dd.h_cst2 math-field.textarea {
  height: 168px;
  min-height: 168px;
}


