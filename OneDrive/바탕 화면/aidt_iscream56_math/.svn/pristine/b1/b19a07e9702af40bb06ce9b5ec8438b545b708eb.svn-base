  border-radius: 0;
  padding: 0;
  font-size: 0;
  transform-origin: left center;
  z-index: 1;
  border: 2px solid #231f20;
  background-color: transparent;
  background-image: none;
}
.ema525_05_ih_0003 .custom_select.math_symbol .select_trigger.completion[data-value='dotted'] {
  border: 2px dashed #231f20;
}
.ema525_05_ih_0003 .custom_select.math_symbol .select_trigger.completion[data-value='solid'] {
  background-color: #231f20;
}
.ema525_05_ih_0003 .custom_select.math_symbol .select_options {
  top: 10px;
}
.ema525_05_ih_0003 .custom_select.math_symbol .select_options li[data-value='solid'] {
  background-size: 29px;
  background-image: url(../../common_practice/img/EMA525_05_IH/dropdown_solid.svg);
}
.ema525_05_ih_0003 .custom_select.math_symbol .select_options li[data-value='dotted'] {
  background-size: 32px;
  background-image: url(../../common_practice/img/EMA525_05_IH/dropdown_dashed.svg);
}
