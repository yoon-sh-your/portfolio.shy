/*dropdown*/
.select_line_type {
  display: none;
  position: absolute;
  top: 0;
  left: 50%;
  transform: translate(-50%, 0);
  padding-top: 16px;
  z-index: 10;
}
.select_type_options {
  position: relative;
  padding: 8px;
  height: 70px;
  width: 100%;
  max-height: 301px;
  background-color: var(--primary-blue-800);
  white-space: nowrap;
  border-radius: 70px;
  border: 1px solid var(--stroke-default);
  box-shadow: 4px 4px 16px 0px rgba(0, 0, 0, 0.2);
  z-index: 999;
}
.select_type_options:after {
  content: '';
  width: 29px;
  height: 15px;
  background: url(../../common_practice/common/img/icon_bubble_tail.svg) no-repeat top center / 100%;
  border-radius: 70px;
  position: absolute;
  top: -15px;
  left: 50%;
  transform: translateX(-50%);
}
.select_type_options li {
  cursor: pointer;
  position: relative;
  display: inline-block;
  width: 52px;
  height: 52px;
  padding: 0;
  background-color: var(--bg-default);
  background-size: 32px;
  background-repeat: no-repeat;
  background-position: center;
  border: 1px solid var(--stroke-default);
  font-size: 0;
  border-radius: 50%;
  vertical-align: top;
  line-height: 36px;
  font-weight: 400;
  letter-spacing: -0.5px;
  cursor: pointer;
  position: relative;
}
.select_type_options li:not(:last-child) {
  margin-right: 12px;
}
.select_type_options li:hover {
  background: var(--primary-blue-50);
  background-repeat: no-repeat;
  background-position: center;
  background-size: 32px;
}
.select_type_options li[data-value='solid'] {
  background-image: url(../../common_practice/img/EMA525_05_IH/dropdown_solid.svg);
}
.select_type_options li[data-value='dotted'] {
  background-image: url(../../common_practice/img/EMA525_05_IH/dropdown_dashed.svg);
}
/*Grid*/
.drawline_grid {
  position: relative;
  z-index: 2;
}
.drawing_grid_area {
  position: relative;
  display: flex;
  align-items: center;
  width: 800px;
  height: 400px;
  border: 0;
  background-image: url(../../common_practice/img/EMA525_05_IH/003_grid.svg);
  background-repeat: no-repeat;
  background-size: 100%;
}
.drawing_grid_area .hit_area {
  background: rgba(0, 0, 0, 0);
}

.drawing_grid_area .point {
  width: 6px;
  height: 6px;
  background-color: #231f20;
  border-radius: 50%;
  position: absolute;
  transform: translate(-50%, -50%);
}

.drawing_grid_area .point.end-point {
  background-color: #0066cc;
}

.drawing_grid_area .default_line {
  stroke: #ff0000;
  stroke-width: 2px;
}
/* .line_canvas line {
  stroke: #231f20;
  stroke-width: 4px;
} */
.line_canvas line.guide-dash {
  stroke-dasharray: 5 5;
}
.line_canvas line.guide-solid {
  stroke-dasharray: none;
}
.line_canvas line.selected {
  stroke: #3b71fe;
  /* stroke-width: 4; */
}
svg.line_canvas line,
svg.line_canvas path {
  pointer-events: visiblePainted !important;
  cursor: pointer;
}
