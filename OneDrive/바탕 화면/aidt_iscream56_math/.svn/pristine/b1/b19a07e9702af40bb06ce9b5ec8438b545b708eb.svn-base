}
.ema525_05_ih_0003 .dropdown_wrap {
  position: absolute;
  top: 55px;
  left: 50%;
  transform: translateX(-50%);
  padding-top: 10px;
  z-index: 10;
}
.ema525_05_ih_0003 .custom_select.math_symbol {
  height: 16px;
}
.ema525_05_ih_0003 .custom_select.math_symbol .select_trigger {
  display: none;
