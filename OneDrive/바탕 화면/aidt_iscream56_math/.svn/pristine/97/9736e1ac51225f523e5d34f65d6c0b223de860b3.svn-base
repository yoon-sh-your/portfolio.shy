.ema515_06_ih_0001 .boolean_count_wrap {
  flex-wrap: wrap;
  width: 120px;
  box-sizing: content-box;
  gap: 0;
}
.ema515_06_ih_0001 .boolean_count_wrap button {
  width: 40px;
  height: 40px;
  min-width: 0;
}
.ema515_06_ih_0001 .boolean_count_wrap button.selected {
  background-color: #d2d2d2;
}
.ema515_06_ih_0001 .boolean_count_wrap button:nth-child(3n + 1):before {
  display: none;
}
.ema515_06_ih_0001 .boolean_count_wrap button:nth-child(n + 4):after {
  content: "";
  position: absolute;
  width: 100%;
  height: 2px;
  background-image: repeating-linear-gradient(
    to right,
    var(--font-main),
    var(--font-main) 4px,
    transparent 4px,
    transparent 8px
  );
  top: 0;
  left: 0;
}
.ema515_06_ih_0001 .item01 .img .boolean_count_wrap:nth-child(1) button::after,
.ema515_06_ih_0001 .item01 .img .boolean_count_wrap:nth-child(2) button::after,
.ema515_06_ih_0001 .item01 .img .boolean_count_wrap:nth-child(1) button::before,
.ema515_06_ih_0001 .item01 .img .boolean_count_wrap:nth-child(2) button::before,
.ema515_06_ih_0001 .item02 .img .boolean_count_wrap:nth-child(1) button::after,
.ema515_06_ih_0001 .item02 .img .boolean_count_wrap:nth-child(1) button::before {
