/* ema626_05_ih_0007 */
.ema626_05_ih_0007 .text_area {
	position: absolute;
	top: 50%;
	left: 50%;
	transform: translate(-50%, -50%);
}
.ema626_05_ih_0007 .text_area li {
	display: flex;
	align-items: center;
	gap: 10px;
}
.ema626_05_ih_0007 .text_area li + li {
	margin-top: 10px;
}
.ema626_05_ih_0007 .text_area p {
	font-weight: 700;
}
.ema626_05_ih_0007 .boolean_wrap {
	position: absolute;
	top: 0;
	right: 0;
	bottom: 0;
	left: 0;
}
.ema626_05_ih_0007 .boolean_wrap div {
	width: 100%;
}
.ema626_05_ih_0007 .boolean_wrap button {
	border-color: transparent;
	background-color: transparent;
	width: 100%;
	height: 50px;
	border-radius: 0;
	aspect-ratio: unset;
	padding: 0 60px;
	position: relative;
}
.ema626_05_ih_0007 .boolean_wrap button + button {
	margin-top: 10px;
}
.ema626_05_ih_0007 .boolean_wrap button:after {
	content: "";
	display: inline-block;
	width: 60px;
	height: 60px;
	mask: url(../common/img/icon_checked_round.svg) no-repeat 50% / 100% auto;
	background-color: transparent;
	position: absolute;
	top: -13px;
	left: 0;
}
.ema626_05_ih_0007 .boolean_wrap button.selected:after {
	background-color: var(--font-main);
}
.ema626_05_ih_0007 .boolean_wrap button.correct[data-answer-single="true"]:after {
	background-color: var(--primary-blue);
}
.ema626_05_ih_0007 .boolean_wrap button.hint[data-answer-single="true"]:after {
	background-color: var(--secondary-red);
}
.ema626_05_ih_0007 .input_area {
	display: flex;
	align-items: flex-start;
	gap: 10px;
}
