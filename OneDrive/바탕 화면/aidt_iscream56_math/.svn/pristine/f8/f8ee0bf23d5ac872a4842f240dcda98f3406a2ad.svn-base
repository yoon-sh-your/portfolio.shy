.question_box {white-space:normal;}
.title.group {padding-bottom:0;}
.tit_data {display:flex; justify-content:center; margin-bottom:14px;}
.tit_data .inner {position:relative; padding:0 6px;}
.tit_data .inner:before {content:''; position:absolute; top:50%; left:0; width:100%; height:22px; transform:translateY(-50%); border-radius:22px; background:#FDDDCE;}
.tit_data span {position:relative; display:flex; align-items:center; justify-content:center; height:32px; padding:0 16px; border:2px solid #FDDDCE; border-radius:32px; font-size:24px; font-weight:bold; color:#911C1A; background-color:#fff;}
.table_wrap {width:100%; max-width:960px;}
.table_wrap th,
.table_wrap td {font-weight:normal; height:46px; font-size:24px;}
.table_wrap thead th {background-color:#FDE6DB;}
.table_wrap tbody th {background-color:#EAEBEC;}
.table_wrap.row th,
.table_wrap.row td {height:52px;}
.table_wrap.row thead td {background-color:#EAEBEC;}
.table_wrap.row thead tr:first-child td:last-child {border-radius:0 19px 0 0;}
.table_wrap.row tbody th {background-color:#FDE6DB;}
.table_wrap.row thead td {border-left:2px solid #fff;;}


.ema526_03_ih_0001 .question_box,
.ema526_03_ih_0002 .question_box {margin-top:70px;}
.ema526_03_ih_0001 .question_box .inner,
.ema526_03_ih_0002 .question_box .inner {display:flex; align-items:center; gap:4px;}

.ema526_03_ih_0003 h3.title {margin-left:40px;}
.ema526_03_ih_0003 .question_box {margin-top:70px;}

.ema526_03_ih_0004 .img {position:relative; flex-shrink:0; text-align:center; color:#58595B;}
.ema526_03_ih_0004 .img span {font-weight:normal;}
.ema526_03_ih_0004 .img .text {position:absolute; top:17px; left:12px; width:270px; line-height:1.3; word-break:keep-all; font-size:22px;} 
.ema526_03_ih_0004 .img .day {position:absolute; top:120px; left:150px; display:flex; width:292px; font-size:18px; gap:2px; color:#fff;}
.ema526_03_ih_0004 .img .day span {width:100%;}
.ema526_03_ih_0004 .img {margin-right:40px;}

.ema526_03_ih_0004 .page_1 .question_box {flex-direction:column;}
.ema526_03_ih_0004 .page_1 .tit_data span {width:260px;}
.ema526_03_ih_0004 .page_1 .table_wrap tbody th {width:160px;}
.ema526_03_ih_0004 .page_1 .table_wrap tbody td {width:156px; padding:20px 0;}
.ema526_03_ih_0004 .page_1 .example_box {display:none;}
.ema526_03_ih_0004 .page_1 .example_box.on {display:block;}
.ema526_03_ih_0004 .page_2 .example_box {position:absolute; top:72px; left:0; margin-top:0;}
.ema526_03_ih_0004 .page_3 .input_wrap {width:712px;}
.ema526_03_ih_0004 .page_3 .input_wrap math-field {width:100%;}

