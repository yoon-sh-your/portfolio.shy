/* ema622_04_su_0003 */
/* 페이지 수: ~ 9 페이지 */
/* picture_wrap 가리는 페이지: 4,5,6,7 페이지 picture_wrap 없음 */
/* 서술형 페이지: 2, 3, 9 페이지 서술형 */
.ema622_04_su_0003 .picture_wrap {
  width: 700px;
  height: 390px;
  border: none;
  /* border: 1px solid orange; */
}

.ema622_04_su_0003 .picture_wrap img,
.ema622_04_su_0003 .picture_wrap svg {
  transform: scale(.7) translate(-70%, -70%);
}

/* picture_wrap 가리는 페이지 */
.ema622_04_su_0003[data-current-page='page_4'] .contents>div.flex,
.ema622_04_su_0003[data-current-page='page_4'] .contents>div.flex>.picture_wrap,
.ema622_04_su_0003[data-current-page='page_5'] .contents>div.flex,
.ema622_04_su_0003[data-current-page='page_5'] .contents>div.flex>.picture_wrap,
.ema622_04_su_0003[data-current-page='page_6'] .contents>div.flex,
.ema622_04_su_0003[data-current-page='page_6'] .contents>div.flex>.picture_wrap,
.ema622_04_su_0003[data-current-page='page_7'] .contents>div.flex,
.ema622_04_su_0003[data-current-page='page_7'] .contents>div.flex>.picture_wrap {
  display: none;
}

.ema622_04_su_0003[data-current-page='page_8'] .contents>div.flex>.picture_wrap,
.ema622_04_su_0003[data-current-page='page_9'] .contents>div.flex>.picture_wrap {
  position: absolute;
  top: calc(30px + 96px + 30px + 48px + 20px);
  left: calc(40px + 70px + 10px);
  width: calc(700px + 40px);
}

