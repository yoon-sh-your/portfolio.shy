  overflow: hidden;
  border-radius: 50%;
}
.ema515_04_ih_0001 .boolean_count_wrap:before,
.ema515_04_ih_0001 .boolean_count_wrap:after {
  content: '';
  position: absolute;
  top: 50%;
  width: 100%;
  height: 2px;
  margin-top: -1px;
  background: url(../img/EMA515_04_IH/0001_line4.svg) no-repeat;
  z-index: 2;
}

.ema515_04_ih_0001 .boolean_count_wrap:before {
  transform: rotate(330deg);
}
.ema515_04_ih_0001 .boolean_count_wrap:after {
  transform: rotate(210deg);
}
.ema515_04_ih_0001 .boolean_count_wrap button {
  position: absolute;
  width: 112px;
  height: 112px;
  background: #f9d5e5;
  top: 0;
  left: 0;
  transform-origin: bottom right;
  opacity: 0;
  clip-path: path('M100 0 C57 0 20 22 0 56 L100 115 Z');
  outline: none;
}
.ema515_04_ih_0001 .img > .boolean_count_wrap:first-child::before,
.ema515_04_ih_0001 .img > .boolean_count_wrap:first-child::after {
  display: none;
}

