}
.input_wrap:has(.w_80) {
  width: 80px !important;
}
math-field.w_80 {
  width: 80px !important;
}
.slash {
  position: relative;
}
.slash:after {
  content: '';
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%) rotate(45deg);
  width: 2px;
  height: 48px;
  background: #e4007f;
}
.slash .slash_txt {
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  color: #e4007f;
  font-size: 24px;
}
.fraction_box > span.slash:first-child .slash_txt {
  top: -32px;
}
.fraction_box > span.slash:last-child .slash_txt {
  bottom: -32px;
}
.fraction_box_wrap .input_wrap,
.fraction_box_wrap .input_wrap math-field,
.input_wrap.fraction_wrap,
.input_wrap.fraction_wrap math-field,
.fraction_box .input_wrap,
.fraction_box .input_wrap math-field {
  min-width: 0;
  width: 80px;
}
.fraction_box_wrap .input_wrap math-field,
.input_wrap.fraction_wrap math-field,
.fraction_box .input_wrap math-field {
  /* min-width: 0; */
  /* width: 100%; */
}
.fraction_box > .flex:first-child .input_wrap .text_hint,
.fraction_box > .input_wrap:first-child .text_hint {
  top: -36px;
}

