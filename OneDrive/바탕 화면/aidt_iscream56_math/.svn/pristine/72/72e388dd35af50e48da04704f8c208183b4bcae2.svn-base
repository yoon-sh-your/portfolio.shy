/* EMA525_03_SU_0004 page_1 */
.scene2 {
  width: 200px;
  height: 200px;
  perspective: 7000px;
  margin: 100px auto;
}

.scene2 .cube {
  position: relative;
  width: 100px;
  height: 100px;
  transform-style: preserve-3d;
  transform: rotateX(-15deg) rotateY(-25deg) scaleZ(2) scale(1.5);
}

.scene2 .face {
  position: absolute;
  width: 150px;
  height: 100px;
  background: rgba(255, 255, 255, 0.9);
  border: 2px solid #222;
  opacity: 0.95;
  border-radius: 0;
}
.scene2 .face.oblong {
  width: 150px;
  height: 100px;
}
.scene2 .face.oblong1 {
  width: 150px;
  height: 150px;
}
.scene2 .face.base-item:after {
  content: "";
  width: 30px;
  height: 30px;
  border: 3px solid #000;
  border-radius: 999px;
}
.scene2 .face.selected {
  background-color: rgba(140, 214, 255, 0.7);
  pointer-events: none;
}

.scene2 .face.disabled {
  pointer-events: none;
}
/* 각 면 배치 */
.scene2 .back {
  transform: translateZ(-75px) rotateX(0deg) rotateY(180deg);
  transform-origin: center bottom;
}
.scene2 .left {
  transform: translateZ(0px) translateX(-75px) rotateY(-90deg) rotateX(0deg);
  transform-origin: center bottom;
}
.scene2 .bottom {
  transform: translateZ(-0px) translateX(0px) translateY(49px) rotateX(-90deg);
}

.scene2 .front {
  transform: translateZ(75px) rotateX(0deg) rotateY(180deg);
  transform-origin: center bottom;
}
.scene2 .right {
  transform: translateZ(0px) translateX(73px) rotateY(-90deg) rotateX(0deg);
  transform-origin: center bottom;
}
.scene2 .top {
  transform: translateZ(-0px) translateX(0px) translateY(-49px) rotateX(-90deg);
}

