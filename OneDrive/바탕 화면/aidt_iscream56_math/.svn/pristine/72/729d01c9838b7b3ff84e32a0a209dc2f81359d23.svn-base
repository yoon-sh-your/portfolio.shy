.relative {
    position: relative;
}

/* .relative:has([class^="division_"]) { */

.relative:has([class^="division_"]) .division_1 {
    position: absolute;
    top: -43px;
    left: 50%;
    transform: translateX(-50%);
    color: #e4007f;

}

.relative:has([class^="division_"]) .division_1::after {
    content: '';
    position: absolute;
    bottom: -34px;
    left: 50%;
    transform: translateX(-50%);
    width: 42px;
    height: 28px;
    background: url(../img/EMA522_02_SU/0005_line.png) no-repeat center;

}

.relative:has([class^="division_"]) .division_2 {
    position: absolute;
    bottom: -43px;
    left: 50%;
    transform: translateX(-50%);
    color: #e4007f;

}

.relative:has([class^="division_"]) .division_2::after {
    content: '';
    position: absolute;
    top: -34px;
    left: 50%;
    transform: translateX(-50%);
    width: 42px;
    height: 28px;
    background: url(../img/EMA522_02_SU/0005_line.png) no-repeat center;
}

/* } */

