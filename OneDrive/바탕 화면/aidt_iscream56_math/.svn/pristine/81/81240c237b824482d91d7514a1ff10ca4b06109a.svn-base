.nemo {
	display: inline-block;
	width: 32px;
	height: 32px;
	border: 2px solid var(--primary-blue);
	border-radius: 4px;
}

.input_area .fraction_box {
	vertical-align: middle;
	margin-left: 5px;
}
.input_wrap:has(.w_80) {
	min-width: 80px !important;
}
math-field.w_80 {
	width: 80px !important;
}
.input_wrap.hint_up .text_hint {
	bottom: calc(100% + 4px);
	top: auto;
}

.input_area:has(.input_sep) > span {
	position: relative;
}
.input_wrap.input_sep {
	position: absolute;
	left: 50%;
	transform: translateX(-50%);
	bottom: -60px;
}
.input_wrap.input_sep.hint_up {
	top: -60px;
	bottom: auto;
}
.input_wrap.input_sep.hint_right .text_hint {
	left: calc(100% + 4px);
	top: 12px;
	width: auto;
}
.mark_error {
	position: relative;
}
.mark_error:after {
	content: "";
	display: inline-block;
	width: 55px;
	height: 3px;
	background: #ff0000;
	position: absolute;
	bottom: 36px;
	left: 50%;
	transform: rotate(-30deg) translateX(-50%);
}
.mark_error.blue:after {
	background: var(--primary-blue);
}
.mark_error i {
	font-style: normal;
	font-size: 24px;
	font-weight: 700;
	color: #ff0000;
	position: absolute;
	top: 35px;
	left: 50%;
	transform: translateX(-50%);
}
.mark_error.up > i {
	bottom: 35px;
}
.fraction_wrap .hint_up .text_hint {
	top: auto;
	bottom: calc(100% + 4px);
}

