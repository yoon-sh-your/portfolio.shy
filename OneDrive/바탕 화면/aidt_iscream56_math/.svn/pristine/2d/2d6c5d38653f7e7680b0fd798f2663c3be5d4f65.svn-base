.ema626_05_ih_0002 .dragndrop_fraction_wrap {
	display: flex;
	align-items: center;
	gap: 60px;
}
.ema626_05_ih_0002 .drag_group {
	flex-direction: column;
}
.ema626_05_ih_0002 .drag_group li {
	justify-content: center;
	display: flex;
	gap: 20px;
}
.ema626_05_ih_0002 .drag_item {
	z-index: 10;
	text-align: center;
}
.ema626_05_ih_0002 .drag_item.dropped {
	height: 60px;
	line-height: 58px;
	width: 100%;
	display: inline-block;
}
.ema626_05_ih_0002 .drop_area {
	position: relative;
}
.ema626_05_ih_0002 .drop_group {
	position: absolute;
	top: 0;
}
.ema626_05_ih_0002 .drop_group .drop_item {
	width: 120px;
	height: 60px;
	border: 2px solid var(--primary-blue);
	border-radius: 12px;
	position: absolute;
	z-index: 10;
}
.ema626_05_ih_0002 .drop_group .drop1 {
	width: 227px;
	top: 0;
	left: 387px;
}
