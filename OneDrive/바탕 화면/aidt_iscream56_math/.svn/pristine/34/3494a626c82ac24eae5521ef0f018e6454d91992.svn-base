/* .input_area .fraction_box {
	vertical-align: middle;
	margin-left: 5px;
} */
.input_wrap:has(.w_80) {
  min-width: 80px !important;
}
math-field.w_80 {
  width: 80px !important;
}
.input_wrap.hint_up .text_hint {
  bottom: calc(100% + 4px);
  top: auto;
}

.input_area:has(.input_sep) > span {
  position: relative;
}
.input_wrap.input_sep {
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  bottom: -60px;
}

.input_wrap.hint_right .text_hint {
  left: calc(100% + 20px);
  top: 12px;
  width: auto;
}
.mark_error {
  position: relative;
}

.mark_error:after {
  content: '';
  display: inline-block;
  bottom: 20px;
  width: 37px;
  height: 2px;
  background: #e4007f;
  position: absolute;
  left: 50%;
  transform: rotate(-30deg) translateX(-50%);
}
.mark_error.blue:after {
  background: var(--primary-blue);
}
.mark_error i {
  font-style: normal;
  font-size: 24px;
  font-weight: 700;
  color: #e4007f;
  position: absolute;
  top: 40px;
  left: 50%;
  transform: translateX(-50%);
}
.mark_error.up > i {
  top: auto;
  bottom: 50px;
}
.fraction_wrap .hint_up .text_hint {
  top: auto;
  bottom: calc(100% + 4px);
}
.fraction_wrap.hint_right .text_hint {
  top: 18px;
}
.label_method {
  display: inline-block;
  color: #a1d08f;
  background: #fff;
  border: 3px solid #a1d08f;
  font-size: 16px;
  width: 52px;
  height: 32px;
  border-top-left-radius: 20px;
  border-top-right-radius: 20px;
  border-bottom-left-radius: 8px;
  border-bottom-right-radius: 8px;
  line-height: 32px;
  text-align: center;
}
math-field.has_ex_text + .text_hint:before {
  content: 'ì˜ˆ';
  display: inline-block;
  vertical-align: middle;
  margin-right: 10px;
  width: 44px;
  height: 44px;
  background: var(--primary-blue);
  border-radius: 12px;
  color: #fff;
  line-height: 42px;
}

.ex_tag {
  width: 52px;
  height: 32px;
  background-color: rgba(255, 255, 255, 1);
  border: 2px solid #ff0000;
  border-radius: 4px;
  display: flex;
  justify-content: center;
  align-items: center;
  font-weight: 700;
  font-size: 16px;
  color: #ff0000;
  line-height: 24px;
  letter-spacing: -0.5px;
}

