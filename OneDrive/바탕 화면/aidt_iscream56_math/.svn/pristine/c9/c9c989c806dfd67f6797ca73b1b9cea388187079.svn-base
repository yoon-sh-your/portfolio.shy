}
/* ema512_02_su_0004 */
.ema512_02_su_0004 .input_area > article {
  width: 1635px;
}
.ema512_02_su_0004 .input_area {
  margin-bottom: 80px;
}
.ema512_02_su_0004 .page_1 .question_box {
  width: 1465px;
  justify-content: space-between;
  justify-content: space-evenly;
  margin: 0 auto;
  font-weight: 700;
  font-size: 32px;
}
.ema512_02_su_0004 .page_1 .question_box > * {
  width: 256px;
}
.ema512_02_su_0004 .page_1_cover {
  background: url(../img/EMA512_02_SU/0007_contents.png) no-repeat left top / 100%;
  display: block;
  width: 1465px;
  height: 325px;
  padding-top: 50px;
}

.ema512_02_su_0004 .question_box > div > span {
  position: relative;
}
.ema512_02_su_0004 .question_box > div > span .text-vertical {
  position: absolute;
  top: -9px;
  left: 103px;
}
.ema512_02_su_0004 .page_2 .input_area {
  display: block;
  margin: 0 auto 40px;
  width: fit-content;
}
.ema512_02_su_0004[data-current-page='page_3'] section.title, 
.ema512_02_su_0004 .btnCheck.etcImgOn, 
.ema512_02_su_0004[data-current-page='page_3'] .btnCheck,
.ema512_02_su_0004[data-current-page='page_3'] .btnType {
  display: none;
}
.ema512_02_su_0004[data-current-page='page_3'] .btnCheck.etcImgOn  {
  display: block;
}

.ema512_02_su_0004 .etc_img.etcImgOn{
  display: block;
}
.ema512_02_su_0004 .etc_img {
  position: absolute;
  right: -20px;
  bottom: 40px;
  right: 0;
  bottom: 56px;
  display: none;
}
.ema512_02_su_0004 .etc_img p {
  width: 185px;
  height: 108px;
  font-size: 24px;
  line-height: 1.5;
  word-break: keep-all;
  transform: translate(-195px, 120px);
  text-align: center;
}
.ema512_02_su_0004 .etc_img p::after {
  content: '';
  width: 280px;
  height: 173px;
  background: url(../img/EMA512_02_SU/0009_contents.png) no-repeat left top / 100%;
  display: block;
  z-index: -1;
  position: absolute;
  top: -30px;
  left: -50px;
}
.ema512_02_su_0004 .etc_img img {
  transform: translate(40px, -90px);
}
