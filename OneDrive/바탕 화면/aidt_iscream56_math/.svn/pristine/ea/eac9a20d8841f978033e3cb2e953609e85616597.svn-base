/* ema521_07_ih_0005 */
.ema521_07_ih_0005 .text_area {
	width: 630px;
	height: 408px;
	background: #ffeee6;
	border-radius: 70px;
	padding: 40px 60px 0 84px;
	margin-right: 60px;
}
.ema521_07_ih_0005 .text_area li {
	text-indent: -36px;
	white-space: normal;
	word-break: keep-all;
}
.ema521_07_ih_0005 .text_area li + li {
	margin-top: 20px;
}
.ema521_07_ih_0005 .dropdown_area {
	display: flex;
	margin-top: 76px;
	align-items: center;
}
.ema521_07_ih_0005 .custom_select {
	width: 167px;
}
