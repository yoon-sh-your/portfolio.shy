.ema625_11_ih_0001#app_wrap {
	max-height: unset;
}
.ema625_11_ih_0001 .title .square {
	display: inline-block;
	font-size: 0;
	width: 40px;
	height: 40px;
	border-radius: 4px;
	background-color: #7dc058;
	margin-right: 6px;
}

.ema625_11_ih_0001 .title .btn_zoom {
	margin-left: 10px;
}

.ema625_11_ih_0001 .question_box {
	width: 1435px;
	margin: 0 auto;
	position: relative;
}

.ladder-container {
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	margin: 0 auto;
	padding: 20px;
	border-radius: 10px;
}

.ema625_11_ih_0001 .btn_area {
	position: relative;
	bottom: 0;
	margin-top: 30px;
}

.obj_box {
	position: absolute;
}

.obj_box1 {
	top: 381px;
	left: 252px;
	width: 921.7px;
	height: 720.9px;
}

.obj_box2 {
	top: 381px;
	left: 252px;
	width: 921.7px;
	height: 720.9px;
}

.obj_box3 {
	top: 381px;
	left: 252px;
	width: 921.7px;
	height: 720.9px;
}

.path-draw {
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	pointer-events: none;
}
.obj_box1 .path-draw path {
	stroke: var(--secondary-red);
}
.obj_box2 .path-draw path {
	stroke: #22b24c;
}
.obj_box3 .path-draw path {
	stroke: #6247dc;
}
.path-draw path {
	display: flex;
	align-items: flex-end;
	stroke: #ff6b6b;
	stroke-width: 8;
	stroke-linecap: round;
	fill: none;
	stroke-dasharray: 4000;
	stroke-dashoffset: 4000;
}

@keyframes dummyAni {
	from {
		opacity: 1;
	}
	to {
		opacity: 1;
	}
}
.obj {
	width: 0;
	height: 0;
	overflow: hidden;
	position: absolute;
	visibility: hidden;
	animation: dummyAni 6.5s linear forwards;
}
