.active .path-draw.path1 path {
	animation: drawPath 5.5s linear forwards;
}

.active .path-draw.path2 path {
	animation: drawPath 5.5s linear forwards;
}

.active .path-draw.path3 path {
	animation: drawPath 5.5s linear forwards;
}

@keyframes drawPath {
	to {
		stroke-dashoffset: 100%;
	}
}

