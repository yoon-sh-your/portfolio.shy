/* 테이블 */
.table_box {
  margin: 0 auto;
  display: flex;
  flex-direction: column;
  gap: 14px;
}

.table_box .title {
  width: 540px;
  padding: 0 124px;
  margin: 0 auto;
  background-color: rgba(255, 255, 255, 1);
  border: 3px solid rgba(253, 227, 215, 1);
  border-radius: 24px;
  font-weight: 700;
  font-size: 32px;
  line-height: 48px;
  letter-spacing: -1px;
  position: relative;
}

.table_box .title::before {
  content: "";
  width: calc(100% + 20px);
  height: 30px;
  background-color: rgba(253, 227, 215, 1);
  border-radius: 15px;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  z-index: -1;
}

.table_box table {
  position: relative;
}

.table_box table::before {
  content: "";
  width: 215px;
  height: 100%;
  border: 2px solid rgba(253, 235, 227, 1);
  border-right: 0;
  border-top-left-radius: var(--table-radius);
  border-bottom-left-radius: var(--table-radius);
  position: absolute;
  left: 0;
  bottom : -2px;
}

.table_box table::after {
  content: "";
  width: 892px;
  height: 100%;
  border: 2px solid rgba(218, 218, 218, 1);
  border-left: 0;
  border-top-right-radius: var(--table-radius);
  border-bottom-right-radius: var(--table-radius);
  position: absolute;
  right: 0;
  bottom : -2px;
}

.table_box tr th,
.table_box tr td {
  height: 60px;
  position: relative;
  font-weight: 700;
  font-size: 32px;
  line-height: 48px;
  letter-spacing: -1px;
}

.table_box tr th {
  width: 137px;
  background-color: rgba(253, 235, 227, 1);
  position: relative;
}

.table_box tr th::before {
  content: "";
  width: 3px;
  height: calc(100% + 4px);
  background-color: rgba(255, 255, 255, 1);
  position: absolute;
  top: 50%;
  right: -1.5px;
  transform: translateY(-50%);
  z-index: 10;
}

.table_box tr:first-child th {
  border-top-left-radius: var(--table-radius);
}

.table_box tr:last-child th {
  border-bottom-left-radius: var(--table-radius);
}

.table_box tr:not(tr:first-child) th::after {
  content: "";
  width: 92%;
  height: 3px;
  background-color: rgba(255, 255, 255, 1);
  position: absolute;
  top: -1.5px;
  left: 50%;
  transform: translateX(-50%);
  z-index: 10;
}

.table_box tr td {
  width: 114px;
  position: relative;
  text-align: center;
}

.table_box tr:first-child td {
  background-color: rgba(240, 240, 240, 1);
}

.table_box tr:first-child td:last-child {
  border-top-right-radius: var(--table-radius);
}

.table_box tr:first-child td:not(:last-child)::before {
  content: "";
  width: 3px;
  height: 71%;
  background-color: rgba(255, 255, 255, 1);
  position: absolute;
  top: 50%;
  right: -1.5px;
  transform: translateY(-50%);
}

.table_box tr:last-child td:last-child {
  border-bottom-right-radius: var(--table-radius);
}

.table_box tr:not(tr:last-child) td:not(:last-child) {
  border-right: 1px solid rgba(218, 218, 218, 1);
}

.table_box tr:not(tr:first-child, tr:last-child) td {
  border-top: 1px solid rgba(218, 218, 218, 1);
}

