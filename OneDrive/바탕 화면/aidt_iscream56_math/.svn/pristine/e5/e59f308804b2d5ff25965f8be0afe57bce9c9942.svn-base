/*EMA526_02_SU_0004_s*/
/* page_1 */
.ema526_02_su_0004 .page_1 h3 {
  gap: 4px;
}

.ema526_02_su_0004 .page_1 h3 .circle {
  display: inline-block;
  width: 32px;
  height: 32px;
  border: 4px solid rgba(34, 34, 34, 1);
  border-radius: 50%;
  position: relative;
}

.ema526_02_su_0004 .page_1 .left_area {
  position: relative;
}

.ema526_02_su_0004 .page_1 .left_area .title {
  width: 189px;
  padding: 0 30px;
  margin: 0 auto;
  background-color: rgba(255, 255, 255, 1);
  border: 1.67px solid rgba(253, 227, 215, 1);
  border-radius: 13px;
  font-weight: 700;
  font-size: 16px;
  line-height: 26px;
  letter-spacing: -0.5px;
  position: relative;
}

.ema526_02_su_0004 .page_1 .left_area .title::before {
  content: "";
  width: calc(100% + 10px);
  height: 16px;
  background-color: rgba(253, 227, 215, 1);
  border-radius: 8px;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  z-index: -1;
}

.ema526_02_su_0004 .page_1 .left_area .table_box {
  display: flex;
  flex-direction: column;
  gap: 7px;
}

.ema526_02_su_0004 .page_1 .left_area .table_box table {
  position: relative;
}

.ema526_02_su_0004 .page_1 .left_area .top_box .table_box table::before {
  content: "";
  width: 137px;
  height: 66px;
  border: 1px solid rgba(253, 235, 227, 1);
  border-right: 0;
  border-top-left-radius: 14px;
  border-bottom-left-radius: 14px;
  position: absolute;
  left: 0;
  bottom : -1px;
}

.ema526_02_su_0004 .page_1 .left_area .top_box .table_box table::after {
  content: "";
  width: 570px;
  height: 66px;
  border: 1px solid rgba(218, 218, 218, 1);
  border-left: 0;
  border-top-right-radius: 14px;
  border-bottom-right-radius: 14px;
  position: absolute;
  right: 0;
  bottom : -1px;
}

.ema526_02_su_0004 .page_1 .left_area .top_box .table_box tr th,
.ema526_02_su_0004 .page_1 .left_area .top_box .table_box tr td {
  height: 33px;
  position: relative;
  font-weight: 700;
  font-size: 16px;
  line-height: 24px;
  letter-spacing: -0.5px;
}

.ema526_02_su_0004 .page_1 .left_area .top_box .table_box tr th {
  width: 137px;
  background-color: rgba(253, 235, 227, 1);
}

.ema526_02_su_0004 .page_1 .left_area .top_box .table_box tr:first-child th {
  border-top-left-radius: 14px;
}

.ema526_02_su_0004 .page_1 .left_area .top_box .table_box tr:last-child th {
  border-bottom-left-radius: 14px;
}

.ema526_02_su_0004 .page_1 .left_area .top_box .table_box tr:last-child th::before {
  content: "";
  width: 126px;
  height: 2px;
  background-color: rgba(255, 255, 255, 1);
  position: absolute;
  top: -1px;
  left: 50%;
  transform: translateX(-50%);
  z-index: 10;
}

.ema526_02_su_0004 .page_1 .left_area .top_box .table_box tr td {
  width: 114px;
  position: relative;
  text-align: center;
}

.ema526_02_su_0004 .page_1 .left_area .top_box .table_box tbody::before {
  content: "";
  width: 2px;
  height: calc(100% + 2px);
  background-color: rgba(255, 255, 255, 1);
  position: absolute;
  top: -1px;
  left: 135px;
  z-index: 10;
}

.ema526_02_su_0004 .page_1 .left_area .top_box .table_box tr:first-child td {
  background-color: rgba(240, 240, 240, 1);
}

.ema526_02_su_0004 .page_1 .left_area .top_box .table_box tr:first-child td:last-child {
  border-top-right-radius: 14px;
}

.ema526_02_su_0004 .page_1 .left_area .top_box .table_box tr:first-child td:not(:last-child)::before {
  content: "";
  width: 2px;
  height: 23px;
  background-color: rgba(255, 255, 255, 1);
  position: absolute;
  top: 50%;
  right: -1px;
  transform: translateY(-50%);
}

.ema526_02_su_0004 .page_1 .left_area .top_box .table_box tr:last-child td:last-child {
  border-bottom-right-radius: 14px;
}

.ema526_02_su_0004 .page_1 .left_area .top_box .table_box tr:last-child td:not(:last-child) {
  border-right: 1px solid rgba(218, 218, 218, 1);
}

.ema526_02_su_0004 .page_1 .left_area .btm_box {
  margin-top: 60px;
  display: flex;
  justify-content: center;
  align-items: flex-end;
  gap: 20px;
}

.ema526_02_su_0004 .page_1 .left_area .btm_box .circle {
  width: 20px;
  height: 20px;
  margin: 0 auto;
  border: 1px solid rgba(0, 0, 0, 1);
  border-radius: 50%;
}

.ema526_02_su_0004 .page_1 .left_area .btm_box .arrow {
  margin-bottom: 92px;
}

.ema526_02_su_0004 .page_1 .left_area .btm_box .table_box { 
  width: 320px;
}

.ema526_02_su_0004 .page_1 .left_area .btm_box .table_box table::before {
  content: "";
  width: 100%;
  height: 165px;
  border: 1px solid rgba(218, 218, 218, 1);
  border-bottom: 0;
  border-top-left-radius: 14px;
  border-top-right-radius: 14px;
  position: absolute;
  left: 0;
  top: -1px;
  z-index: -1;
}

.ema526_02_su_0004 .page_1 .left_area .btm_box .table_box table::after {
  content: "";
  width: 100%;
  height: 33px;
  border: 1px solid rgba(253, 235, 227, 1);
  border-top: 0;
  border-bottom-left-radius: 14px;
  border-bottom-right-radius: 14px;
  position: absolute;
  left: -1px;
  bottom : 0;
}

.ema526_02_su_0004 .page_1 .left_area .btm_box .table_box tr th,
.ema526_02_su_0004 .page_1 .left_area .btm_box .table_box tr td {
  width: 64px;
  max-width: 64px;
  height: 33px;
  position: relative;
  font-weight: 700;
  font-size: 16px;
  line-height: 24px;
  letter-spacing: -0.5px;
}

.ema526_02_su_0004 .page_1 .left_area .btm_box .table_box tr th {
  background-color: rgba(253, 235, 227, 1);
}

.ema526_02_su_0004 .page_1 .left_area .btm_box .table_box tr th:first-child {
  border-bottom-left-radius: 14px;
}

.ema526_02_su_0004 .page_1 .left_area .btm_box .table_box tr th:last-child {
  border-bottom-right-radius: 14px;
}

.ema526_02_su_0004 .page_1 .left_area .btm_box .table_box tr + tr td {
  border-top: 1px solid rgba(218, 218, 218, 1);
}

.ema526_02_su_0004 .page_1 .left_area .btm_box .table_box tr td + td {
  border-left: 1px solid rgba(218, 218, 218, 1);
}

.ema526_02_su_0004 .page_1 .left_area .btm_box .table_box tr td.ui-droppable {
  pointer-events: none;
}

.ema526_02_su_0004 .page_1 .left_area .btm_box .table_box tr th:not(:last-child)::before {
  content: "";
  width: 2px;
  height: 23px;
  background-color: rgba(255, 255, 255, 1);
  position: absolute;
  top: 50%;
  right: -1px;
  transform: translateY(-50%);
}

.ema526_02_su_0004 .page_1 .left_area .btm_box .table_box tbody::before {
  content: "";
  width: calc(100% + 2px);
  height: 2px;
  background-color: rgba(255, 255, 255, 1);
  position: absolute;
  bottom: 33px;
  left: -1px;
  z-index: 10;
}

.ema526_02_su_0004 .page_1 .input_area {
  width: 709px;
