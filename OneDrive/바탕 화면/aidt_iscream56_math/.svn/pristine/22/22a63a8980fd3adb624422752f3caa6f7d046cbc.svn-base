.nav {
  height: 80px;
  background-color: #273b56;
  color: #fff;
  display: flex;
  align-items: center;
  justify-content: space-between;
  position: relative;
}
.nav .pagination {
  position: absolute;
  left: 61px;
  bottom: 0;
  /* transform: translateX(-50%); */
}
.nav .pagination button {
  width: 70px;
  height: 58px;
  line-height: 63px;
  font-size: 30px;
  background-color: #435c7e;
  color: #fff;
  border: none;
  border-radius: 10px 10px 0 0;
  margin: 0 6px;
  position: relative;
}
.nav .pagination button.on {
  background-color: #fff;
}

.nav .pagination button.o-icon::after {
  content: '';
  position: absolute;
  top: calc(50% + 2px);
  left: 50%;
  transform: translate(-50%, -50%);
  width: 46px;
  height: 47px;
  background: url('../img/mamuri/paging-o.svg') no-repeat center center;
  background-size: contain;
}
.nav .pagination button.x-icon::after {
  content: '';
  display: block; /* 이미지로 표시 */
  position: absolute;
  top: calc(50% + 2px);
  left: 50%;
  transform: translate(-50%, -50%);
  width: 35px;
  height: 34px;
  background: url('../img/mamuri/paging-x.svg') no-repeat center center;
  background-size: contain;
}

.nav .pagination button.on span {
  background: linear-gradient(to right, #347cf2, #0cb4b2);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  font-weight: 900;
}
.nav .center {
  position: absolute;
  right: 50%;
  transform: translateX(55px);
}
.nav .center .submit {
  width: 120px;
  height: 56px;
  border-radius: 12px;
  background-color: #3b71fe;
  display: flex;
  align-items: center;
  justify-content: center;
  gap: 10px;
  font-size: 20px;
  margin-right: 25px;
}
.nav .submit.disable {
  opacity: 0.5;
  filter: saturate(30%);
}
.nav .submit img {
  width: 25px;
}

