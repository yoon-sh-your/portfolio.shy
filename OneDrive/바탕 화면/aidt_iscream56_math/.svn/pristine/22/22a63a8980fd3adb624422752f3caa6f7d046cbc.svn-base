/* 기본 요소 초기화 */
*,
*::before,
*::after {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
  -webkit-tap-highlight-color: transparent;
  touch-action: manipulation;
  box-sizing: border-box;
}

/* HTML5 요소 초기화 */
article,
aside,
details,
figcaption,
figure,
footer,
header,
main,
nav,
section,
summary {
  display: block;
}

/* 문서 기본 스타일 */

html,
body {
  height: 100%;
  font-family: Pretendard, sans-serif;
  font-size: 26px;
  letter-spacing: -1px;
  line-height: 1.5;
  -webkit-text-size-adjust: 100%;
  background: #fff;
  color: #000;
  user-select: none;
  -webkit-user-select: none; /* Safari */
  -ms-user-select: none; /* IE/Edge */
  overflow: hidden;
}
body {
  position: relative;
}

/* 링크 초기화 */
a {
  color: inherit;
  text-decoration: none;
}

/* 버튼 초기화 */
button {
  background: none;
  border: none;
  padding: 0;
  font: inherit;
  color: inherit;
  cursor: pointer;
  touch-action: manipulation;
}

/* 리스트 초기화 */
ul,
ol {
  list-style: none;
}

/* 이미지 초기화 */
img {
  max-width: 100%;
  height: auto;
  display: inline-block;
  user-select: none;
  -webkit-user-drag: none;
}

/* 폼 요소 초기화 */
input,
textarea,
select {
  font: inherit;
  color: inherit;
  background: none;
  border: none;
  outline: none;
  resize: none;
  appearance: none;
  -webkit-appearance: none;
}

/* 테이블 초기화 */
table {
  border-collapse: collapse;
  border-spacing: 0;
}
.sr-only {
  position: absolute;
  width: 1px;
  height: 1px;
  padding: 0;
  margin: -1px;
  overflow: hidden;
  clip: rect(0, 0, 0, 0);
  border: 0;
  white-space: nowrap;
}

@font-face {
  font-family: 'Cafe24Ssurround';
  src: url('../fonts/Cafe24Ssurround-v2.0.woff2') format('woff2'), url('../fonts/Cafe24Ssurround-v2.0.woff') format('woff');
  font-weight: normal;
  font-style: normal;
  font-display: swap;
}
@font-face {
