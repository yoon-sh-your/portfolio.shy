/*
■■■■ 2번 유형 Item 선택 ■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■
■■■■ 3번 유형 선택지 선택 ■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■
*/
.item_wrap {
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  row-gap: 50px;
  column-gap: 55px;
  justify-content: center;
  border: 0;
}
.item_wrap.vertical,
.item_wrap.vertical3,
.item_wrap.vertical4,
.item_wrap.vertical5,
.item_wrap.vertical6 {
  flex-wrap: nowrap;
}
.item_wrap.i2-2 {
  width: 80%;
}
.item_wrap.half {
  row-gap: 30px;
}
.item {
  position: relative;
  display: inline-flex;
  align-items: center;
  justify-content: center;
  width: 418px;
  min-height: 143px;
  font-size: 44px;
  font-weight: 400;
  background-color: #fff;
  border: 10px solid #fff;
  cursor: pointer;
  transition: background-color 0.3s, border-color 0.3s;
  border-radius: 30px;
  box-sizing: border-box;
  padding: 8px 20px;
}
.item_wrap.sm {
  row-gap: 30px;
}
.item_wrap.half .item {
  min-height: 120px;
}
.item_wrap.vertical .item {
  width: 200px;
}
.item_wrap.vertical2 .item {
  width: calc(100% / 2);
}
.item_wrap.vertical3 .item {
  width: calc(100% / 3);
}
.item_wrap.vertical4 .item {
  width: calc(100% / 4);
}
.item_wrap.vertical5 .item {
  width: calc(100% / 5);
}
.item_wrap.vertical6 .item {
  width: calc(100% / 6);
}

.item_wrap.sm .item {
  padding: 0 40px;
}
.item_wrap.i2-2 .item {
  width: 42%;
}
.item.selected {
  border: 10px solid #3b71fe;
}
.item.is-answer,
.item.is-incorrect {
  border-color: #ff0000;
}
.item .txt {
  display: flex;
  align-items: center;
  flex: 1;
  text-align: left;
}
.item .txt:has(img) {
  justify-content: center;
}
.item_wrap.fs37 .item .txt {
  font-size: 37px;
}
.item input[type='checkbox']:disabled ~ .txt {
  opacity: 0.6;
  cursor: default;
}

.item .num,
.item .checked {
  width: 44px;
  height: 44px;
  border-radius: 12px;
  background-color: #fff;
  border: 1px solid #26bc72;
  color: #26bc72;
  font-weight: 700;
  font-size: 24px;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-right: 20px;
}
.item.selected .checked,
.item.selected .num {
  background: url(../img/mamuri/ic_check.svg) no-repeat center center;
  background-color: #3b71fe;
  background-size: 22.5px 17px;
  color: #fff;
  border-radius: 1px solid #3b71fe;
}
.item.is-answer .checked,
.item.is-answer .num {
  background: url(../img/mamuri/ic_check.svg) no-repeat center center;
  background-color: #ff0000;
  background-size: 22.5px 17px;
  color: #fff;
  border: 1px solid #ff0000;
}
/*
■■■■ 4번유형 D&D ■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■
*/

.matching-pairs {
  display: flex;
  flex-wrap: wrap;
  gap: 40px;
  justify-content: center;
  align-items: center;
}
.matching-pairs li {
  display: flex;
  justify-content: center;
  align-items: center;
  gap: 40px;
}
.matching-pairs li.v-txt {
  display: flex;
  align-items: flex-end;
  flex-direction: column;
  gap: 0;
}
.matching-pairs li.v-txt .txt {
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: flex-end;
  padding: 0 10px;
}
.matching-pairs li.v-txt i {
  display: inline-block;
  margin-right: auto;
  font-style: normal;
}
.matching-pairs li.v-txt .txt > span {
  width: 90px;
  display: inline-flex;
  align-items: center;
  justify-content: flex-end;
  font-size: 44px;
}
.withKeypad .matching-pairs li.v-txt .txt * {
  color: #fff;
}
.matching-pairs li.v-txt hr {
  border-top: 2px solid;
  margin: 6px 0 16px;
  width: 100%;
}
