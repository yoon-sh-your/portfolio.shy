  font-size: 44px;
  font-weight: 600;
  display: flex;
  align-items: center;
}
.matching-pairs li.v-txt .has-txt > span:not(.droppable) {
  width: 90px;
  display: inline-flex;
  align-items: center;
  justify-content: flex-end;
}
.matching-pairs li.v-txt .ans {
  width: 90px;
  margin-right: -20px;
}
.matching-pairs li.v-txt .ans.wide2 {
  width: 140px;
  margin-right: 0px;
}
.matching-pairs li.v-txt .ans.wide3 {
  width: 220px;
  margin-right: 0px;
}
.matching-pairs.txt li {
  gap: 10px;
}

.matching-pairs .txt {
  font-size: 44px;
  font-weight: 600;
  display: inline-block;
  text-align: center;
  white-space: nowrap;
}
.matching-pairs li .txt:first-child {
  text-align: right;
}
.matching-pairs li .txt:last-child {
  text-align: left;
}
.droppable {
  position: relative;
  display: inline-block;
  width: 180px;
  height: 90px;
  background-color: #fff;
  border: 4px solid #0075af;
  border-radius: 12px;
}
.v-txt .droppable {
  position: relative;
  right: -24px;
}
.v-txt .droppable,
.v-txt .droppable .draggable {
  width: 90px;
}
.drag_circle {
  width: 90px;
  border-radius: 50%;
}
.drag_circle.sign_gt {
  background: url(../img/mamuri/sign_gt.svg) no-repeat center center #fff;
}
.drag_circle.sign_eq {
  background: url(../img/mamuri/sign_eq.svg) no-repeat center center #fff;
}
.drag_circle.sign_lt {
  background: url(../img/mamuri/sign_lt.svg) no-repeat center center #fff;
}
.drag_circle.draggable {
  width: 90px;
  border: 0;
  box-shadow: 1px 4px 4px #414141;
  border-radius: 100%;
}

.draggable-items {
  position: absolute;
  bottom: 0;
  left: 50%;
  transform: translateX(-50%);
  width: 1298px;
  height: 310px;
  display: flex;
  align-items: center;
  justify-content: space-evenly;
  background: url(../img/mamuri/draggable_bg.svg) no-repeat 0 0 / 100%;
  padding: 26px 150px 0;
}
.draggable-items.half {
  height: 200px;
}

.draggable {
  width: 180px;
  height: 90px;
  background-color: #fff;
  border: 4px solid #0075af;
  border-radius: 12px;
  display: flex;
  align-items: center;
  justify-content: center;
  cursor: url(../../common/img/clickicon-sm.png) 35 15, auto;
  font-size: 44px;
}

.draggable-items .draggable.ui-draggable-dragging {
  transform: scale(1.15);
}
.draggable-items .draggable.disabled {
  opacity: 0;
}
.droppable .draggable {
  position: absolute;
  top: 0;
  left: 0;
}
.droppable.center .draggable {
  top: 50% !important;
  left: 50% !important;
  transform: translate(-50%, -50%);
}
.draggable:hover {
  opacity: 0.5;
}

.clicked-drag-scale {
  transform: scale(1.15);
}

.dragged {
  user-select: none;
  cursor: default;
  position: relative;
  top: -4px;
  left: -4px;
}
.draggable-items .dragged {
  top: 0;
  left: 0;
}
.draggable.dragged {
  background: #ddd;
}
.draggable.dragged:hover {
  opacity: 1;
}
.draggable.is-answer {
  border: 4px solid #f00;
}
