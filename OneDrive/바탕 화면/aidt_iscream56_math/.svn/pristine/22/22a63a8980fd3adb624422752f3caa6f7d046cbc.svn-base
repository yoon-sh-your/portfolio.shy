/* 크롬, 사파리, 엣지 (Webkit 계열 브라우저) */
input::-webkit-outer-spin-button,
input::-webkit-inner-spin-button {
  -webkit-appearance: none;
  margin: 0;
}
/* 파이어폭스 */
.calc_result_input {
  -moz-appearance: textfield;
}
.calc_option {
  display: flex;
  justify-content: space-between;
}
.calc_option button,
.btnReset {
  background: url(../img/mamuri/ic_calc_reset.svg) no-repeat center center;
  background-color: #3b71fe;
  border-radius: 16px;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-top: 21px;
}
.calc_option .calc_reset,
.btnReset {
  width: 64px;
  height: 64px;
  background-image: url(../img/mamuri/ic_calc_reset.svg);
}
.calc_option .calc_check {
  width: 157px;
  height: 64px;
  background-image: none;
  color: #fff;
}

.calc_num {
  display: grid;
  grid-template-columns: repeat(3, 1fr);
  gap: 20px;
  margin-top: 20px;
}
.withKeypad.txt .calc_num {
  opacity: 0.5;
  pointer-events: none;
}
.calc_num button {
  width: 70px;
  height: 70px;
  background-color: #fff;
  border-radius: 50%;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 36px;
  font-weight: 600;
  color: #234fc2;
  box-shadow: 1px 4px 4px #c0c0c0;
}
.calc_num button.del {
  background-image: url(../img/mamuri/ic_calc_del.svg);
  background-repeat: no-repeat;
  background-position: center;
  background-color: #fff;
}
.keypad button:active {
  transform: scale(0.9);
  box-shadow: 0px 2px 2px #c0c0c0;
  transition: transform 0.1s ease-out, box-shadow 0.1s ease-out;
}

