/* .ema514_01_su_0001 */
.ema514_01_su_0001#app_wrap {
  background: url(../img/EMA514_01_SU/0001_bg.png) no-repeat left 102% / 824px, #91c14a;
}

.intro_page .text_box {
  color: #008834;
  top: 200px;
  left: 388px;
}

.text_box img {
  position: absolute;
}

.input_area label.img {
  background-color: #008834;
}

.input_area label::after {
  background-color: #008834;
}

.ema514_01_su_0001 .fraction_row {
  display: flex;
  align-items: center;
}
.ema514_01_su_0001 .fraction_row .fraction_box::before {
  background-color: #008834;
}

/*ema514_01_SU_0002_s*/
.ema514_01_su_0002 .btn_area>button.btnReset.reset_off.active {
  pointer-events: none;
  background-color: var(--btn-disabled-bg);
  border-color: var(--stroke-default);
  background-image: url(../../common_contents/common/img/icon_reset_disabled.svg);
}
.ema514_01_su_0002 .btn_area>button.btnReset.reset_on {
  pointer-events: all;
  background-color: var(--btn-default);
  border-color: transparent;
  background-image: url(../../common_contents/common/img/icon_reset_white.svg);
}

/*ema514_01_SU_0003_s*/
