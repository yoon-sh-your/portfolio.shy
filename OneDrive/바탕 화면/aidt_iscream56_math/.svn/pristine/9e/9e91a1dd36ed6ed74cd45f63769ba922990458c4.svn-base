/* show_arc */
.diameter-point-area {
  fill: transparent;
  cursor: pointer;
}

.diameter-point {
  /* fill: #ccc; */
  fill: transparent;
  cursor: pointer;
  transition: all 0.2s;
}

.diameter-point:hover {
  /* fill: #ff6666; */
  /* r: 8; */

  stroke: #333;
  stroke-width: 3;
  stroke-dasharray: 6 6;
  fill: transparent;
}

.click-point {
  fill: #aaa;
  /* fill: darkslateblue; */
  pointer-events: none;
}


















