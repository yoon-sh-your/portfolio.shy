/*EMA522_08_SU_0002_s*/
/* 이해해요 */
h4.understand,
.understand {
    font-size: 24px;
    font-weight: bold;
    position: relative;
    width: 90px;
    flex: 0 0 auto;
}

h4.understand::before,
.understand::before {
    content: '';
    width: 57px;
    height: 40px;
    background: url(../img/EMA522_08_SU/understand_bg.png) no-repeat left top / 100%;
    display: block;
    position: absolute;
    opacity: 0.8;
    left: -14px;
    top: -2px;
    z-index: -1;
}

h4.understand .txt_bluelight,
.understand .txt_bluelight {
    color: #1454b9;
}

