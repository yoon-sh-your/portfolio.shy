  display: flex;
  align-items: center;
}


.ema621_02_su_0006 .boolean_count_wrap {
  position: relative;
  width: 400px;
  height: 400px;
  border: 2px solid rgba(88, 89, 91, 1);
  border-radius: 50%;
  flex-wrap: wrap;
}

.ema621_02_su_0006 .boolean_count_wrap button {
  position: absolute;
  top: 0;
  right: 0;
  width: 50%;
  height: 50%;
  transform-origin: 0% 100%;
  clip-path: polygon(0 0, 0 100%, 305% 0);
  border-radius: 0 100% 0 0;
}
.ema621_02_su_0006 .boolean_count_wrap button:focus {
  outline: 0;
}
.ema621_02_su_0006 .boolean_count_wrap button:not(:first-child)::after {
  display: none;
}
.ema621_02_su_0006 .boolean_count_wrap button:before {
  content: '';
  display: block;
  width: 1px;
  height: 100%;
  background-image: repeating-linear-gradient(to bottom, var(--font-main), var(--font-main) 4px, transparent 4px, transparent 8px);
  position: absolute;
  top: 0;
  left: -1px;
}

.ema621_02_su_0006 .boolean_count_wrap .btn1 { transform: rotate(0deg); }
.ema621_02_su_0006 .boolean_count_wrap .btn2 { transform: rotate(72deg); }
.ema621_02_su_0006 .boolean_count_wrap .btn3 { transform: rotate(144deg); }
.ema621_02_su_0006 .boolean_count_wrap .btn4 { transform: rotate(216deg); }
.ema621_02_su_0006 .boolean_count_wrap .btn5 { transform: rotate(288deg); }

.ema621_02_su_0006 .boolean_count_wrap button.selected {
  background-color: var(--font-main-500);
}
.ema621_02_su_0006 .boolean_count_wrap.hint button.correct {
  background-color: rgba(237, 28, 36, 0.3);
  border-color: var(--stroke-wrong);  
}
