.ema627_03_su_0003 .input_wrap {
  display: block;
  margin: 0 auto;
}
.ema627_03_su_0003 .table_wrap {
  border-radius: 20px;
  border:1px solid #D2D2D2;
  overflow: hidden;
}
.ema627_03_su_0003 .table_wrap table {
  border-style: hidden;
}
.ema627_03_su_0003 th,
.ema627_03_su_0003 td {
  padding:14px 16px;
  border:1px solid #D2D2D2;
}
.ema627_03_su_0003 th:nth-child(1),
.ema627_03_su_0003 td:nth-child(1) {
  width:312px;
  max-width:312px;
}
.ema627_03_su_0003 th:nth-child(2),
.ema627_03_su_0003 td:nth-child(2) {
  width:180px;
  max-width:180px;
}
.ema627_03_su_0003 th:nth-child(3),
.ema627_03_su_0003 td:nth-child(3) {
  width:716px;
  max-width:716px;
}
.ema627_03_su_0003 thead th  {
  background-color:#EEF0F4;
  border:1px solid #F4F4F4;
}
.ema627_03_su_0003 tbody td math-field.textarea {
  min-width: 100%;
  min-height: 60px;
  height: 60px;
  padding: 10px 20px;
  line-height: 1.4;
}


