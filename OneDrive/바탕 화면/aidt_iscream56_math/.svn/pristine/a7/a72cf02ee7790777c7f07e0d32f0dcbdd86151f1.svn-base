.boolean_wrap button.hint {
  border-width: 3px;
  /* border-color: transparent; */
}
.img_area {
  position: relative;
}
.img_area p {
  position: absolute;
}
.input_area > span,
.input_row > span {
  display: inline-block;
  vertical-align: middle;
}
math-field.has_ex_text + .text_hint:before {
  content: 'ì˜ˆ';
  display: inline-block;
  vertical-align: middle;
  margin-right: 10px;
  width: 44px;
  height: 44px;
  background: var(--primary-blue);
  border-radius: 12px;
  color: #fff;
  line-height: 42px;
}
.ico_round {
  display: inline-block;
  position: relative;
  top: 4px;
}
.nemo {
  display: inline-block;
  width: 32px;
  height: 32px;
  border: 2px solid var(--primary-blue);
  border-radius: 4px;
}
.solve_area .conts_solve .nemo.green {
  border-color: #22b24c;
  background-color: #22b24c;
}
/* ema521_05_ih_0001 */
span.label_important {
  display: inline-block;
  vertical-align: top;
  width: 71px;
  height: 48px;
  background: url(./../img/EMA521_05_IH/icon_important.png) no-repeat 50% / 100% auto;
  font-size: 16px;
  text-align: center;
  line-height: 48px;
  font-size: 24px;
  color: #fff;
}
.ema521_05_ih_0001 .boolean_wrap {
  padding: 0 216px;
  margin-bottom: 30px;
}
.ema521_05_ih_0001 .boolean_row {
  display: flex;
  align-items: center;
  width: 100%;
  justify-content: space-between;
}
.ema521_05_ih_0001 .boolean_row .text {
  font-size: 32px;
  width: 600px;
  height: 80px;
  background: #e5f3fc;
  border-radius: 999px;
  position: relative;
}
.ema521_05_ih_0001 .boolean_row .text:after {
  content: '';
  display: inline-block;
  width: 52px;
  height: 46px;
  background: url(./../common/img/icon_arrow_right_navy.svg) no-repeat 50% / 52px auto;
  position: absolute;
  top: 18px;
  left: calc(100% + 60px);
}
.ema521_05_ih_0001 .boolean_row .text p {
  display: block;
  text-align: center;
  height: 100%;
  line-height: 78px;
  letter-spacing: -1px;
}
.ema521_05_ih_0001 .boolean_box {
  width: 430px;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.ema521_05_ih_0001 .boolean_box > span {
  display: inline-block;
  font-size: 32px;
  vertical-align: middle;
  padding-top: 4px;
}
.ema521_05_ih_0001 .button_box button + button {
  margin-left: 30px;
  position: relative;
}
.ema521_05_ih_0001 .button_box button + button:before {
  content: ',';
  display: inline-block;
  font-size: 32px;
  position: absolute;
  left: -30px;
}
/* .result .ema521_05_ih_0001 button.selected {
	border-color: var(--font-main);
} */
.ema521_05_ih_0001 button[data-answer-single='true'].correct {
  border-color: var(--primary-blue);
  background-color: rgba(59, 113, 254, 0.3);
}

.label_bogi {
  color: #e76eb7;
  border-color: #e76eb7;
}

/* ema521_05_ih_0002 */
.ema521_05_ih_0002 .question_box {
  padding: 0 157px;
}
.ema521_05_ih_0002 .text_wrap {
  /* width: 100%; */
  padding: 76px 45px;
  display: flex;
  flex-direction: column;
  justify-content: center;
  border: 3px solid #e76eb7;
  border-radius: 20px;
  position: relative;
}
.ema521_05_ih_0002 .text_wrap dt {
  position: absolute;
}
.ema521_05_ih_0002 .text_wrap dd + dd {
  margin-top: 10px;
}
.ema521_05_ih_0002 .text_wrap .label_bogi {
  top: -16px;
  left: 40px;
}
.ema521_05_ih_0002 .input_area {
  width: 100%;
}
.ema521_05_ih_0002 .input_row {
  padding-left: 40px;
}
.ema521_05_ih_0002 .input_row + .input_row {
  margin-top: 50px;
}
.ema521_05_ih_0002 .input_wrap {
  margin-left: 12px;
}

/* ema521_05_ih_0003 */
.ema521_05_ih_0003 .boolean_wrap {
  width: 687px;
  height: 174px;
  background: url(./../../common_practice/img/EMA521_05_IH/0003_01.png) no-repeat 50% / 100% auto;
  margin: 0 auto;
}
.ema521_05_ih_0003 .button_box {
  display: flex;
  gap: 50px;
}
.ema521_05_ih_0003 .button_box button {
  background: none;
  width: 68px;
  height: 68px;
}
.ema521_05_ih_0003 .button_box button.selected {
  border-color: var(--font-main);
}
.ema521_05_ih_0003 .button_box button[data-answer-single='true'].correct {
  border-color: var(--primary-blue-800);
  background-color: rgba(59, 113, 254, 0.3);
}

