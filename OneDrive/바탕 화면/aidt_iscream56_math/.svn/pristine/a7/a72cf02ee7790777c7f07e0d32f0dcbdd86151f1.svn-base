}
.ema521_05_ih_0004 .img_area .name2 {
  right: 49px;
  transform: rotate(-2deg);
  bottom: -58px;
}
.ema521_05_ih_0004 .img_area .name3 {
  bottom: -54px;
  left: 83px;
  transform: rotate(3deg);
}
.ema521_05_ih_0004 .input_area {
  margin-top: 420px;
  margin-left: 80px;
}
.ema521_05_ih_0004 .input_area > span {
  display: inline-block;
  vertical-align: middle;
}

