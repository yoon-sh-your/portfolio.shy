.bg_trans,
.bg_coral,
.bg_sky,
.bg_green {
  display: inline-block;
  height: 60px;
  width: auto;
  line-height: 60px;
  padding: 0 16px;
  border-radius: 12px;
  margin: 8px;
}

.bg_coral {
  background-color: #ffcbc2;
}
.bg_sky {
  background-color: #eef9ff;
}
.bg_green {
  background-color: #e9efc6;
}

.bg_trans {
  background-color: transparent;
}

.small_border,
.small_bg_sky {
  display: inline-block;
  height: 32px;
  width: auto;
  line-height: 32px;
  padding: 0 10px;
  border-radius: 8px;
  font-size: 16px;
}
.small_bg_sky {
  background-color: #d9e1f4;
}
.small_border {
  border: 1px solid #ee4f24;
  background-color: var(--bg-default);
  color: #ee4f24;
}

h2 .small_border {
  position: relative;
  top: -3px;
  vertical-align: middle;
}

.inwrap-color {
  color: #3b71fe;
}
.inwrap-color2 {
  color: var(--secondary-red);
}
.empty_quadrangle {
  display: inline-block;
  width: 40px;
  height: 40px;
  border: 2px solid var(--primary-blue);
  border-radius: 5px;
  background-color: #fff;
  margin-right: 5px;
  margin-left: 5px;
}
.empty_circle {
  display: inline-block;
  width: 40px;
  height: 40px;
  border: 2px solid var(--primary-blue);
  border-radius: 5px;
  background-color: #fff;
  margin-right: 5px;
  margin-left: 5px;
  border-radius: 999px;
}

.hint_top .text_hint {
  top: calc(-100% + 20px);
  left: 0;
}

.fraction_box.three::before {
  top: 60%;
}

.hint_right .text_hint {
  top: 50%;
  transform: translateY(-50%);
  left: 88%;
}

.hint_right_120 .text_hint {
  left: auto;
  right: 0;
  width: 120px;
}
.hint_bottom_right .text_hint {
  top: 60px;
  left: 80px;
}

.hint_170 .text_hint {
  width: 170px;
}
.hint_120 .text_hint {
  width: 120px;
}
.hint_100 .text_hint {
  width: 100px;
}
.hint_80 .text_hint {
  width: 80px;
}

.input_wrap.height_60 .example_box {
  height: 60px;
}
.input_wrap.height_90 .example_box {
  height: 90px;
}

.input_wrap.small math-field:not(.textarea) {
  min-width: 100px;
  width: 100px;
}
.input_wrap.medium math-field:not(.textarea) {
  min-width: 120px;
  width: 120px;
}
.input_wrap.large math-field:not(.textarea) {
  min-width: 240px;
  width: 240px;
}

.input_wrap.xlarge math-field:not(.textarea) {
  min-width: 552px;
  width: 240px;
}

.small_border,
.small_bg_sky {
  display: inline-block;
  height: 32px;
  width: auto;
  line-height: 32px;
  padding: 0 10px;
  border-radius: 8px;
  font-size: 16px;
}
.small_border {
  border: 1px solid #ee4f24;
  background-color: var(--bg-default);
  color: #ee4f24;
}

.small_bg_sky {
  background-color: #d9e1f4;
}

h3.non-after {
  align-items: center;
}
h3.non-after::before {
  display: none;
}

.gray_Box {
  display: inline-block;
  text-align: center;
  width: 120px;
  padding: 8px 0;
  background-color: #9e9e9e;
  color: #fff;
  border-radius: 999px;
  margin-right: 18px;
}

.gray_Box_bubble {
  display: inline-block;
  text-align: center;
  padding: 8px 0;
  background-color: #9e9e9e;
  color: #fff;
  border-radius: 999px;
  margin-right: 18px;
  width: 100px;
  border-bottom-right-radius: 0;
}

/* 방법 1,2 */
.method {
  color: #ffffff;
  text-shadow: -2px 0px #5bb8a1, 0px 2px #5bb8a1, 2px 0px #5bb8a1,
    0px -2px #5bb8a1;
  position: relative;
  width: 115px;
  height: 40px;
  display: flex;
  justify-content: flex-end;
  align-items: flex-end;
  background-repeat: no-repeat;
  background-position: bottom right 10px;
  /* background-color: tomato; */
  padding-right: 50px;
}

.method[circle-number="1"] {
  background-image: url(../img/EMA515_04_SU/0005_cricle-1.png);
}

.method[circle-number="2"] {
  background-image: url(../img/EMA515_04_SU/0005_cricle-2.png);
}

.method::before {
  content: "";
  width: 97px;
  height: 23px;
  background-color: #5bb8a1;
  position: absolute;
  left: 0px;
  bottom: 0;
  z-index: -1;
  border-radius: 20px;
}

/* 
<p lang="y" class="draw_title f24b"><span>이해</span>해요</p>
<p lang="y" class="draw_title medium f24b"><span>이해해</span>해요</p>  
*/
.draw_title {
  background: url(../../common_contents/img/EMA524_08_SU/0002_titlebg.svg)
    no-repeat left top;
  background-size: auto 100%;
  height: 40px;
  padding: 0 16px;
}
.draw_title.medium {
  background-image: url(../../common_contents/img/EMA524_08_SU/0002_titlebg1.svg);
}
.draw_title span {
  line-height: 40px;
  color: var(--primary-blue-800);
}

/* 문제 만들기 박스 */
.quiz-builder {
  width: 1200px;
  margin: 30px auto 0;
}
.quiz-builder .quiz-title {
  background-color: #fff5ed;
  border-radius: 20px;
  line-height: 64px;
  padding: 0 24px;
  margin-left: 40px;
  display: inline-block;
}
.quiz-builder .quiz-color {
  color: #c0573d;
}
.quiz-builder .quiz-content {
  background-color: #fff5ed;
  border-radius: 40px;
  padding: 20px;
  margin-top: -20px;
}
.quiz-builder .quiz-content .quiz-content-inner {
  border-radius: 30px;
  border: 1px solid #fbcba1;
  background-color: var(--bg-default);
  padding: 40px 60px;
}
.quiz-builder .quiz-content-inner .input_wrap {
  width: 100%;
}
.quiz-builder .input_wrap math-field.textarea {
  min-height: 60px;
}
.quiz-builder .quiz-answer-inner .input_wrap > p:first-child {
  display: inline-block;
  line-height: 42px;
  padding: 0 11px;
  border-radius: 999px;
  border-bottom-right-radius: 0;
  background-color: var(--bg-default);
  margin-right: 10px;
}
.quiz-builder .quiz-answer-inner {
  text-align: center;
}
.quiz-builder .quiz-answer-inner .input_wrap {
  margin: 20px auto 0;
  align-items: center;
  justify-content: center;
  flex-direction: row;
  gap: 10px;
  width: auto;
}

/* 메모지 디자인 박스 */
.note-paper {
  display: flex;
  justify-content: center;
  gap: 35px;
  margin: 30px auto;
}
.note-paper .paper-con {
  position: relative;
  background: url(../../common_contents/img/EMA524_07_SU/0003_bg.svg) no-repeat
    center center / 100%;
  min-width: 377px;
  height: 220px;
  display: flex;
  flex-direction: column;
  padding: 27px 50px 36px;
}
.note-paper .paper-con > p {
  position: absolute;
  top: 8px;
  left: 10px;
  font-size: 16px;
}

/* 복잡한 분수식 */

.question_box._524 .wrapper .box:nth-child(1) {
  width: 43%;
}
.question_box._524 .wrapper .box:nth-child(2) {
  width: 58%;
  gap: 15px;
}

.question_box._524 .wrapper .vertical-math {
  align-items: flex-end;
  gap: 0;
}
.question_box._524 .wrapper .vertical-math span {
  line-height: 60px;
  margin: 0 8px;
  padding: 0 16px;
}
.question_box._524 .wrapper .vertical-math span.input_wrap {
  padding: 0;
  margin: 0;
}
.question_box._524 .wrapper .vertical-math::before {
  top: 64%;
}
.question_box._524 .vertical-math:has(.PM_0) {
  gap: 5px;
}
.question_box._524 .wrapper .vertical-math span.PM_0 {
  margin: 0;
  padding: 0;
  position: relative;
  top: -4px;
}
.question_box._524 .wrapper .vertical-math span.PM_0 span:first-child {
  position: relative;
  right: -15px;
}
.question_box._524 .wrapper .box:nth-child(2) > div {
  width: 128px;
}
.question_box._524 .wrapper .box:nth-child(1) > .arrow-row {
  gap: 55px;
  margin: 10px auto;
  padding-left: 30px;
}
.question_box._524 .wrapper .box:nth-child(1) > .arrow-row > div {
  gap: 5px;
}

.question_box._524 .wrapper .box:nth-child(2) > .arrow-col {
  gap: 50px;
}

.question_box._524 .wrapper .box:nth-child(2) > .arrow-col > div {
  width: 100%;
  text-align: center;
  background: url(../../common_contents/img/EMA524_01_SU/0004_arrow2.svg)
    no-repeat left center / 100%;
}
.question_box._524 .wrapper .box:nth-child(2) > .arrow-col > div > div {
  gap: 5px;
  background-color: var(--bg-default);
  width: 60px;
  margin: 0 auto;
}

/* 예시박스 오른쪽인경우 */

.example_left_wrap {
  flex-direction: row;
  gap: 12px;
}
.example_left_wrap .example_box {
  margin-top: 0;
  padding: 18px 24px 18px 82px;
}
.example_left_wrap .example_box::before {
  top: 14px;
}

/* hidden 버튼 */
.hidden_btn button:focus,
.hidden_btn button:focus-visible,
.hidden_btn *[role="button"]:focus,
.hidden_btn *[role="button"]:focus-visible {
  outline: 3px dashed transparent !important;
  outline-offset: -3px;
  border-color: transparent !important;
  /* border: 3px dashed black !important; */
  border-radius: 8px;
  box-sizing: border-box !important;
  background-clip: padding-box !important;
}
.hidden_btn button.hint,
.hidden_btn button:hover {
  border-color: transparent !important;
}

.hidden_btn button.selected {
  border-color: rgba(0, 0, 0, 0);
}

