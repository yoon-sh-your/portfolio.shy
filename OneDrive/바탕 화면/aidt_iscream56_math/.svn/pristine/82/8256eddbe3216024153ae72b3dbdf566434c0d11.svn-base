/* ema617_01_su_0002 */
.ema617_01_su_0002 .contents {
    width: 1208px;
    margin: 0 auto;
    padding: 104px 90px 20px 46px;
    background: url(../img/EMA617_01_SU/EMA617_01_SU_0002_bg.png) no-repeat;
    background-size: 1208px 502px;
}
.ema617_01_su_0002 .f24b u {
    text-decoration-thickness : 3px;
}
.ema617_01_su_0002 .tables {
    gap:80px;
}
.ema617_01_su_0002 .tables table {
    background-color: #fff;
}
.ema617_01_su_0002 .tables table caption {
    caption-side: bottom;
}
.ema617_01_su_0002 .tables table th,
.ema617_01_su_0002 .tables table td {
    width:135px;
    height: 32px;
    border:1px solid #D2D2D2;
}
.ema617_01_su_0002 .tables table th {
    width:150px;
    background-color: #D9F0FF;
}
.ema617_01_su_0002 .tables table thead td {
    background-color: #EEF9FF;
}



/* ema617_01_su_0003 */
.ema617_01_su_0003 .img_wrap {
    position: relative;
    width: max-content;
    margin: 0 auto;
}
.ema617_01_su_0003 .img_wrap .img_txt {
    position: absolute;
    left: 133px;
    top: 26px;
}

/* ema617_01_su_0004 */
.ema617_01_su_0004 .input_wrap {
    width:100%;
    display: block;
    margin: 0 auto;
}
.ema617_01_su_0004 .input_wrap math-field.textarea {
    width:100%;
    height: 194px;
}
