/* .ema512_06_su_0004 */
/* .ema512_06_su_0004 드래그앤드롭-도형멀티*/
/* 상단 예시 칸 */
.ema512_06_su_0004 .page_1 .draggable_wrap {
  margin: 0 auto;
}
.ema512_06_su_0004 .page_1 .draggable_wrap {
  display: flex;
  gap: 10px;
  width: 300px;
  height: 108px;
  padding: 10px 50px;
  justify-content: center;
  align-items: center;
  flex-shrink: 0;
  border-radius: 16px;
  border: 0.797px solid var(--Line-Gray-300, #d2d2d2);
  background: var(--BG-Gray-50, #f7f7f7);
  position: relative;
}
.ema512_06_su_0004 .page_1 .draggable_wrap .triangle{
  transform: translateY(5px);
}

/* 움직일 도형 */
.ema512_06_su_0004 .page_1 .triangle {
  background: url(../../common_contents/img/EMA512_04_SU/0004_drag_triangle_.svg) no-repeat center center / 100%;
  width: 70px;
  height: 70px;
}
.ema512_06_su_0004 .page_1 .circle {
  background: url(../../common_contents/img/EMA512_04_SU/0004_drag_circle_.svg) no-repeat center center / 100%;
  width: 70px;
  height: 70px;
}

/* 매칭 박스 wrap */
.ema512_06_su_0004 .page_1 .drag_group {
  display: flex;
  align-items: center;
  justify-content: center;
  gap: 20px;
  padding: 10px 50px;
  background-repeat: no-repeat;
  background-position: center bottom;
}
.ema512_06_su_0004 .page_1 .drag_group.g1 {
  background-image: url(../../common_contents/img/EMA512_06_SU/04_0004_contents.png);
  background-size: contain;
  height: 200px;
  margin-top: 20px;
  flex-direction: column;
}

/* 매칭 숫자들 */
.ema512_06_su_0004 .page_1 .droppable {
  font-size: 32px;
  font-weight: bold;
  text-align: center;
  line-height: 70px;
  background-image: none;
  width: 70px;
  height: 70px;
  position: relative;
}

/* 드래그 후 매칭숫자 하위에 생기는 div */
.ema512_06_su_0004 .page_1 .droppable .ui-draggable-handle {
  top: -100%;
}

/* 제출전 하위div */
.ema512_06_su_0004 .page_1 .droppable[data-value="true true"] .ui-draggable-handle {
  position: absolute !important;
  top: 0;
}

/* 제출후 상태 */
/* 힌트 */
/* 삼각형 단일 */
.ema512_06_su_0004 .page_1 .drag_group.hint .droppable[data-value="true,false"].triangle,
.ema512_06_su_0004 .page_1 .drag_group.hint .droppable[data-value="true,false"] .ui-draggable-handle.triangle {
  background: url(../../common_contents/img/EMA512_04_SU/0004_drag_triangle_hint.svg) no-repeat center center / 100%;
}
/* 원형단일 */
.ema512_06_su_0004 .page_1 .drag_group.hint .droppable[data-value="false,true"].circle,
.ema512_06_su_0004 .page_1 .drag_group.hint .droppable[data-value="false,true"] .ui-draggable-handle.circle {
  background: url(../../common_contents/img/EMA512_04_SU/0004_drag_circle_hint.svg) no-repeat center center / 100%;
}
/* 삼각형+원형 */
.ema512_06_su_0004 .page_1 .drag_group.hint .droppable[data-value="true,true"].circle.triangle,
.ema512_06_su_0004 .page_1 .drag_group.hint .droppable[data-value="true,true"] .ui-draggable-handle.circle,
.ema512_06_su_0004 .page_1 .drag_group.hint .droppable[data-value="true,true"] .ui-draggable-handle.triangle {
  background: url(../../common_contents/img/EMA512_04_SU/0004_drag_together_hint.svg) no-repeat center center / 100%;
}

/* 하위 div */
.ema512_06_su_0004 .page_1 .droppable[data-value="true,true"] .ui-draggable-handle,
.ema512_06_su_0004 .page_1 .droppable[data-value="false,true"] .ui-draggable-handle,
.ema512_06_su_0004 .page_1 .droppable[data-value="true,false"] .ui-draggable-handle,
.ema512_06_su_0004 .page_1 .droppable[data-value="false,false"] .ui-draggable-handle {
  position: absolute !important;
  top: 0;
}
/* 하위div 힌트*/
.ema512_06_su_0004 .page_1 .drag_group.hint .droppable[data-value="true,true"] .ui-draggable-handle.circle,
.ema512_06_su_0004 .page_1 .drag_group.hint .droppable[data-value="true,true"] .ui-draggable-handle.triangle,
.ema512_06_su_0004 .page_1 .drag_group.hint .droppable[data-value="false,true"] .ui-draggable-handle.circle.drag_group.hint .droppable[data-value="true,false"] .ui-draggable-handle.triangle {
  position: absolute !important;
  top: 0;
}

/* 정답 */
