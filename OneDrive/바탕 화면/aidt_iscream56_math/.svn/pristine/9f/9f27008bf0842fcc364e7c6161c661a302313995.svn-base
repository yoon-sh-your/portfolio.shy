      <section class="contents">
        <div class="question_box justify_center">
          <div class="flex">
            <div class="col">
              <div class="input_wrap answer1">
                <span>3.6:4.8=</span>
                <div class="field_wrap">
                  <math-field data-answer-single="empty_answer"></math-field>
                  <div class="example_box">
                    <p>3:4</p>
                  </div>
                </div>
              </div>
            </div>
            <div class="col">
              <span class="fraction_box f32b">
                <span>2</span>
                <span>5</span>
              </span>
              <span class="f32b">:</span>
              <span class="fraction_box f32b">
                <span>4</span>
                <span>9</span>
              </span>
              <span class="f32b">=</span>
              <div class="input_wrap">
                <math-field data-answer-single="empty_answer"></math-field>
                <div class="example_box">
                  <p>9:10</p>
                </div>
              </div>
            </div>
          </div>
          <div class="flex">
            <div class="col">
              <span class="f32b">0.3</span>
              <span class="f32b">:</span>
              <span class="fraction_box f32b">
                <span>9</span>
                <span>20</span>
              </span>
              <span class="f32b">=</span>
              <div class="input_wrap">
                <math-field data-answer-single="empty_answer"></math-field>
                <div class="example_box">
                  <p>2:3</p>
                </div>
              </div>
            </div>
            <div class="col">
              <span class="mixed_fraction f32b">
                <span>1</span>
                <span class="fraction_box">
                  <span>1</span>
                  <span>6</span>
                </span>
              </span>
              <span class="f32b">:</span>
              <span class="fraction_box f32b">
                <span>1</span>
                <span>8</span>
              </span>
              <span class="f32b">=</span>
              <div class="input_wrap">
                <math-field data-answer-single="empty_answer"></math-field>
                <div class="example_box">
                  <p>28:3</p>
                </div>
              </div>
            </div>
          </div>
        </div>

        <div class="btn_area">
          <button class="btnType key">입력타입토글</button>
          <button class="btnReset">리셋</button>
          <button class="btnSubmit" data-submit="true">제출</button>
        </div>
