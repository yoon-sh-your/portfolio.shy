/* ema524_09_su_0002 */

.image-preview {
  width: 900px;
  position: relative;
  margin: 0px auto 0;
  background: url(../../common_contents/img/EMA524_08_SU/0002_contents.png)
    no-repeat center center;
  height: 484px;
}
.image-preview .box_wrap {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}
.image-preview .box {
  position: absolute;
}
.image-preview .box span,
.image-preview .box p {
  font-size: 14px;
  font-weight: 700;
}
.image-preview .box:nth-child(1) {
  top: 40%;
  left: 33%;
  width: 390px;
}
.image-preview .box:nth-child(1) p {
  flex: 1;
  text-align: center;
}
.image-preview .box:nth-child(2) {
  top: 57%;
  right: -8%;
  width: 390px;
}
.image-preview .box:nth-child(2) p {
  line-height: 25px;
  padding-left: 15px;
}
.image-preview .box:nth-child(2) p:first-child {
  color: var(--bg-default);
  margin-bottom: 9px;
  padding-left: 0px;
}
.image-preview .box:nth-child(3) {
  bottom: 20%;
  left: 24.5%;
  line-height: 19px;
}
.image-preview .box:nth-child(4) {
  bottom: 36%;
  left: 35%;
}
.image-preview .box:nth-child(4) span {
  font-size: 10px;
  font-weight: 400;
}

/* ema524_09_SU_0002 page_2 */

.ema524_09_su_0002 .image-preview {
  border-color: transparent;
}
.ema524_09_su_0002 .input_area > div:last-child {
  margin-left: 110px;
}
.ema524_09_su_0002 .input_wrap.answer5 math-field:not(.textarea),
.ema524_09_su_0002 .page_2 .input_wrap math-field:not(.textarea) {
  width: 430px;
  height: 60px;
}
.ema524_09_su_0002 .page_2 .input_wrap .text_hint {
  width: 430px;
}
.image-preview._2 {
  background: url(../../common_contents/img/EMA524_08_SU/0002_contents.png)
    no-repeat 100% center / 125%;
}
.image-preview._2 .box_wrap {
  top: -0%;
  left: -18%;
}
.image-preview._2 .box:nth-child(1) {
  left: 33.5%;
  width: 405px;
}

.image-preview._2 .box:nth-child(2) {
  right: -28.5%;
}
.image-preview._2 .box:nth-child(3) {
  left: 24%;
}

/* ema524_09_SU_0002 page_3 */

.ema524_09_su_0002 .page_3 .input_area > div:last-child {
  gap: 40px;
}
/* ema524_09_SU_0002 page_4 */

.ema524_09_su_0002 .page_4 .input_area {
  position: relative;
}

.ema524_09_su_0002 .page_4 .input_wrap math-field.textarea {
  min-height: 60px;
}

/* ema524_09_SU_0002 page_5 */

.ema524_09_su_0002 .page_5 .input_area > div:last-child {
  position: relative;
}
.ema524_09_su_0002 .page_5 .title_label {
  min-width: 100px;
}
.ema524_09_su_0002 .page_5 .color_table {
  position: absolute;
  left: -100px;
}
.ema524_09_su_0002 .page_5 .color_table .color_table_title {
  background: url(../../common_contents/img/EMA524_08_SU/0002_titlebg2.svg)
    no-repeat center center;
  width: 146px;
  text-align: center;
  line-height: 32px;
  color: #008a70;
  font-weight: 700;
}
.ema524_09_su_0002 .page_5 .color_table > div > div {
  min-width: 40px;
  max-width: 140px;
}
.ema524_09_su_0002 .page_5 .color_table > div > div:first-child {
  min-width: 140px;
}
.ema524_09_su_0002 .page_5 .color_table > div > div > span {
  flex: 1;
  min-height: 68px;
  max-height: 68px;
  display: flex;
  justify-content: center;
  align-items: center;
  text-align: center;
  font-size: 16px;
  font-weight: 700;
  padding: 13px 29px;
}
.ema524_09_su_0002 .page_5 .color_table > div > div > div {
  flex: 1;
  min-height: 68px;
  max-height: 68px;
  display: flex;
  justify-content: center;
  align-items: center;
  text-align: center;
  font-size: 16px;
  font-weight: 700;
  padding: 13px 19px;
}
.ema524_09_su_0002 .page_5 .color_table > div > div > span:first-child {
  background-color: #efeeee;
}
.color_table > div > div > div:last-child {
  border-right: 1px solid #dadada;
  border-bottom: 1px solid #dadada;
}
.color_table > div > div:last-child div:last-child {
  border-bottom-right-radius: 20px;
}
.color_table > div > div:first-child > span {
  background-color: #ddeee9;
}
.color_table > div > div:last-child > span:first-child {
  border-top-right-radius: 20px;
}
.color_table > div > div:first-child > span:first-child {
  border-top-left-radius: 20px;
}
.color_table > div > div:first-child > span:last-child {
  border-bottom-left-radius: 20px;
}

#app_wrap.ema524_09_su_0002[data-current-page="page_1"] .btn_area,
#app_wrap.ema524_09_su_0002[data-current-page="page_2"] .btnSubmit,
#app_wrap.ema524_09_su_0002[data-current-page="page_3"] .btnSubmit,
#app_wrap.ema524_09_su_0002[data-current-page="page_4"] .btnCheck,
#app_wrap.ema524_09_su_0002[data-current-page="page_5"] .btnSubmit,
#app_wrap.ema524_09_su_0002[data-current-page="page_6"] .btn_area {
  display: none;
}
/* ema524_09_SU_0002 page_6 */

.ema524_09_su_0002 .page_6 > div > div > p {
  margin-top: 5px;
}

.ema524_09_su_0002 .page_6 .solve_box_wrap {
  width: 95%;
  margin: 30px auto 0;
}
.ema524_09_su_0002 .page_6 .solve_box {
  flex: 1;
  height: 306px;
  border: 3px dashed #c6c6c6;
  border-top-left-radius: 40px;
  border-top-right-radius: 40px;
  border-bottom: 0;
  display: flex;
  align-items: center;
  flex-direction: column;
}

.ema524_09_su_0002 .page_6 .solve_box > p {
  background: url(../../common_contents/img/EMA524_08_SU/0002_bg.png) no-repeat
    52% 100% / 250px;

  width: 100%;
  height: 90px;
  display: flex;
  align-items: center;
  justify-content: center;
}

.ema524_09_su_0002 .page_6 .solve_box:last-child > p {
  background: url(../../common_contents/img/EMA524_08_SU/0002_bg1.png) no-repeat
    52% 100% / 300px;
}
.ema524_09_su_0002 .page_6 .solve_box:first-child > ol {
  margin-top: 40px;
  font-size: 32px;
}
.ema524_09_su_0002 .page_6 .solve_box:first-child > ol li {
  padding-left: 20px;
  margin-bottom: 20px;
}
.ema524_09_su_0002 .page_6 .solve_box:first-child > ol li::before {
  content: "";
  display: inline-block;
  width: 8px;
  height: 8px;
  position: relative;
  left: -10px;
  top: -8px;
  border-radius: 50%;
  background-color: var(--font-main);
  flex-shrink: 0;
  flex-grow: 0;
}
.ema524_09_su_0002 .page_6 .solve_box:last-child > div:nth-child(2) {
  display: flex;
  justify-content: center;
  gap: 40px;
  margin-top: 40px;
  margin-bottom: 40px;
}
.ema524_09_su_0002 .page_6 .solve_box:last-child > div:nth-child(3) {
  font-size: 32px;
}

