.ema615_02_su_0003 .page_2 .input_area,
.ema615_02_su_0003 .page_3 .input_area {
    width: 709px;
    /* height: calc(502px - 96px - 20px); */
}

.ema615_02_su_0003 .page_1 .input_area {
    height: var(--ip_area);
    /* background-color: tomato; */
}

.ema615_02_su_0003 .page_1 .input_wrap {
    width: 100%;
    height: calc(var(--ip_area) - var(--h3_1) - var(--common_h3_b_space));
    /* height: calc(var(--ip_area) - var(--h3_1) - var(--figma_h3_b_space)); */
    /* background-color: orange; */
}

.ema615_02_su_0003 .page_1 .input_wrap math-field.textarea {
    /* min-height: unset; */
    /* height: 316px; */
    height: 77%;
}

.icon_GA {
    display: inline-block;
    width: 46px;
    height: 44px;
    background-image: url(../img/EMA615_02_SU/icon_GA.svg);
    background-position: center center;
    background-repeat: no-repeat;

}

.icon_NA {
    display: inline-block;
    width: 46px;
    height: 44px;
    background-image: url(../img/EMA615_02_SU/icon_NA.svg);
    background-position: center center;
    background-repeat: no-repeat;
}

.ema615_02_su_0003 .page_2 .icon_GA,
.ema615_02_su_0003 .page_3 .icon_NA {
    position: absolute;
    top: 2px;
}







