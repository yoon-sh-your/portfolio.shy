/* paging layout > input_area */
.paging_layout {
    --ip_area: 502px;
    --h3_1: 48px;
    --h3_2: 96px;
    --figma_h3_b_space: 12px;
    --common_h3_b_space: 30px;
}

/* ema615_02_su_0003 */
/* 페이지 수: ~ 4 페이지 */
/* picture_wrap 가리는 페이지: 4 */
/* 서술형 페이지: 1 */
.ema615_02_su_0003 .picture_wrap {
    width: 700px;
    height: 490px;
    border: none;
}

.ema615_02_su_0003 .picture_wrap img,
.ema615_02_su_0003 .picture_wrap svg {
    /* transform: scale(1) translate(-70%, -70%); */
    height: 100%;
}

.ema615_02_su_0003 .picture_wrap img {
    object-fit: contain;
}

/* picture_wrap 가리는 페이지 */
.ema615_02_su_0003[data-current-page='page_4'] .contents>div.flex,
.ema615_02_su_0003[data-current-page='page_4'] .contents>div.flex>.picture_wrap {
    display: none;
}

/* picture_wrap 안 가리는 페이지 */
.ema615_02_su_0003[data-current-page='page_1'] .flex.align_start,
.ema615_02_su_0003[data-current-page='page_2'] .flex.align_start,
.ema615_02_su_0003[data-current-page='page_3'] .flex.align_start {
    /* padding: 0 20px; */
    gap: 26px;
}

/* 서술형 페이지 버튼 */
.ema615_02_su_0003:not([data-current-page='page_1']) .btn_area .btnSubmit,
.ema615_02_su_0003[data-current-page='page_1'] .btn_area .btnCheck {
    display: none;
}

/* 약속해 볼까요 가리기 */
.ema615_02_su_0003[data-current-page='page_4'] section.title,
.ema615_02_su_0003[data-current-page='page_4'] .contents>div.flex,
.ema615_02_su_0003[data-current-page='page_4'] .contents .picture_wrap,
.ema615_02_su_0003[data-current-page='page_4'] .btn_area .btnType {
    display: none;
}

section.title.type2+section.contents {
    /* background-color: aqua; */
}

section.contents.paging_layout {
    /* background-color: aquamarine; */
}

.page {
    /* background-color: orange; */
}

