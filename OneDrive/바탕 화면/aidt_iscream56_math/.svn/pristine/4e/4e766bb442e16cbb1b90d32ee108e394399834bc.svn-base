.nemo {
	display: inline-block;
	width: 32px;
	height: 32px;
	border: 2px solid var(--primary-blue);
	border-radius: 4px;
}

.input_area > span,
.input_area .text_row > span {
	display: inline-block;
	vertical-align: middle;
}
.input_area .text_row + .text_row {
	margin-top: 50px;
}
.text_square_bg {
	display: inline-block;
	vertical-align: middle;
	width: 47px;
	height: 46px;
	background: #d8e9f8;
	text-align: center;
	line-height: 45px;
	font-size: 18px;
	position: absolute;
	top: 0;
	left: 0;
}
.input_area .text_square_bg {
	position: static;
}
.img_area {
	position: relative;
}
.solve_area .text_square_bg {
	position: static;
	vertical-align: middle;
	border: 1px solid var(--font-main);
	position: relative;
	top: -4px;
}
.solve_area .nemo {
	position: relative;
	top: 4px;
	background: #fff;
}
.ico_round {
	display: inline-block;
	position: relative;
	top: 4px;
}
.input_wrap.hint_right .text_hint {
	left: calc(100% + 20px);
	top: 12px;
	width: auto;
}

/* ema521_08_ih_0001 */
.paging .boolean_area {
	width: 1157px;
	height: 174px;
	background: url(./../img/EMA521_08_IH/0001_01.png) no-repeat 50% / 100% auto;
	font-size: 32px;
	font-weight: 700;
	display: flex;
	align-items: center;
	justify-content: center;
	margin: 0 auto;
}
.paging .boolean_wrap {
	display: inline-flex;
}
.paging .boolean_box {
	display: flex;
	gap: 12px;
}
.paging .boolean_wrap button {
	width: auto;
	height: auto;
	background: none;
	border-radius: 0;
	aspect-ratio: unset;
	padding: 0;
	border-color: transparent;
	line-height: 1;
	position: relative;
}
.paging .boolean_wrap button:after {
	content: "";
	display: inline-block;
	width: 68px;
	height: 68px;
	border-radius: 50%;
	border: 4px solid transparent;
	position: absolute;
}
.paging .boolean_wrap button.selected:after {
	border-color: var(--font-main);
}

.paging h3 .ico_round {
	top: 0;
	margin: 0 4px 0 8px;
}
.result .paging .boolean_wrap button[data-answer-single="true"]:after {
	border-color: var(--primary-blue-800);
	background: rgba(59, 113, 254, 0.3);
}

