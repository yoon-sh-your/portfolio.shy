/* .ema614_10_su_0002 */
.ema614_10_su_0002 .table_box {
    background: url(../img/EMA614_10_SU/0002_bg.png) no-repeat 0 0;
    width: 1200px;
    height: 194px;   
    position: relative;
}
.ema614_10_su_0002 .table_box th,
.ema614_10_su_0002 .table_box td {   
    position: relative;
    height: 64.5px;
}

.ema614_10_su_0002 .table_box th,
.ema614_10_su_0002 .table_box td {
    font-size: 24px;
    letter-spacing: -0.5px;
    font-weight: 700;
    text-align: center;
}
.ema614_10_su_0002 .table_box .input_wrap {
    min-width: 80px;
}
.ema614_10_su_0002 .table_box .input_wrap math-field:not(.textarea) {
    width: 80px;
}
.ema614_10_su_0002 .col2 {
    margin-top: 20px;
}

.ema614_10_su_0002 .page_2 .question_box,
.ema614_10_su_0002 .page_3 .question_box {
    gap: 20px;
    flex-direction: column;
    padding-top: 45px;
}
.ema614_10_su_0002 .page_2 h3,
.ema614_10_su_0002 .page_3 h3 {
    gap: 8px;
}
.ema614_10_su_0002 .page_2 h3:before,
.ema614_10_su_0002 .page_3 h3:before {
    top: 50px;
}

.ema614_10_su_0002 .page_2 .input_wrap,
.ema614_10_su_0002 .page_3 .input_wrap {
    min-width:247px;
}
.ema614_10_su_0002 .page_2 .input_wrap math-field,
.ema614_10_su_0002 .page_3 .input_wrap math-field {
    min-width: 247px;
}


/* .ema614_10_su_0003 */
.ema614_10_su_0003 .empty_box {
    display: inline-flex;
    width: 32px;
    height: 32px;
    padding: 0;
    border-radius: 4px;
}
.ema614_10_su_0003 .page_1 .flex,
.ema614_10_su_0003 .page_2 .flex {
    gap: 26px;
}
.ema614_10_su_0003 .page_1 .question_box,
.ema614_10_su_0003 .page_2 .question_box {
    gap: 20px;
    align-items: flex-start;
    width: 1200px;
}
.ema614_10_su_0003 .page_1 h3:before,
.ema614_10_su_0003 .page_2 h3:before {
    top: 24px;
}

.ema614_10_su_0003 .page_1 .picture_wrap,
.ema614_10_su_0003 .page_2 .picture_wrap {
    width: 414px;
    min-width: 414px;
    height: 183px;
    max-height: 183px;
    aspect-ratio: unset;
    flex-grow: 0;
    border: 0;
    border-radius: 0;
}
.ema614_10_su_0003 .page_1 .picture_wrap + h3,
.ema614_10_su_0003 .page_2 .picture_wrap + h3 {
    flex:1;
}
.ema614_10_su_0003 .page_1 .arr,
.ema614_10_su_0003 .page_2 .arr {
    position: relative;
    margin-right: 74px;
}
.ema614_10_su_0003 .page_1 .arr:before,
.ema614_10_su_0003 .page_2 .arr:before {
    content: '';
    display: inline-block;
    position: absolute;
    top: 50%;
    left: calc(100% + 14px);
    transform: translateY(-50%);
    width: 50px;
    height: 50px;
    background: url(../img/EMA614_10_SU/0003_bg.png) no-repeat 0 0;
}


/* .ema614_10_su_0004 */
.ema614_10_su_0004 .question_box {
    align-items: flex-start;
    margin-top: -10px;
}
.ema614_10_su_0004 .connect_wrap {
    width: 186px;
    height: 442px;
}
.ema614_10_su_0004 .start_group, 
.ema614_10_su_0004 .end_group {
    gap: 134px;
}

.connect_line_box li {
    display: flex;
    align-items: center;
    justify-content: center;
    height: 100px;
}
.connect_line_box.start li {
    border: 2px dashed #8CD6FF
}
.connect_line_box.mid1 li {
    border: 2px dashed #9ED4B0;
    text-align: center;
}
.connect_line_box.mid2 li {
    border: 2px dashed #C5BCDD;
    text-align: center;
}
.connect_line_box.mid3 li {
    border: 2px dashed #FBBD98;
    text-align: center;
}
.ema614_10_su_0004 .connect_line_box.start li::after {
    background: #EEF9FF;
}
.ema614_10_su_0004 .connect_line_box.mid1 li::after {
    background: #D4EBDB;
}
.ema614_10_su_0004 .connect_line_box.mid2 li::after {
    background: #E4E0F0;
}
.ema614_10_su_0004 .connect_line_box.end li::after {
    background: #FEE1CF;
}

