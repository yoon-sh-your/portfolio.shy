.question_box {
  white-space: normal;
}
.title.group {
  padding-bottom: 0;
}
.tit_data {
  display: flex;
  justify-content: center;
  margin-bottom: 6px;
}
.tit_data .inner {
  position: relative;
  padding: 0 6px;
}
.tit_data .inner:before {
  content: '';
  position: absolute;
  top: 50%;
  left: 0;
  width: 100%;
  height: 22px;
  transform: translateY(-50%);
  border-radius: 22px;
  background: #fdddce;
}
.tit_data span {
  position: relative;
  display: flex;
  align-items: center;
  justify-content: center;
  height: 32px;
  padding: 0 16px;
  border: 2px solid #fdddce;
  border-radius: 32px;
  font-size: 24px;
  font-weight: bold;
  color: #911c1a;
  background-color: #fff;
}
.table_wrap th,
.table_wrap td {
  font-weight: normal;
  height: 46px;
  font-size: 24px;
}
.table_wrap thead th {
  background-color: #fde6db;
}
.table_wrap tbody th {
  background-color: #eaebec;
}
.table_wrap.row th,
.table_wrap.row td {
  height: 52px;
}
.table_wrap.row thead td {
  background-color: #eaebec;
}
.table_wrap.row thead tr:first-child td:last-child {
  border-radius: 0 19px 0 0;
}
.table_wrap.row tbody th {
  background-color: #fde6db;
}
.table_wrap.row thead td {
  border-left: 2px solid #fff;
}

.ema526_02_ih_0001 .img {
  position: relative;
}
.ema526_02_ih_0001 .img span {
  position: absolute;
  top: 74px;
  left: 36px;
  width: 102px;
  text-align: center;
}
.ema526_02_ih_0001 .bottom {
  width: 1022px;
  height: 184px;
  margin: 40px auto 0;
  background: url(../img/EMA526_02_IH/0001_bg.svg) no-repeat center center / 100% 100%;
}
.ema526_02_ih_0001 .boolean_wrap {
  display: inline-flex;
  gap: 4px;
}
.ema526_02_ih_0001 .boolean_wrap button {
  position: relative;
  width: auto;
  height: auto;
  aspect-ratio: unset;
  padding: 0;
  border: 0;
  border-radius: 0;
  background: none !important;
}
.ema526_02_ih_0001 .boolean_wrap button:after {
  position: absolute;
  top: 50%;
  left: 50%;
  width: 68px;
  height: 68px;
  transform: translate(-50%, -50%);
  border-radius: 100%;
  border: 4px solid #000;
}
.ema526_02_ih_0001 .boolean_wrap button.selected::after {
  content: '';
}
.ema526_02_ih_0001 .boolean_wrap.result button[data-answer-single='true']:after {
  content: '';
  border-color: var(--font-negative);
  background: rgba(255, 67, 67, 0.3);
}
.ema526_02_ih_0001 .boolean_wrap.result button.selected[data-answer-single='true']:after {
  border-color: var(--primary-blue);
  background: rgba(59, 113, 254, 0.3);
}

.ema526_02_ih_0002 .table_wrap tbody th {
  width: 86px;
}
.ema526_02_ih_0002 .table_wrap td {
  text-align: left;
  padding: 0 10px;
}
.ema526_02_ih_0002 .table_wrap td img {
  display: inline-block;
  vertical-align: middle;
}
.ema526_02_ih_0002 .table_wrap + .flex {
  margin-top: 10px;
  font-size: 22px;
  font-weight: normal;
}

.ema526_02_ih_0003 .table_wrap {
  width: 950px;
  margin-top: 14px;
}
.ema526_02_ih_0003 .page_1 .question_box {
  margin-top: 26px;
  gap: 8px;
}
.ema526_02_ih_0003 .page_2 .question_box {
  display: block;
  width: 966px;
  margin-top: 12px;
  border: 20px solid #c2e1ee;
  border-radius: 90px;
}
.ema526_02_ih_0003 .page_2 .question_box .inner {
  display: flex;
  flex-direction: column;
  gap: 38px;
  padding: 20px 40px;
  margin: -12px;
  border: 4px dashed #fff;
  border-radius: 85px;
}
.ema526_02_ih_0003 .page_2 .question_box .row {
  display: flex;
  align-items: center;
}
.ema526_02_ih_0003 .page_2 .question_box .row .input_wrap {
  margin: 0 4px 0 8px;
}
.ema526_02_ih_0003 .page > .dir_col {
  padding-right: 40px;
}
.ema526_02_ih_0003 section.title.type2 + section.contents {
  padding-top: 20px;
}

.ema526_02_ih_0004 .table_wrap {
  width: 950px;
}
.ema526_02_ih_0004 .img {
  position: relative;
}
.ema526_02_ih_0004 .img span {
  position: absolute;
  font-size: 24px;
  font-weight: normal;
}
.ema526_02_ih_0004 .img .txt {
  top: 45px;
  left: 30px;
  line-height: 1.2;
  word-break: keep-all;
}
.ema526_02_ih_0004 .img .name {
  top: 142px;
  left: 113px;
  color: #fff;
}
.ema526_02_ih_0004 .question_box {
  align-items: flex-start;
  gap: 12px;
  position: relative;
}
.ema526_02_ih_0004 .question_box .iw1 math-field {
  height: 100px;
}
.ema526_02_ih_0004 .question_box .example_box {
  width: calc(100% - 200px);
}
.ema526_02_ih_0004 .question_box .answer1 {
  position: absolute;
  bottom: 0;
  right: 0;
  transform: translateY(-50%);
}
.ema526_02_ih_0004 .label_gray {
  flex-shrink: 0;
}
.ema526_02_ih_0004 .label_gray + .input_wrap,
.ema526_02_ih_0004 .label_gray + .input_wrap math-field {
  max-width: 100%;
  width: 939px;
}
