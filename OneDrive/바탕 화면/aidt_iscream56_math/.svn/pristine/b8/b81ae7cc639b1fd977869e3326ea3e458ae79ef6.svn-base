.title.group .pink {
  color: #e05167;
}

.img_box {
  display: flex;
  justify-content: center;
  align-items: center;
  margin-bottom: 60px;
}

.ema513_05_ih_0001 .question_box {
  width: 660px;
  margin: 0 auto;
  gap: 50px;
  align-items: flex-start;
}
.ema513_05_ih_0001 .question_box .answer1 math-field {
  width: 210px;
}
.ema513_05_ih_0001 .question_box .answer2 math-field {
  width: 186px;
}
.ema513_05_ih_0001 .question_box > div {
  width: 100%;
}
.ema513_05_ih_0001 .question_box > div + div .input_wrap_underline {
  flex-grow: 1;
}
.ema513_05_ih_0001 .question_box .answer3,
.ema513_05_ih_0001 .question_box .answer3 math-field {
  width: 100%;
}
.ema513_05_ih_0002 td {
  width: 430px;
  padding: 12px;
}
.ema513_05_ih_0002 td:first-child .input_wrap math-field {
  width: 170px;
}
.ema513_05_ih_0002 td:not(:nth-child(3)) .text_hint {
  width: auto;
  top: 50%;
  transform: translateY(-50%);
  left: 120px;
  padding: 0 16px;
}
.ema513_05_ih_0002 td:first-child .text_hint {
  left: 170px;
}
.ema513_05_ih_0002 td .example_box:not(.on) {
  display: none;
}

.ema513_05_ih_0003 .img_wrap {
  position: relative;
}
.ema513_05_ih_0003 .img_wrap span {
  position: absolute;
  top: 2px;
  font-size: 18px;
  transform: translateX(-50%);
  text-align: center;
}
.ema513_05_ih_0003 .img_wrap .txt1 {
  left: 68px;
}
.ema513_05_ih_0003 .img_wrap .txt2 {
  left: 251px;
}
.ema513_05_ih_0003 .img_wrap .txt3 {
  left: 434px;
}
.ema513_05_ih_0003 .table_wrap {
  width: 1523px;
}
.ema513_05_ih_0003 .table_wrap td {
  width: 19.5%;
}
.ema513_05_ih_0003 .table_wrap .custom_select {
  width: 190px;
}
.ema513_05_ih_0003 .table_wrap .flex {
  justify-content: center;
  align-items: center;
  gap: 10px;
}

.ema513_05_ih_0004 .question_box.dir_col {
  width: 1200px;
  margin: 0 auto;
  padding: 40px 30px;
  border: 1px solid #facc9c;
  border-radius: 40px;
  gap: 50px;
  align-items: flex-start;
}

.ema513_05_ih_0005 .tabbox {
  position: relative;
  width: 504px;
  padding-top: 20px;
}
.ema513_05_ih_0005 .tabbox .tit_tabbox {
  position: absolute;
  top: 0;
  left: 30px;
  padding: 0 20px;
  border-radius: 16px;
  background: #fef6ed;
  line-height: 40px;
  font-size: 24px;
  color: #c45938;
  font-weight: bold;
}
.ema513_05_ih_0005 .tabbox .conts_tabbox {
  border-radius: 48px;
  background: #fef6ed;
  padding: 30px 20px 20px;
}
.ema513_05_ih_0005 .tabbox .flex {
  height: 190px;
  background: #fff;
  border: 1px dashed #facc9c;
  border-radius: 40px;
}
.ema513_05_ih_0005 .question_box .question_box {
  flex-grow: 1;
  border: 1px solid #facc9c;
  border-radius: 40px;
  padding: 40px 30px;
  width: 1070px;
}
.ema513_05_ih_0005 .question_box .question_box .label_gray {
  margin-top: 10px;
  background: #fef6ed;
  color: #c45938;
}
.ema513_05_ih_0005 .question_box .question_box .flex {
  flex-grow: 1;
}
.ema513_05_ih_0005 .question_box .question_box .input_wrap {
  flex-grow: 1;
}
.ema513_05_ih_0005 .question_box .question_box .input_wrap math-field {
  width: 100%;
}
.ema513_05_ih_0005 .example_box {
  white-space: normal;
}
