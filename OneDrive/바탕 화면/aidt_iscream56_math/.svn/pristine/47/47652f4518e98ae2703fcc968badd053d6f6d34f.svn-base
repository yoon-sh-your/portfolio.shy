/* ema524_06_SU_0002 */

.ema524_06_su_0002 .video_wrap {
  background: url(../../common_contents/img/EMA524_05_SU/0002_contents.png)
    no-repeat center 0% / 100%;
}
.ema524_06_su_0002 .video_wrap p {
  position: absolute;
  top: 30%;
  left: 56%;
  text-align: center;
}
/* ema524_06_SU_0003 page_1 */

.ema524_06_su_0003 .page_1 .picture_wrap {
  background: url(../../common_contents/img/EMA524_05_SU/0002_contents.png)
    no-repeat center 0% / 100%;
}
.ema524_06_su_0003 .page_1 .picture_wrap img {
  display: none;
}
.ema524_06_su_0003 .picture_wrap p {
  position: absolute;
  top: 29.5%;
  left: 55.5%;
  text-align: center;
}

.ema524_06_su_0003 .picture_wrap text {
  transform: translateX(1360px) translateY(455px) scale(1.2);
}

.ema524_06_su_0003 .image-content text {
  transform: translateX(1425px) translateY(415px) scale(1.4);
}
/* ema524_06_SU_0003 page_2 */
.ema524_06_su_0003 .character_img {
  position: absolute;
  bottom: 35px;
  left: 205px;
  z-index: 1;
}
.ema524_06_su_0003 .character_img p {
  position: absolute;
  top: 30px;
  right: 10px;
  text-align: center;
}
.ema524_06_su_0003 .contents_img {
  position: relative;
}
.ema524_06_su_0003 .example_num {
  position: absolute;
  bottom: 68px;
  left: 64px;
}
.ema524_06_su_0003 .example_num > li {
  position: absolute;
}
.ema524_06_su_0003 .example_num > li:nth-child(1) {
  left: 0;
}
.ema524_06_su_0003 .example_num > li:nth-child(2) {
  top: 8px;
  left: 94px;
}
.ema524_06_su_0003 .example_num > li:nth-child(3) {
  top: 0px;
  left: 240px;
}
.ema524_06_su_0003 .example_num > li:nth-child(4) {
  bottom: 58px;
  left: -20px;
}
.ema524_06_su_0003 .example_num > li:nth-child(5) {
  bottom: 220px;
  left: 0px;
}
.ema524_06_su_0003 .example_num > li:nth-child(6) {
  bottom: 0px;
  left: -45px;
}
.ema524_06_su_0003 .multiply-box {
  max-width: 1100px;
  height: 312px;
  background: url(../../common_contents/img/EMA524_05_SU/0003_bg1.png) no-repeat
    left center;
  background-size: contain;
  margin: 0 auto;
  padding-left: 70px;
}
.ema524_06_su_0003 .multiply-box > div:first-child {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: flex-start;
  justify-content: center;
  gap: 20px;
}

.ema524_06_su_0003 .multiply-box > div:first-child > div {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: flex-start;
  justify-content: center;
  gap: 15px;
}

.ema524_06_su_0003 .multiply-box > div:first-child > div .f32b {
  margin-top: 32px;
}
.ema524_06_su_0003 .multiply-box > div img {
  display: inline-block;
  margin-right: 10px;
  vertical-align: middle;
}

/* ema524_06_SU_0003_s page3 */

.ema524_06_su_0003 .page_3 .picture_wrap {
  background: url(../../common_contents/img/EMA524_05_SU/0002_contents.png)
    no-repeat center 0% / 100%;
}
.ema524_06_su_0003 .page_3 .picture_wrap img {
  display: none;
}
.ema524_06_su_0003 .page_3 .input_wrap {
  flex-direction: row;
  align-items: center;
  gap: 12px;
}
#app_wrap.ema524_06_su_0003[data-current-page="page_3"] .btnSubmit,
#app_wrap.ema524_06_su_0003[data-current-page="page_1"] .btnSubmit,
#app_wrap.ema524_06_su_0003[data-current-page="page_2"] .btnSubmit {
  display: none;
}

/* ema524_06_SU_0004 page_1 */

.ema524_06_su_0004 .page_1 .wrap {
  display: flex;
  flex-direction: column;
  gap: 20px;
  margin: 0px auto 0;
  align-items: center;
}
.ema524_06_su_0004 .page_1 .wrap .textarea {
  min-height: 80px;
  width: 1108px;
}

/* ema524_06_SU_0004_s page2 */

.ema524_06_su_0004 .page_2 .fra_parens {
  display: flex;
  justify-content: center;
  align-items: center;
  margin-top: 50px;
}

/* ema524_06_SU_0004_s page3 */
.ema524_06_su_0004 .page_3 .question_box {
  width: 902px;
  height: 271px;
  background: url(../../common_contents/img/EMA524_03_SU/0004_bg.png) no-repeat
    left center / 100%;
  margin: 50px auto 0;
}
.question_box .wrapper {
  width: 100%;
  margin-bottom: 1%;
}
.question_box .wrapper .bg_coral.mmb_0 {
  margin-right: 12px;
}

.question_box .wrapper .box:nth-child(1) {
  width: 43%;
}
.question_box .wrapper .box:nth-child(2) {
  width: 58%;
  gap: 20px;
}
.question_box .wrapper .box:nth-child(1) > .arrow-row {
  gap: 55px;
  margin: 10px auto;
  padding-left: 10px;
}

.question_box .wrapper .box:nth-child(1) > .arrow-row > div {
  gap: 5px;
}

.question_box .wrapper .box:nth-child(1) > .arrow-row > div:first-child {
  margin-left: -30px;
  margin-right: 40px;
}
.question_box .wrapper .box:nth-child(2) > .arrow-col {
  margin-top: -10px;
  gap: 40px;
}

.question_box .wrapper .box:nth-child(2) > .arrow-col > div {
  width: 100%;
  text-align: center;
  background: url(../../common_contents/img/EMA524_01_SU/0004_arrow2.svg)
    no-repeat left center / 100%;
  padding: 0 30px;
}
.question_box .wrapper .box:nth-child(2) > .arrow-col > div > div {
  gap: 5px;
  background-color: var(--bg-default);
  width: 70px;
  margin: 0 auto;
}

/* ema524_06_SU_0004_s page4 */
.ema524_06_su_0004 .page_4 .wrap {
  display: flex;
  flex-direction: column;
  gap: 50px;
  align-items: center;
  padding-top: 30px;
}
.ema524_06_su_0004 .page_4 .wrap .textarea {
  min-height: 80px;
}

.ema524_06_su_0004 .page_4 .wrap > .input_wrap {
  width: 1108px;
}
.ema524_06_su_0004 .page_4 .wrap > .input_wrap math-field.textarea {
  width: 100% !important;
}
.ema524_06_su_0004[data-current-page="page_2"] .btnSample,
.ema524_06_su_0004[data-current-page="page_3"] .btnSample,
.ema524_06_su_0004[data-current-page="page_1"] .btnCheck,
.ema524_06_su_0004[data-current-page="page_4"] .btnCheck {
  display: none;
}
/* ema524_06_su_0005 */
.ema524_06_su_0005 .wrap {
  display: flex;
  gap: 10px;
  flex-direction: column;
  width: 1100px;
  margin: 0px auto 0;
}
.ema524_06_su_0005 .wrap > div {
  background-image: url(../../common_contents/img/EMA524_01_SU/0005_bg1.png);
  background-repeat: no-repeat;
  background-position: left top 20px;
  height: 255px;
  padding: 0 40px;
}

.ema524_06_su_0005 .contents h3 {
  padding-left: 0;
}

.ema524_06_su_0005 .contents h3::before {
  display: none;
}

.ema524_06_su_0005 .fra_parens {
  margin-left: 30px;
}
.ema524_06_su_0005 .wrap > div:last-child .fra_parens,
.ema524_06_su_0005 .wrap > div:first-child .fra_parens {
  margin-top: 40px;
  display: flex;
  align-items: center;
}

/* ema524_06_SU_0006_s */

.ema524_06_su_0006 .wrap > div {
  width: 1200px;
  margin: 50px auto 0;
  justify-content: space-between;
}

.ema524_06_su_0006 .wrap > div > div {
  min-width: 280px;
}
