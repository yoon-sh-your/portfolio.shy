/* paging layout > input_area */
.paging_layout {
    --ip_area-h: 502px;
    --h3_1: 48px;
    --h3_2: 96px;
    --figma_h3_b_space: 12px;
    --common_h3_b_space: 30px;
    --ip_area-w: 709px;
    --ip_wrap-h3_1: calc(var(--ip_area-h) - var(--h3_1) - var(--common_h3_b_space));
