.flex.rows {
  flex-direction: row;
}
.flex.col {
  flex-direction: column;
}
.align-center {
  align-items: center;
}
.text-align-center {
  text-align: center;
}
.flex-end {
  align-items: flex-end;
}
.justify-center {
  justify-content: center;
}
.flex-start {
  justify-content: flex-start;
}
.gap_2 {
  gap: 2px;
}
.gap_5 {
  gap: 5px;
}
.gap_10 {
  gap: 10px;
}
.gap_20 {
  gap: 10px;
}
.gap_35 {
  gap: 35px;
}
.bg_coral,
.bg_sky,
.bg_green {
  display: inline-block;
  height: 60px;
  width: auto;
  line-height: 60px;
  padding: 0 16px;
  border-radius: 12px;
}

.bg_coral {
  background-color: #ffcbc2;
}
.bg_sky {
  background-color: #eef9ff;
}
.bg_green {
  background-color: #e9efc6;
}

.small_border,
.small_bg_sky {
  display: inline-block;
  height: 32px;
  width: auto;
  line-height: 32px;
  padding: 0 10px;
  border-radius: 8px;
  font-size: 16px;
}
.small_border {
  border: 1px solid #ee4f24;
  background-color: var(--bg-default);
  color: #ee4f24;
}

.small_bg_sky {
  background-color: #d9e1f4;
}

h3.non-after {
  align-items: center;
}
h3.non-after::before {
  display: none;
}

.gray_Box {
  display: inline-block;
  text-align: center;
  width: 120px;
  padding: 8px 0;
  background-color: #9e9e9e;
  color: #fff;
  border-radius: 999px;
  margin-right: 18px;
}

