                  <div class="connect_point" data-id="e2"></div>
                  <div class="connect_point" data-id="e3"></div>
              </div>
              <svg class="connect_lines"></svg>
          </div>
          <ul class="connect_line_box end">
              <li>5</li>
              <li>
                <span class="mixed_fraction">
                  <span>1</span>
                  <span class="fraction_box">
                    <span>1</span>
                    <span>5</span>
                  </span>
                </span>
              </li>
              <li>
                <span class="fraction_box">
                    <span>6</span>
                    <span>7</span>
                </span>
              </li>
          </ul>
        </div>

        <div class="btn_area">
          <button class="btnType key">입력타입토글</button>
          <button class="btnReset">리셋</button>
          <button class="btnCheck">확인</button>
