.ema615_07_su_0002 {
    --v_wrap_w: 812px;
    --v_wrap_h: 428px;
    --ip_area: 502px;
    --h3_1: 48px;
    --h3_2: 96px;
    --figma_h3_b_space: 12px;
    --common_h3_b_space: 30px;
    --ip_wrap_h_h3_1: calc(var(--ip_area) - var(--h3_1) - var(--common_h3_b_space));
    --ip_wrap_h_h3_2: calc(var(--ip_area) - var(--h3_2) - var(--common_h3_b_space));
    --border: 1px solid red;
}

/* ema615_07_su_0002 */
.ema615_07_su_0002 .video_wrap {
    width: 812px;
    height: 428px;
    /* border: 1px solid red; */
}

.ema615_07_su_0002 .video_wrap img,
.ema615_07_su_0002 .video_wrap svg {
    transform: translate(-50.8%, -54%) scale(1.08);
}

.ema615_07_su_0002 .flex.align_start {
    gap: 26px;
}

.ema615_07_su_0002 .input_area {
    width: 797px;
    height: var(--ip_area);
    /* background-color: tomato; */
}

.ema615_07_su_0002 .input_wrap {
    width: 100%;
    height: var(--ip_wrap_h_h3_2);
    /* background-color: orange; */
}

.ema615_07_su_0002 .input_wrap math-field.textarea {
    /* height: 51%; */
    height: 62%;
}








/* paging layout > input_area */
.paging_layout {
    --ip_area-h: 502px;
    --h3_1: 48px;
    --h3_2: 96px;
    --figma_h3_b_space: 12px;
    --common_h3_b_space: 30px;
    --ip_area-w: 709px;
    --ip_wrap-h3_1: calc(var(--ip_area-h) - var(--h3_1) - var(--common_h3_b_space));
    --ip_wrap-h3_2: calc(var(--ip_area-h) - var(--h3_2) - var(--common_h3_b_space));
    --border: 1px solid orange;
}

