/* grid canvas */
.grid_canvas_wrap {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: url(../../common_contents/img/EMA612_05_SU/0005_paper_011.svg) 0 0 no-repeat;
  padding-top: 7px;
  padding-left: 10px;
}
.grid-canvas {
  position: absolute;
  width: 548px;
  height: 374px;
  bottom: 2px;
  right: 0;
}
.grid-canvas .overlay-layer {
  position: absolute;
  top: 24px;
  left: 54px;
  width: 324px;
  height: 325x;
  transform: scale(0.9859);
}
svg.userLines {
  stroke-linecap: round;
  stroke-linejoin: round;
  /* pointer-events: none; */
}
svg.userLines line[data-style="dotted"] {
  stroke-dasharray: 5, 5;
}
svg.userLines line[data-style="solid"] {
  stroke-dasharray: none;
}
svg.userLines .guide-dash {
  stroke: #222;
  stroke-width: 4;
  stroke-dasharray: 3 3;
  fill: none;
}

svg.userLines .guide-solid {
  stroke: #222;
  stroke-width: 4;
  stroke-dasharray: none;
  fill: none;
}

svg.userLines .red-solid {
  stroke: #f60c0e;
  stroke-width: 4;
  stroke-linecap: round;
  fill: none;
}
svg.userLines .red-dash {
  stroke: #f60c0e;
  stroke-width: 4;
  stroke-dasharray: 3 3;
  fill: none;
}
/*Grid*/
.drawline_grid {
  position: relative;
  z-index: 2;
}
.ema612_05_su_0005 .drawing_grid_area {
  position: relative;
  display: flex;
  align-items: center;
  border: 0;
  background-image: url(../../common_contents/img/EMA612_05_SU/0005_paper_01.svg);
  background-repeat: no-repeat;
  background-size: 100%;
}
.ema612_05_su_0005 .page_1 .drawing_grid_area {
  width: 570px;
  height: 390px;
}
.drawing_grid_area .hit_area {
  background: rgba(0, 0, 0, 0);
}

.drawing_grid_area .point {
  width: 6px;
  height: 6px;
  background-color: #231f20;
  border-radius: 50%;
  position: absolute;
  transform: translate(-50%, -50%);
}

.drawing_grid_area .point.end-point {
  background-color: #0066cc;
}

.drawing_grid_area .default_line {
  stroke: #ff0000;
  stroke-width: 2px;
}
.line_canvas line {
  stroke: #231f20;
  stroke-width: 4px;
}
.line_canvas line.guide-dash {
  stroke-dasharray: 5 5;
}
.line_canvas line.guide-solid {
  stroke-dasharray: none;
}
.line_canvas line.selected {
  stroke: #3b71fe;
  stroke-width: 4;
}
svg.line_canvas line,
svg.line_canvas path {
  pointer-events: visiblePainted;
  cursor: pointer;
}
/* draw correct, incorrect */
.drawing_grid_area .answer_bg_line {
  stroke: rgba(255, 0, 0, 0.65);
  stroke-width: 4px;
  stroke-linecap: round;
  stroke-linejoin: round;
  pointer-events: none;
  opacity: 1;
  cursor: default;
}
.correct .drawing_grid_area .answer_bg_line {
  stroke: rgba(0, 0, 255, 0.65);
}
.incorrect .drawing_grid_area .answer_bg_line {
  stroke: rgba(255, 0, 0, 0.65);
}
