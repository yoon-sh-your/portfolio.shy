    /* border: 2px solid white; */

    border-collapse: separate;
}

thead {
    background-color: #EFEEEE;
}

thead th {
    background-color: #EFEEEE;
}

thead th:nth-of-type(1),
tbody th {
    background-color: #E4E9F7;
    border-right: 3.47px solid #fff;
    width: 200px;
}

th {
    /* border: 3.47px solid #fff; */
    border: 1px solid #fff;
}

th,
td {
    text-align: center;
    width: 160px;
    height: 70px;
}

tbody tr {
    background-color: #fff;
}

tbody td {
    /* border: 1.74px solid #DADADA; */
    border: 1px solid #DADADA;
}

/* 왼쪽 위 */
.my-table thead tr:first-child th:first-child {
    border-top-left-radius: 30px;
}

/* 오른쪽 위 */
.my-table tr:first-child th:last-child {
    border-top-right-radius: 30px;
}

/* 왼쪽 아래 */
.my-table tbody tr:last-child th:first-child {
    border-bottom-left-radius: 30px;
}

/* 오른쪽 아래 */
.my-table tbody tr:last-child td:last-child {
    border-bottom-right-radius: 30px;
    /* border: 2px solid purple; */
    /* border-radius: 40px; */
}









/* graph */
svg {
    /* border: 1px solid #ccc; */
    background: transparent;
}

.bar {
    fill: transparent;
    stroke: #000;
    stroke-width: 2;
    cursor: pointer;
    pointer-events: none;
}

.circle {
    fill: transparent;
    /* stroke: #000;
    stroke-width: 2; */
    cursor: pointer;
    pointer-events: none;
}

.center-point {
    fill: #000;
    cursor: pointer;
    pointer-events: none;
}

.diameter-point-area {
    fill: transparent;
    cursor: pointer;
}

.diameter-point {
    fill: #ff0000;
    cursor: pointer;
    transition: all 0.2s;
}

.diameter-point:hover {
    fill: #ff6666;
    r: 8;
}

.click-line {
    stroke: #0000ff;
    stroke-width: 3;
    pointer-events: none;
}

.click-point {
    fill: #0000ff;
    pointer-events: none;
}

.preview-line {
    stroke: #0000ff;
    stroke-width: 2;
    stroke-dasharray: 5, 5;
    opacity: 0.5;
}

.percentage-text {
    font-size: 14px;
    fill: #000;
    font-weight: bold;
    text-anchor: middle;
    dominant-baseline: middle;
}

.option-selector {
    position: absolute;
    background: white;
    border: 1px solid #ccc;
    padding: 10px;
    border-radius: 5px;
    box-shadow: 0 2px 5px rgba(0, 0, 0, 0.2);
    display: none;
}

.option-button {
    display: block;
    width: 100%;
    padding: 5px 10px;
    margin: 5px 0;
    border: 1px solid #ccc;
    background: #f5f5f5;
    cursor: pointer;
    text-align: left;
}

.option-button:hover {
    background: #e0e0e0;
}

.click-line {
    stroke: #000;
    stroke-width: 2;
}

.click-point {
    fill: #000;
    cursor: pointer;
}

.diameter-point {
    fill: #ccc;
    cursor: pointer;
}

.diameter-point-area {
    fill: transparent;
    cursor: pointer;
}

.preview-line {
    stroke: #ccc;
    stroke-width: 1;
    stroke-dasharray: 5, 5;
}

.percentage-text {
    font-size: 14px;
    text-anchor: middle;
    dominant-baseline: middle;
}

.percentage-text {
    color: #1D1D1B;
    text-align: center;
    font-family: Pretendard;
    font-size: 22.011px;
    font-style: normal;
    font-weight: 700;
    line-height: 33.017px;
    /* 150% */
    letter-spacing: -0.688px;
}









