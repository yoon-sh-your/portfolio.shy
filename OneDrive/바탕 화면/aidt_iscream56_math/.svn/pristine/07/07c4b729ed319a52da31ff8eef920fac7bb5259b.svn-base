}

/*준비물 아이콘*/
.icon_badge {
  display: flex;
  align-items: center;
  font-size: 20px;
  font-weight: 700;
  color: #fff;
  padding: 0 20px;
  min-width: 90px;
  margin-left: 12px;
  background: var(--primary-blue);
  border-radius: 12px;
  line-height: 65px;
  height: 56px;
}
