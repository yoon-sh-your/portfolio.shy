
/*EMA523_03_SU_0003_s*/
.ema523_03_su_0003 .dragndrop_fraction_wrap {
  position: relative;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 1136px;
  height: 420px;
  background-color: #F7F7F7;
  border-radius: 60px;
  margin: 0 auto;
  gap:200px;
}
.ema523_03_su_0003 .dragndrop_fraction_wrap .drag_group {
  display: flex;
  gap:90px
}
.ema523_03_su_0003 .figure_triangle {
  position:relative;
  width: 300px;
  height: 228px;
  background: url(../img/EMA523_03_SU/0003_01_content.svg) no-repeat 50%;
  background-size: 300px 170px;
}
.ema523_03_su_0003 .figure_triangle {
  cursor: url(../common/img/cursor_pointer_small.svg) 35 15, auto;

}
.ema523_03_su_0003 .figure_triangle > span {
  position:absolute;
  font-size: 32px;
  font-weight: 700;
}
.ema523_03_su_0003 .figure_triangle > span:nth-child(1) {
  top:-5px;
  left: 48px;
}
.ema523_03_su_0003 .figure_triangle > span:nth-child(2) {
  bottom:8px;
  left: -27px;
}
.ema523_03_su_0003 .figure_triangle > span:nth-child(3) {
  bottom:8px;
  right: -27px;
}
.ema523_03_su_0003 .figure_wrap {
  display: flex;
  align-items: center;
  width: 1316px;
  margin: 0 auto;
  gap:98px;
}
.ema523_03_su_0003 .figure_box {
  display: flex;
  gap:150px
}
.ema523_03_su_0003 .connect_box_wrap {
  display: flex;
  align-items: center;
  gap: 16px;
}
.ema523_03_su_0003 .connect_wrap {
  width:165px
}
.ema523_03_su_0003 .connect_text_box {
  display: flex;
  flex-direction: column;
  gap: 40px;
}
.ema523_03_su_0003 .connect_text_box li{
  font-size:32px;
  font-weight: 700;
}
.ema523_03_su_0003 .start_group, .ema523_03_su_0003 .end_group {
  gap: 67px;
}
.ema523_03_su_0003[data-current-page='page_1'] .btnCheck,
.ema523_03_su_0003[data-current-page='page_5'] section.title,
.ema523_03_su_0003[data-current-page='page_5'] .btnType,
.ema523_03_su_0003 .btnType {
  display: none;
}

.ema523_03_su_0003 .page_5 .img_txt {
  position: absolute;
  top: -9px;
  left: 220px;
  display: flex;
  flex-direction: column;
  gap: 0;
}
.ema523_03_su_0003 .page_5 .img_txt > span {
  position: relative;
  font-size: 16px;
  font-weight: 400;
  line-height: 47px;
}
.ema523_03_su_0003 .page_5 .img_txt > span:nth-child(1) {
  transform:translateX(-47px)
}
.ema523_03_su_0003 .page_5 .img_txt > span:nth-child(2) {
  transform:translateX(-28px)
}
.ema523_03_su_0003 .page_5 .img_txt > span:nth-child(3) {
  transform:translateX(-8px)
}

/*EMA523_03_SU_0004_s*/
.ema523_03_su_0004 .paging_layout {
  padding: 0 40px;
}

.ema523_03_su_0004 .page_1 {
  width:100%;
}
.ema523_03_su_0004 .cont_box {
  width:1322px;
  height: 292px;
  margin: 0 auto;
  background: url(../img/EMA523_03_SU/0004_bg.png) no-repeat 0 0;
}

.ema523_03_su_000 .custom_select {
  width: 200px;
}

.ema523_03_su_0004[data-current-page='page_4'] section.title,
.ema523_03_su_0004[data-current-page='page_4'] .btnType,
.ema523_03_su_0004 .btnType {
  display: none;
}

.ema523_03_su_0004 .page_4 .summarize label::after {
  content: none;
}
.ema523_03_su_0004 .page_4 .summarize {
  font-weight: bold;
  font-size: 32px;
  gap: 30px;
  height: 562px;
  background-color: white;
  border: 4px solid var(--secondary-yellow);
}
.ema523_03_su_0004 .page_4 .summarize label {
  background-color: var(--secondary-yellow);
  color: #a55935;
}
.ema523_03_su_0004 .page_4 .summarize_wrap .summarize {
  flex-direction: row;
  gap: 99.5px;
}
.ema523_03_su_0004 .page_4 .summarize > div {
  font-size: 32px;
  flex-direction: column;
  align-items: flex-start;
}

.ema523_03_su_0004 .page_4 .summarize .reveal_btn {
  font-size: 32px;
}

/*EMA523_03_SU_0005_s*/
.ema523_03_su_0005 .page_2 .input_wrap .textarea {
  width:1208px;
  height: 340px;
}

.ema523_03_su_0005[data-current-page='page_1'] .btnType,
.ema523_03_su_0005[data-current-page='page_1'] .btnSubmit {
  display: none;
}

.ema523_03_su_0005[data-current-page='page_2'] .btnCheck{
  display: none;
}

/*EMA523_03_SU_0006_s*/
.ema523_03_su_0006 .cont_img_wrap {
  display: flex;
  align-items: center;
  flex-direction: column;
}
.ema523_03_su_0006 .cont_img_box {
 position: relative;
  width: 1016px;
  height: 291px;
  background: url(../img/EMA523_03_SU/0006_bg.png) no-repeat 0 0;
}

.ema523_03_su_0006 .cont_img_box .text_box {
  position:absolute;
  top:24px;
  left:15px;
  font-size: 20px;
  text-align: center;
}
.ema523_03_su_0006 .cont_img_box .text_box:nth-child(2) {
  left:auto;
