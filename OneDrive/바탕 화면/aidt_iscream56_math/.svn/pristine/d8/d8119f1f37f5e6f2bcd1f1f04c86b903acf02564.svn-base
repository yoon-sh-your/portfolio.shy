/* ema524_11_SU_0003_s */

.ema524_11_su_0003 .wrap > div {
  width: 1200px;
  margin: 50px auto 0;
  justify-content: space-between;
}

.ema524_11_su_0003 .wrap > div > div {
  min-width: 280px;
}

/* ema524_11_SU_0004_s */
.ema524_11_su_0004 .question_box {
  background: url(../../common_contents/img/EMA524_09_SU/0004_bg.png) no-repeat
    center center / 806px;
  height: 250px;
  margin-top: 50px;
  text-align: center;
  display: flex;
  justify-content: center;
}

.ema524_11_su_0004 .circle {
  display: inline-flex;
  justify-content: center;
  align-items: center;
  width: 44px;
  height: 44px;
  border-radius: 100%;
  border: 2px solid #000;
  font-size: 24px;
}
.ema524_11_su_0004 .top .item {
  display: inline-flex;
  align-items: center;
  gap: 12px;
}
.ema524_11_su_0004 .top .circle {
  position: relative;
  z-index: 2;
}
.ema524_11_su_0004 .boolean_wrap {
  position: relative;
  display: flex;
  flex-wrap: wrap;
  width: 75%;
  flex-direction: row;
  align-items: flex-start;
  justify-content: space-evenly;
  gap: 0;
  margin: 0 auto;
}
.ema524_11_su_0004 .boolean_wrap button {
  position: relative;
  background: none;
  width: 35%;
  padding: 0;
  border: none;
  border-radius: 0;
  justify-content: flex-center;
}
.ema524_11_su_0004 .boolean_wrap button .circle {
  flex-shrink: 0;
}
.ema524_11_su_0004 .boolean_wrap button:after {
  position: absolute;
  top: -5px;
  left: -5px;
  width: 60px;
  height: 60px;
  background: url(../../common_practice/common/img/icon_checked_round.svg)
    no-repeat center center / contain;
}
.ema524_11_su_0004 .boolean_wrap button.selected:after {
  content: "";
}
.ema524_11_su_0004
  .boolean_wrap.result
  button.selected[data-answer-single="true"]:after {
  content: "";
  filter: invert(35%) sepia(57%) saturate(3152%) hue-rotate(215deg)
    brightness(103%) contrast(99%);
}
.ema524_11_su_0004
  .boolean_wrap.result
  button.hint[data-answer-single="true"]:after {
  content: "";
  filter: invert(17%) sepia(79%) saturate(3636%) hue-rotate(354deg)
    brightness(104%) contrast(135%);
}
.ema524_11_su_0004 .solve_area .conts_solve p + p {
  margin-top: 24px;
}

