/* 방법 1,2 */
.method {
  color: #ffffff;
  text-shadow: -2px 0px #5bb8a1, 0px 2px #5bb8a1, 2px 0px #5bb8a1,
    0px -2px #5bb8a1;
  position: relative;
  width: 115px;
  height: 40px;
  display: flex;
  justify-content: flex-end;
  align-items: flex-end;
  background-repeat: no-repeat;
  background-position: bottom right 10px;
  /* background-color: tomato; */
  padding-right: 50px;
}

.method[circle-number="1"] {
  background-image: url(../img/EMA515_04_SU/0005_cricle-1.png);
}

.method[circle-number="2"] {
  background-image: url(../img/EMA515_04_SU/0005_cricle-2.png);
}

.method::before {
  content: "";
  width: 97px;
  height: 23px;
  background-color: #5bb8a1;
  position: absolute;
  left: 0px;
  bottom: 0;
  z-index: -1;
  border-radius: 20px;
}

.hidden_character {
  display: none;
}

h3.non-after {
  align-items: center;
}
h3.non-after::before {
  display: none;
}

/* 문제 만들기 박스 */
.quiz-builder {
  width: 1200px;
  margin: 0px auto 0;
}
.quiz-builder .quiz-title {
  background-color: #fff5ed;
  border-radius: 20px;
  line-height: 64px;
  padding: 0 24px;
  margin-left: 40px;
  display: inline-block;
}
.quiz-builder .quiz-color {
  color: #c0573d;
}
.quiz-builder .quiz-content {
  background-color: #fff5ed;
  border-radius: 40px;
  padding: 20px;
  margin-top: -20px;
}
.quiz-builder .quiz-content .quiz-content-inner {
  border-radius: 30px;
  border: 1px solid #fbcba1;
  background-color: var(--bg-default);
  padding: 40px 60px;
}
.quiz-builder .quiz-content-inner .input_wrap {
  width: 100%;
}
.quiz-builder .input_wrap math-field.textarea {
  min-height: 60px;
}
.quiz-builder .quiz-answer-inner .input_wrap > p:first-child {
  display: inline-block;
  line-height: 42px;
  padding: 0 11px;
  border-radius: 999px;
  border-bottom-right-radius: 0;
  background-color: var(--bg-default);
  margin-right: 10px;
}
.quiz-builder .quiz-answer-inner {
  text-align: center;
}
.quiz-builder .quiz-answer-inner .input_wrap {
  margin: 20px auto 0;
  align-items: center;
  justify-content: center;
  flex-direction: row;
  gap: 10px;
  width: auto;
}

/* 예시박스 오른쪽인경우 */

.example_left_wrap {
  flex-direction: row;
  gap: 12px;
}
.example_left_wrap .example_box {
  margin-top: 0;
  padding: 18px 24px 18px 82px;
}
.example_left_wrap .example_box::before {
  top: 14px;
}

/* hidden 버튼 */
.hidden_btn button:focus,
.hidden_btn button:focus-visible,
.hidden_btn *[role="button"]:focus,
.hidden_btn *[role="button"]:focus-visible {
  outline: 3px dashed transparent !important;
  outline-offset: -3px;
  border-color: transparent !important;
  /* border: 3px dashed black !important; */
  border-radius: 8px;
  box-sizing: border-box !important;
  background-clip: padding-box !important;
}
.hidden_btn button.hint,
.hidden_btn button:hover {
  border-color: transparent !important;
}

.hidden_btn button.selected {
  border-color: rgba(0, 0, 0, 0);
}

/* 버튼 span 으로 정답 오답 표기  */

.boolean_wrap.span_type > button {
  background-color: rgba(0, 0, 0, 0);
  display: flex;
  border: 0;
  height: 100%;
  align-items: flex-start;
  justify-content: flex-start;
}
.boolean_wrap.span_type > button span {
  border: 3px solid rgba(0, 0, 0, 0);
  border-radius: 999px;
  width: 60px;
  line-height: 55px;
}
.boolean_wrap.span_type > button.selected span {
  border-color: #222222;
  background-color: rgba(59, 113, 254, 0);
}
.boolean_wrap.span_type button[data-answer-single="true"].hint span {
  background-color: rgba(59, 113, 254, 0.3);
  border-color: #3b71fe;
}
/* .boolean_wrap.span_type button[data-answer-single="false"].selected.hint span {
  border-color: var(--secondary-red);
} */
.boolean_wrap.span_type.correct button[data-answer-single="true"].selected span,
.boolean_wrap.span_type button[data-correction="true"].selected.hint span {
  background-color: rgba(59, 113, 254, 0.3);
  border-color: #3b71fe;
}

