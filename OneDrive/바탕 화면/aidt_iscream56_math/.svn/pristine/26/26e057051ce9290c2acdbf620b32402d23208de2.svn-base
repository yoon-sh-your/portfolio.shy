.bg_trans,
.bg_coral,
.bg_sky,
.bg_green {
  display: inline-block;
  height: 60px;
  width: auto;
  line-height: 60px;
  padding: 0 16px;
  border-radius: 12px;
  margin: 8px;
}

.bg_coral {
  background-color: #ffcbc2;
}
.bg_sky {
  background-color: #eef9ff;
}
.bg_green {
  background-color: #e9efc6;
}

.bg_trans {
  background-color: transparent;
}

.small_border,
.small_bg_sky {
  display: inline-block;
  height: 32px;
  width: auto;
  line-height: 32px;
  padding: 0 10px;
  border-radius: 8px;
  font-size: 16px;
}
.small_bg_sky {
  background-color: #d9e1f4;
}
.small_border {
  border: 1px solid #ee4f24;
