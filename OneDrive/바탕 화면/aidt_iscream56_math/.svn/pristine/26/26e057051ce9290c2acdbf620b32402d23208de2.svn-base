  border: 2px solid var(--primary-blue);
  border-radius: 5px;
  background-color: #fff;
  margin-right: 5px;
  margin-left: 5px;
}
.empty_circle {
  display: inline-block;
  width: 40px;
  height: 40px;
  border: 2px solid var(--primary-blue);
  border-radius: 5px;
  background-color: #fff;
  margin-right: 5px;
  margin-left: 5px;
  border-radius: 999px;
}

