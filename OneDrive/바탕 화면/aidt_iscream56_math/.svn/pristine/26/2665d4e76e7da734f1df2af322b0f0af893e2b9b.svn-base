/*격자 그리드*/
.drawline_grid {
  position: relative;
}

.drawing_grid_area {
  position: relative;
  display: flex;
  align-items: center;
  width: 450px;
  height: 290px;
  /* background-size: 32px 32px; */
  border: 0;
  background-image: url(../../common_contents/img/EMA523_05_SU/0007_bg.svg);
  background-repeat: no-repeat;
  background-size: 100%;
}

.drawing_grid_area .point {
  width: 16px;
  height: 16px;
  background-color: #222;
}

.drawing_grid_area.img_area1 {
  padding-left: 61px;
}

.drawing_grid_area.img_area2 {
  padding-left: 95px;
}

.dot_wrap {
  position: absolute;
  top: 0;
  left: 0;
  display: flex;
}

.dot_wrap .dot {
  width: 16px;
  height: 16px;
  background-color: #222;
  border-radius: 50%;
}

/*준비물 아이콘*/
.icon_badge {
  display: flex;
  align-items: center;
  font-size: 20px;
  font-weight: 700;
  color: #fff;
  padding: 0 20px;
  min-width: 90px;
  margin-left: 12px;
  background: var(--primary-blue);
  border-radius: 12px;
  line-height: 65px;
  height: 56px;
}

/*Grid 클릭시 선택 팝업 관련*/
.icon_txt01 {
  background: url(../img/EMA523_05_SU/icon_txt01.svg) no-repeat 50%;
}

.icon_txt02 {
  background: url(../img/EMA523_05_SU/icon_txt02.svg) no-repeat 50%;
}

.grid-container {
  position: relative;
  width: 480px;
  height: 326px;
  cursor: pointer;
}

.marked-point {
  position: absolute;
  width: 16px;
  height: 16px;
  background-color: #222;
  border-radius: 50%;
  color: #222;
  font-size: 12px;
  text-align: center;
  line-height: 16px;
  transform: translate(-50%, -50%);
  pointer-events: none;
  z-index: 5;
}

.marked-point.hint {
  display: none;
  color: #FF0000;
  background-color: #FF0000;
}

.mark-text {
  position: absolute;
  top: 19px;
  color: #222;
  font-size: 24px;
  font-weight: 700;
  left: 50%;
  transform: translateX(-50%);
}

.correct .marked-point {
  background-color: #3B71FE;
}

.correct .mark-text {
  color: #3B71FE;
}

.popup {
  position: absolute;
  display: none;
  max-width: unset;
  width: unset;
  max-height: unset;
  height: unset;
  height: 70px;
  padding: 8px;
  background-color: var(--primary-blue-800);
  overflow: unset;
  white-space: nowrap;
  border-radius: 70px;
}

.popup button {
  display: inline-block;
  width: 52px;
  height: 52px;
  min-width: auto;
  padding: 0;
  background-color: var(--bg-default);
  background-size: 32px;
  background-repeat: no-repeat;
  background-position: center;
  border: 1px solid var(--stroke-default);
  font-size: 0;
  border-radius: 50%;
  vertical-align: top;
}

.popup::after {
  content: '';
  position: absolute;
  width: 29px;
  height: 19px;
  background: url(../common/img/icon_bubble_tail.svg) no-repeat center/100%;
  top: -15.5px;
  left: 50%;
  transform: translateX(-50%);
}
