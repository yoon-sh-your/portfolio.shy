  z-index: 1;
}
.ema525_01_su_0005 .cube_wrap {
  position: absolute;
  display: flex;
  justify-content: center;
  align-items: center;
  perspective: 1000px;
  padding-left: 300px;
  cursor: pointer;
  height: 130px;
  width: 50%;
  top: 55%;
}

.ema525_01_su_0005 .cube_wrap .cube_box {
  position: absolute;
  transform: translateY(-50%);
}

.ema525_01_su_0005 .cube_wrap .cube {
  position: relative;
  width: 100px;
  height: 100px;
  transform-style: preserve-3d;
  transform: scale(0.8) rotateX(-20deg) rotateY(338deg);
}

.ema525_01_su_0005 .cube_wrap .face {
  position: absolute;
  text-align: center;
  width: 100px;
  height: 100px;
  transition: transform 1s ease;
  border: 1px solid rgb(0, 0, 0);
  opacity: 0;
}

.ema525_01_su_0005 .cube_wrap .face img {
  margin: 0 auto;
}

.ema525_01_su_0005 .cube_wrap .cube + .arrow {
  position: absolute;
  left: -59%;
  top: 30%;
  width: 28px;
  opacity: 0;
  transition: all 1s ease;
}

.ema525_01_su_0005 .cube_wrap .cube.fold0 + .arrow {
  opacity: 1;
}

.cube_wrap .face img {
  height: 70%;
  margin-top: 15%;
}

.cube_wrap .front {
  background: #b7d8f1;
  background: linear-gradient(
    61deg,
    rgba(183, 216, 241, 1) 63%,
    rgba(217, 240, 255, 1) 100%
  );
  width: 130px;
  height: 100px;
}
.cube_wrap .back {
  background: #b7d8f1;
  background: linear-gradient(
    -61deg,
    rgba(183, 216, 241, 1) 63%,
    rgba(217, 240, 255, 1) 100%
  );
  width: 130px;
  height: 100px;
}
.cube_wrap .left {
  transform-origin: right center;
  background: #b0dab2;
  background: linear-gradient(
    -61deg,
    rgba(176, 218, 178, 1) 27%,
    rgba(196, 227, 198, 1) 100%
  );
}
.cube_wrap .right {
  transform-origin: left center;
  background: #b0dab2;
  background: linear-gradient(
    61deg,
    rgba(176, 218, 178, 1) 27%,
    rgba(196, 227, 198, 1) 100%
  );
}
.cube_wrap .top {
  transform-origin: bottom center;
  background: #f7b4bd;
  background: linear-gradient(
    -61deg,
    rgba(247, 180, 189, 1) 27%,
    rgba(251, 207, 211, 1) 100%
  );
  width: 130px;
  height: 100px;
}
.cube_wrap .bottom {
  transform-origin: top center;
  background: #f7b4bd;
  background: linear-gradient(
    61deg,
    rgba(247, 180, 189, 1) 27%,
    rgba(251, 207, 211, 1) 100%
  );
  width: 130px;
  height: 100px;
}

/* 애니메이션 1 */
.cube_wrap._1 {
  left: 8%;
}
.cube_wrap._1 .back {
  transform: translateX(0px) translateY(50px) translateZ(-149px) rotateX(90deg);
  width: 130px;
  height: 100px;
}
.cube_wrap._1 .left {
  transform: translateX(-98px) translateY(50px) translateZ(-49px) rotateX(90deg);
}
.cube_wrap._1 .bottom {
  transform: translateX(0px) translateY(100px) translateZ(0px) rotateX(-90deg);
  width: 130px;
  height: 100px;
}

.cube_wrap._1 .fold0 .face {
  opacity: 1;
}
.cube_wrap._1 .fold .left {
  transform: translateX(-98px) translateY(50px) translateZ(-49px) rotateX(90deg)
    rotateY(90deg);
}
.cube_wrap._1 .fold .back {
  transform: translateX(0px) translateY(00px) translateZ(-100px) rotateX(0deg);
}

.cube_wrap._2 {
  left: 29%;
}

.cube_wrap._2 .front {
  transform: translateY(70px) translateX(-20px) translateZ(0px) rotateX(90deg);
  width: 130px;
  height: 100px;
}
.cube_wrap._2 .back {
  transform: translateX(0px) translateY(1px) translateZ(-100px) rotateY(180deg);
  width: 130px;
  height: 100px;
}
.cube_wrap._2 .left {
  transform: translateX(-99px) translateY(1px) rotateY(-90deg);
}
.cube_wrap._2 .right {
  transform: translateX(170px) translateY(58px) translateZ(-23px) rotateY(90deg)
    rotateX(90deg);
}
.cube_wrap._2 .top {
  transform: translateX(0px) translateY(-97px) translateZ(-100px) rotateZ(0deg)
    rotateX(0deg) rotateY(0deg);
  width: 130px;
  height: 100px;
}
.cube_wrap._2 .bottom {
  transform: translateY(100px) rotateX(-90deg);
  width: 130px;
  height: 100px;
}

.cube_wrap._2 .fold0 .face {
  opacity: 1;
}
.cube_wrap._2 .fold .front {
  transform: translateY(0px) translateZ(0px) rotateX(180deg);
}
.cube_wrap._2 .fold .right {
  transform: translateX(130px) translateY(0px) rotateY(90deg) rotateX(180deg);
}

.cube_wrap._2 .fold1 .top {
  transform: translateX(0px) translateY(-100px) translateZ(-100px) rotateZ(0deg)
    rotateX(-40deg) rotateY(0deg);
}

.cube_wrap._3 {
  left: 52%;
}
.cube_wrap._3 .cube {
  transition: transform 1s ease;
  transform: scale(1.6) rotateX(-20deg) rotateY(338deg);
  opacity: 0;
}
.cube_wrap._3 .face {
  opacity: 1;
  transition: all 0s ease;
}

.cube_wrap._3 .front {
  transform: translateY(1px) translateZ(0px) rotateX(180deg);
  width: 130px;
  height: 100px;
}
.cube_wrap._3 .back {
  transform: translateX(0px) translateY(1px) translateZ(-100px) rotateY(180deg);
  width: 130px;
  height: 100px;
}
.cube_wrap._3 .left {
  transform: translateX(-99px) translateY(1px) rotateY(-90deg);
}
.cube_wrap._3 .right {
  transform: translateX(130px) translateY(0px) rotateY(90deg) rotateX(180deg);
}
.cube_wrap._3 .top {
  transform: translateX(0px) translateY(-100px) translateZ(-100px) rotateZ(0deg)
    rotateX(-90deg) rotateY(0deg);
  width: 130px;
  height: 100px;
}
.cube_wrap._3 .bottom {
  transform: translateX(0px) translateY(100px) translateZ(0px) rotateX(-90deg);
  width: 130px;
  height: 100px;
}

.cube_wrap._3 .fold0 {
  transform: scale(0.8) rotateX(-20deg) rotateY(338deg);
  opacity: 1;
}

