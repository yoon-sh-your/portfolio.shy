                            <div class="connect_point" data-id="ms1"></div>
                            <div class="connect_point" data-id="ms2"></div>
                            <div class="connect_point" data-id="ms3"></div>
                        </div>
                        <div class="end_group">
                            <div class="connect_point" data-id="me1"></div>
                            <div class="connect_point" data-id="me2"></div>
                            <div class="connect_point" data-id="me3"></div>
                        </div>
                        <svg class="connect_lines"></svg>
                    </div>
                    <ul class="connect_line_box mid2">
                        <li>0.3</li>
                        <li>0.75</li>
                        <li>0.2</li>
                    </ul>
                    <div class="connect_wrap" data-connections="[]" data-answer-single='[["mee1","e3"],["mee2","e1"],["mee3","e2"]]'>
                        <div class="start_group">
                            <div class="connect_point" data-id="mee1"></div>
